            function pur5710_on_survey_headers_ds_loadFun(dataSet) {
                var record = dataSet.getAt(0);
                record.getMeta().getField('company_code').setReadOnly(true);
                record.getMeta().getField('vendor_code').setReadOnly(true);
                if (record.get('survey_type_code') == '10_MANUFACTURER') {
                    record.getMeta().getField('account_period_desc').setReadOnly(false);
                    record.getMeta().getField('account_period_desc').setRequired(true);
                    document.getElementById('account_period_id').style.display = 'block';
                } else {
                    record.getMeta().getField('account_period_desc').setReadOnly(true);
                    record.getMeta().getField('account_period_desc').setRequired(false);
                    document.getElementById('account_period_id').style.display = 'none';
                }
                // record.getMeta().getField('survey_type_desc').setReadOnly(true);
            }
            
            function pur5710_on_survey_headers_ds_updateFun(dataSet, record, name, value, oldvalue) {
                if (name == 'company_code' || name == 'company_name') {
                    var vendor_id = '${/parameter/@vendor_id}';
            
                    if (vendor_id) {
                        return;
                    }
                    var company_id = record.get('require_company_id');
                    var company_name = record.get('company_name');
                    record.set('vendor_code', '');
                    record.set('vendor_name', '');
                    record.set('supplier_categroy_desc', '');
                    record.set('enterprise_nature_desc', '');
                    record.set('domestic_foreign_relation_desc', '');
                    if (company_name == '' || company_name == 'undefined' || company_name == null) {
                        record.getMeta().getField('vendor_code').setLovPara('company_id', company_id);
                        record.getMeta().getField('vendor_code').setReadOnly(true);
                        // record.getMeta().getField('survey_type_desc').setReadOnly(true);
                    } else {
                        record.getMeta().getField('vendor_code').setLovPara('company_id', company_id);
                        record.getMeta().getField('vendor_code').setReadOnly(false);
                        // record.getMeta().getField('survey_type_desc').setReadOnly(false);
                    }
            
                } else if (name == 'vendor_code' || name == 'vendor_name') {
                    if (record.get('vendor_name') == '' || record.get('vendor_name') == 'undefined' || record.get('vendor_name') == null) {
                        record.set('supplier_categroy_desc', '');
                        record.set('enterprise_nature_desc', '');
                        record.set('domestic_foreign_relation_desc', '');
                    }
                } else if (name == 'supplier_categroy') {
                    var type_records = $('pur5710_vendor_survey_type_ds').getAll();
                    if (record.get('supplier_categroy') == 'MANUFACTURER') {
                        for (var i = 0;i < type_records.length;i++) {
                            if ((type_records[i].get('flex_value') == '10_MANUFACTURER')) {
                                record.set('survey_type_code', type_records[i].get('flex_value'));
                                record.set('survey_type_desc', type_records[i].get('flex_desc'));
                            }
                        }
                    } else if (record.get('supplier_categroy') != '' || record.get('supplier_categroy') != 'undefined' || record.get('supplier_categroy') != null) {
                        for (var j = 0;j < type_records.length;j++) {
                            if ((type_records[j].get('flex_value') == '20_MERCHANT')) {
                                record.set('survey_type_code', type_records[j].get('flex_value'));
                                record.set('survey_type_desc', type_records[j].get('flex_desc'));
                            }
                        }
                    }
                } else if (name == 'survey_type_code') {
                    var survey_header_id = record.get('survey_header_id');
                    var survey_type_code = record.get('survey_type_code');
                    record.getMeta().getField('template_name').setLovPara('template_type', survey_type_code);
                    if (record.get('survey_type_code') == '10_MANUFACTURER') {
                        record.getMeta().getField('account_period_desc').setReadOnly(false);
                        record.getMeta().getField('account_period_desc').setRequired(true);
                        document.getElementById('account_period_id').style.display = 'block';
                        $('pur5710_survey_lines_ds').setQueryParameter('vendor_survey_tab_codes', 'VENDOR_SURVEY_MANUFACTURER_TAB');
                        $('pur5710_survey_lines_ds').setQueryUrl($('pur5710_vendor_survey_lines_query_link_1').getUrl());
                        $('pur5710_survey_lines_ds').query();
                    } else if (record.get('survey_type_code') == '30_EQUIPMENT') {
                        record.getMeta().getField('account_period_desc').setReadOnly(true);
                        record.getMeta().getField('account_period_desc').setRequired(false);
                        document.getElementById('account_period_id').style.display = 'none';
                        record.set('account_period_desc', null);
                        record.set('account_period', null);
                        $('pur5710_survey_lines_ds').setQueryParameter('vendor_survey_tab_codes', 'VENDOR_SURVEY_EQUIPMENT_TAB');
                        $('pur5710_survey_lines_ds').setQueryUrl($('pur5710_vendor_survey_lines_query_link_1').getUrl());
                        $('pur5710_survey_lines_ds').query();
                    } else if (record.get('survey_type_code') == '40_BIDIRECTIONAL') {
                        record.getMeta().getField('account_period_desc').setReadOnly(true);
                        record.getMeta().getField('account_period_desc').setRequired(false);
                        document.getElementById('account_period_id').style.display = 'none';
                        record.set('account_period_desc', null);
                        record.set('account_period', null);
                        $('pur5710_survey_lines_ds').setQueryParameter('vendor_survey_tab_codes', 'VENDOR_SURVEY_BIDIRECTE_TAB');
                        $('pur5710_survey_lines_ds').setQueryUrl($('pur5710_vendor_survey_lines_query_link_1').getUrl());
                        $('pur5710_survey_lines_ds').query();
                    } else {
                        record.getMeta().getField('account_period_desc').setReadOnly(true);
                        record.getMeta().getField('account_period_desc').setRequired(false);
                        document.getElementById('account_period_id').style.display = 'none';
                        record.set('account_period_desc', null);
                        record.set('account_period', null);
                        $('pur5710_survey_lines_ds').setQueryParameter('vendor_survey_tab_codes', 'VENDOR_SURVEY_LOGISTICS_TAB');
                        $('pur5710_survey_lines_ds').setQueryUrl($('pur5710_vendor_survey_lines_query_link_1').getUrl());
                        $('pur5710_survey_lines_ds').query();
                    }
            
                }
            }
            
            function pur5710_survey_lines_ds_loadFun(dataSet) {
                debugger;
                var records = dataSet.getAll();
                var header_record = $('pur5710_survey_headers_ds').getAt(0);
                var survey_type_code = header_record.get('survey_type_code');
                if (survey_type_code == '30_EQUIPMENT') {
                    if (!Aurora.isEmpty(survey_type_code)) {
                        header_record.getMeta().getField('template_name').setLovPara('template_type', survey_type_code);
                        header_record.getMeta().getField('template_name').setReadOnly(false);
                        header_record.getMeta().getField('template_name').setRequired(true);
                        $('pur5710_survey_template_hds_ds').setQueryParameter('template_type', survey_type_code);
                        $('pur5710_survey_template_hds_ds').setQueryParameter('default_flag', 'Y');
                        $('pur5710_survey_template_hds_ds').query();
                    } else {
                        header_record.set('tmpl_hds_id', '');
                        header_record.set('template_name', '');
                        header_record.getMeta().getField('template_name').setReadOnly(true);
                        $('pur5710_survey_lines_ds').removeAll();
                    }
                } else if (survey_type_code == '40_BIDIRECTIONAL') {
                    if (!Aurora.isEmpty(survey_type_code)) {
                        header_record.getMeta().getField('template_name').setLovPara('template_type', survey_type_code);
                        header_record.getMeta().getField('template_name').setReadOnly(false);
                        header_record.getMeta().getField('template_name').setRequired(true);
                        $('pur5710_survey_template_hds_ds').setQueryParameter('template_type', survey_type_code);
                        $('pur5710_survey_template_hds_ds').setQueryParameter('default_flag', 'Y');
                        $('pur5710_survey_template_hds_ds').query();
                    } else {
                        header_record.set('tmpl_hds_id', '');
                        header_record.set('template_name', '');
                        header_record.getMeta().getField('template_name').setReadOnly(true);
                        $('pur5710_survey_lines_ds').removeAll();
                    }
                } else {
                    for (var i = 0;i < records.length;i++) {
                        if (records[i].get('survey_tab_code') == '90_ATTACHMENT') {
                            if (records[i].get('enabled_flag') == 'Y') {
                                if (!Aurora.isEmpty(survey_type_code)) {
                                    header_record.getMeta().getField('template_name').setLovPara('template_type', survey_type_code);
                                    header_record.getMeta().getField('template_name').setReadOnly(false);
                                    header_record.getMeta().getField('template_name').setRequired(true);
                                    $('pur5710_survey_template_hds_ds').setQueryParameter('template_type', survey_type_code);
                                    $('pur5710_survey_template_hds_ds').setQueryParameter('default_flag', 'Y');
                                    $('pur5710_survey_template_hds_ds').query();
                                } else {
                                    header_record.set('tmpl_hds_id', '');
                                    header_record.set('template_name', '');
                                    header_record.getMeta().getField('template_name').setReadOnly(true);
                                    $('pur5710_survey_lines_ds').removeAll();
                                }
                            } else {
                                header_record.set('tmpl_hds_id', '');
                                header_record.set('template_name', '');
                                header_record.getMeta().getField('template_name').setReadOnly(true);
                                header_record.getMeta().getField('template_name').setRequired(false);
                            }
                        }
                    }
                }
            }
            
            function pur5710_survey_lines_ds_updateFun(dataSet, record, name, value, oldvalue) {
                if (record.get('survey_tab_code') == '90_ATTACHMENT') {
                    var header_record = $('pur5710_survey_headers_ds').getAt(0);
                    if (record.get('enabled_flag') == 'Y') {
                        header_record.getMeta().getField('template_name').setReadOnly(false);
                        header_record.getMeta().getField('template_name').setRequired(true);
                    } else {
                        header_record.set('tmpl_hds_id', '');
                        header_record.set('template_name', '');
                        header_record.getMeta().getField('template_name').setReadOnly(true);
                        header_record.getMeta().getField('template_name').setRequired(false);
                    }
                }
            }
            
            function pur5710_on_survey_headers_ds_submitsuccessFun(dataSet, res) {
                var survey_header_id = dataSet.getAt(0).get('survey_header_id');
                if (survey_header_id == '' || survey_header_id == '' || survey_header_id == 'undefined') {
                    survey_header_id = res.result.record.survey_header_id;
                }
                Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SAVE_SUCCESS}', null);
                $('pur5710_survey_headers_ds').setQueryParameter('survey_header_id', survey_header_id);
                $('pur5710_survey_headers_ds').query();
            }
            
            function pur5710_survey_template_hds_ds_loadFun(dataSet) {
                var header_record = $('pur5710_survey_headers_ds').getAt(0);
                var survey_header_id = header_record.get('survey_header_id');
                var tmpl_hds_ids = dataSet.getAll();
                if (tmpl_hds_ids.length == 0) {
                    header_record.set('tmpl_hds_id', '');
                    header_record.set('template_name', '');
                    header_record.getMeta().getField('template_name').setReadOnly(true);
                    $('pur5710_survey_lines_ds').removeAll();
                } else {
                    for (var i = 0;i < tmpl_hds_ids.length;i++) {
                        if (survey_header_id == '' || survey_header_id == 'undefined' || survey_header_id == null) {
                            header_record.set('tmpl_hds_id', dataSet.getAt(0).get('tmpl_hds_id'));
                            header_record.set('template_name', dataSet.getAt(0).get('template_name'));
                        }
                    }
                }
            }
            
            function pur5710_selectFcn() {
                var ds = $('pur5710_survey_lines_ds');
                var records = ds.getAll();
                for (var i = 0;i < records.length;i++) {
                    records[i].set('enabled_flag', 'Y');
                }
            }
            
            function pur5710_unselectFcn() {
                var ds = $('pur5710_survey_lines_ds');
                var records = ds.getAll();
                for (var i = 0;i < records.length;i++) {
                    if (records[i].get('survey_tab_code') != '90_ATTACHMENT') {
                        records[i].set('enabled_flag', 'N');
                    }
                }
            }
        ]]></script>
