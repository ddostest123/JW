        <script><![CDATA[
            function pur5710_init() {
                debugger;
                var record = $('pur5710_survey_headers_ds').getAt(0);
                var survey_header_id = record.get('survey_header_id');
                if (survey_header_id == null || survey_header_id == 'undefined' || survey_header_id == '') {
                    var company_name = record.get('company_name');
                    if (company_name == '' || company_name == 'undefined' || company_name == null) {
                        record.getMeta().getField('company_code').setReadOnly(false);
                    }
                }
                
                var vendor_id = '${/parameter/@vendor_id}';
                
                if(vendor_id){
                    var record = $('pur5710_survey_headers_ds').getCurrentRecord();
                    record.set('supplier_business_group','${/model/pur5710_vendor_query/record/@coop_business_group}');
                    record.set('supplier_company_id','${/model/pur5710_vendor_query/record/@coop_company_id}');
                    record.set('vendor_code','${/model/pur5710_vendor_query/record/@vendor_code}');  
                        
                    record.set('vendor_name','${/model/pur5710_vendor_query/record/@vendor_name}');
                    record.set('supplier_categroy','${/model/pur5710_vendor_query/record/@supplier_categroy}');
                    record.set('supplier_categroy_desc','${/model/pur5710_vendor_query/record/@supplier_categroy_desc}');
                    record.set('enterprise_nature_desc','${/model/pur5710_vendor_query/record/@enterprise_nature_desc}');
                    record.set('domestic_foreign_relation_desc','${/model/pur5710_vendor_query/record/@domestic_foreign_relation_desc}');
                    
                    record.getField('company_code').setLovPara('vendor_id',vendor_id);
                    
                    record.getField('display_vendor_code').setReadOnly(true);
                } 
            }
           
            pur5710_init();
        ]]></script>
