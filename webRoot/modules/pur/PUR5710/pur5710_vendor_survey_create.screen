        <script><![CDATA[
            function pur5710_edit_return() {
                $('pur5710_vendor_survey_create_win').close();
            }
            
            function pur5710_release_return() {
            
                $('pur5710_vendor_survey_create_win').close();
            }
            
            //保存
            
            function pur5710_edit_save() {
                debugger;
                var header_record = $('pur5710_survey_headers_ds').getAt(0);
                var line_records = $('pur5710_survey_lines_ds').getAll();
                var survey_header_id = header_record.get('survey_header_id');
                if (!$('pur5710_survey_headers_ds').validate()) {
                    return;
                } else {
                    if (survey_header_id == '' || survey_header_id == null || survey_header_id == 'undefined') {
                        header_record.isNew = 'true';
                        for (i = 0;i < line_records.length;i++) {
                            line_records[i].isNew = 'true';
                        }
                    }
                    Aurora.Masker.mask($('pur5710_vendor_survey_create_win').wrap, '...');
                    $('pur5710_survey_headers_ds').submit();
                    Aurora.Masker.unmask($('pur5710_vendor_survey_create_win').wrap);
                }
            }
            
            //发布
            
            function pur5710_edit_release() {
            
                var record = $('pur5710_survey_headers_ds').getAt(0);
                var survey_header_id = record.get('survey_header_id');
            
                if (survey_header_id == '' || survey_header_id == 'undefined' || survey_header_id == null) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PUR5710.SAVE_FIRST}', null);
                    return '';
                }
                if (!$('pur5710_survey_headers_ds').validate()) {
                    return;
                } else {
                    Aurora.showConfirm('${l:PROMPT}', '${l:PUR5710.CONFIRM_TO_RELEASE}', function() {
                        Aurora.Masker.mask($('pur5710_vendor_survey_create_win').wrap, '...');
                        Aurora.request({
                            url: $('pur5710_survey_release_link').getUrl(),
                            para: {
                                "survey_header_id": survey_header_id
                            },
                            success: function(res) {
                                Aurora.Masker.unmask($('pur5710_vendor_survey_create_win').wrap);
                                pur5710_edit_return();
                                Aurora.showMessage('${l:PROMPT}', '${l:PUR5710.RELEASE_SUCCESS}', null);
                            },
                            scope: this
                        });
                        Aurora.Masker.unmask($('pur5710_vendor_survey_create_win').wrap);
                    }, null);
                }
            }
            
            //删除
            
            function pur5710_edit_delete() {
                var record = $('pur5710_survey_headers_ds').getCurrentRecord();
                var survey_header_id = record.get('survey_header_id');
            
                Aurora.showConfirm('${l:PROMPT}', '${l:PUR5710.CONFIRM_TO_DELETE}', function() {
                    if (survey_header_id == null || survey_header_id == 'undefined' || survey_header_id == '') {
                        pur5710_edit_return();
                    } else {
                        Aurora.Masker.mask($('pur5710_vendor_survey_create_win').wrap, '...');
                        Aurora.request({
                            url: $('pur5710_survey_delete_link').getUrl(),
                            para: {
                                "survey_header_id": survey_header_id
                            },
                            success: function(res) {
                                Aurora.Masker.unmask($('pur5710_vendor_survey_create_win').wrap);
                                pur5710_edit_return();
                                Aurora.showMessage('${l:PROMPT}', '${l:PUR5710.DELETE_SUCCESS}', null);
                            },
                            scope: this
                        });
                        Aurora.Masker.unmask($('pur5710_vendor_survey_create_win').wrap);
                    }
                });
            }
            
