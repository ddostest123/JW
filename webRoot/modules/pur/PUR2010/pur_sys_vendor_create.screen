			//供应商加载事件
			function pur2010_vendor_load_handler(ds){
			    var vendor_code = $('pur2020_pur_system_venders_ds').getCurrentRecord().get('vendor_code');
			    if(vendor_code){
				    //保存后，供应商性质等字段不可维护
				    ds.getCurrentRecord().getField('status_display').setRequired(false);
