	                ds.getCurrentRecord().getField('foreign_vendor_flag_display').setReadOnly(true);
	                ds.getCurrentRecord().getField('d_b_code').setReadOnly(true);
	                ds.getCurrentRecord().getField('national_organization_code').setReadOnly(true);
	                ds.getCurrentRecord().getField('tax_id_number').setReadOnly(true);			        
			    }
			}
			
			//供应商作废			
			function pur2020_vendorDelete(){
