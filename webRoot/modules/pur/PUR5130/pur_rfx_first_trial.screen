            function pur51303_rfxFirstTrialFunction() {
                records = $('pur51303_rfx_ln_items_ds').getAll();
                var count = 0;
                for (i = 0;i < records.length;i++) {
                    if (records[i].get('round_flag') == 'Y') {
                        count++;
            
                    } else {
                        if (!Aurora.isEmpty(records[i].get('cost_price')) && records[i].get('cost_price') != 0 && records[i].dirty) {
                            Aurora.showMessage('${l:PROMPT}', '${l:PUR5130.THERE_ARE_DATA_NOT_SAVED}');
                            return false;
                        }
                    }
                }
                if (count == 0) {
            
                    //校验头上的总成本是否有维护
                    var header_record = $('pur51303_rfx_headers_ds').getCurrentRecord();
                    if (header_record.dirty) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PUR5130.THERE_ARE_DATA_NOT_SAVED}');
                        return false;
            
                    } else {
                        Aurora.showConfirm('${l:PROMPT}', '${l:PROMPT.SUBMIT}', function() {
                            Aurora.request({
                                url: $('pur5130_rfx_first_trail_check_price_link').getUrl(),
                                para: {
                                    'rfx_header_id': '${/parameter/@rfx_header_id}'
                                },
                                success: function(res) {
                                    pur51303_confirmBeforeBack();
                                },
                                failure: function() {},
                                error: function() {},
                                scope: this
                            });
                        });
                    }
            
                } else {
            
                    //调用再次询价请求方法
                    pur51303_rfxFirstTrialRoundAgain(records);
                }
            }
            
            function pur51303_rfxFirstTrialAllFunction() {
            
                item_records = $('pur51303_rfx_ln_items_ds').getAll();
