            function pur51303_priceCompute() {
                var url = $('rfx_price_compute').getUrl();
                var ds = $('pur51303_rfx_headers_ds');
            
                var line_rec = $('pur51303_rfx_ln_items_ds').getAll();
                console.log(line_rec);
                var inv_organization_id = line_rec[0].get('inv_organization_id');
                var record = ds.getCurrentRecord();
                var rfx_category = record.get('rfx_category');
                var rfx_header_id = record.get('rfx_header_id');
                var rfx_round = record.get('round');
                var rfx_version = record.get('version');
                var quotation_id = record.get('quotation_id');
                var currency_code = record.get('currency_code');
                if (rfx_category != 'RLF') {
            
                    new Aurora.Window({
                        url: url + '?rfx_header_id=' + rfx_header_id + '&rfx_round=' + rfx_round + '&rfx_version=' + rfx_version + '&quotation_id=' + quotation_id + '&currency_code=' + currency_code + '&inv_organization_id=' + inv_organization_id,
                        title: '价格测算',
                        id: 'pur_rfx5020_price_compute_win',
                        fullScreen: true
                    });
                } else {
                    var logistics_quotation_code = record.get('logistics_quotation_code');
                    var url;
                    var id;
                    var title;
                    if (logistics_quotation_code == 'EXPRESS_FRAME') {
                        url = $('pur5220_expressFrame_computer_link').getUrl();
                        id = 'pur5130_expressFrameComputer_window';
                        title = '快递框架模型价格测算';
                    } else if (logistics_quotation_code == 'DOMESTIC_LOGISTICS_FRAME') {
                        url = $('pur5220_logisticsFrame_computer_link').getUrl();
                        id = 'pur5130_logisticsFrameComputer_window';
                        title = '国内物流框架模型价格测算';
                    } else if (logistics_quotation_code == 'VEHICLE_LOGISTICS_FRAME') {
                        url = $('pur5220_vehicleFrame_computer_link').getUrl();
                        id = 'pur5130_vehicleFrameComputer_window';
                        title = '整车物流框架模型价格测算';
                    }
            
                    new Aurora.Window({
                        url: url + '?rfx_header_id=' + rfx_header_id,
                        title: title,
                        id: id,
                        fullScreen: true
                    });
                }
            
            }
            
