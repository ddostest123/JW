            function pur51303_rfxFirstTrialSaveFunction() {
                var header_record = $('pur51303_rfx_headers_ds').getCurrentRecord();
                var line_records = $('pur51303_rfx_ln_items_ds').getAll();
                var datas = {
                    "rfx_header_id": header_record.get('rfx_header_id'),
                    "total_cost": header_record.get('total_costs'),
                    "firsttrail_comments": header_record.get('firsttrail_comments') /* 初审备注 add by Harry 9952 2016/11/23 */
                };
            
                //物品行数据
                var line_data = [];
                for (var i = 0;i < line_records.length;i++) {
                    line_data.push({
                        "rfx_ln_item_id": line_records[i].get('rfx_line_item_id'),
                        "cost_price": line_records[i].get('cost_price'),
                        "cost_total_price": line_records[i].get('cost_total_price')
                    });
                }
            
                datas['items_datas'] = line_data;
            
                Aurora.Masker.mask($('pur5130_rfx_first_trail_window').wrap, '${l:LOADING}');
                Aurora.request({
                    url: $('pur5130_rfx_first_trail_save_link').getUrl(),
                    para: datas,
                    success: function() {
                        Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.OPERATE_SUCCESS}', function() {
                            Aurora.Masker.unmask($('pur5130_rfx_first_trail_window').wrap);
                            $('pur51303_rfx_headers_ds').query();
                            $('pur51303_rfx_ln_items_ds').query();
                            $('pur52402_rfx_feedback_all_ds').query();
                        });
                    },
                    failure: function(res) {
                        Aurora.Masker.unmask($('pur5130_rfx_first_trail_window').wrap);
                    },
                    error: function(res) {
                        Aurora.Masker.unmask($('pur5130_rfx_first_trail_window').wrap);
                    },
                    scope: this
                });
            }
            
