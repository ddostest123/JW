            function pur9060_vendor_modify_header_loadFn() {
                var record = $('pur9060_vendor_modify_header_ds').getAt(0);
            
                var iso14000_flag = record.get('iso14000_flag');
                var iso9001_flag = record.get('iso9001_flag');
                var tar1 = document.getElementById('pur9050_iso14000_hBox');
                var tar2 = document.getElementById('pur9050_iso9001_hBox');
                if (iso14000_flag == 'Y') {
                    tar1.style.display = 'block';
                } else {
                    tar1.style.display = 'none';
                }
                if (iso9001_flag == 'Y') {
                    tar2.style.display = 'block';
                } else {
                    tar2.style.display = 'none';
                }
                var modify_status = record.get('modify_status');
                if (modify_status == 'APPROVE' || modify_status == 'REJECT' || modify_status == 'OA_SUBMIT') {
                    $('pur9060_vendor_modify_approve_btn').disable();
                    $('pur9060_vendor_modify_reject_btn').disable();
                    $('pur9060_vendor_oa_submit_btn').disable();
                    record.getField('ou_jxpcb').setReadOnly(true);
                    record.getField('ou_lcfpc').setReadOnly(true);
                    record.getField('ou_lcmccl').setReadOnly(true);
            
                    record.getField('ou_lcmpcb').setReadOnly(true);
                    record.getField('ou_lcpcb').setReadOnly(true);
                    record.getField('ou_lcsmt').setReadOnly(true);
            
                    record.getField('ou_szfpc').setReadOnly(true);
                    record.getField('ou_szpcb').setReadOnly(true);
                    record.getField('ou_zhfpc').setReadOnly(true);
            
                    record.getField('ou_zrfpc').setReadOnly(true);
                    record.getField('ou_zrsmt').setReadOnly(true);
                }
            
            }
            
