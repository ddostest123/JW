            function pur9060_maskWindow() {
                if ($('pur9060_vendor_modify_new')) {
                    Aurora.Masker.mask($('pur9060_vendor_modify_new').wrap, '...');
                } else {
                    Aurora.Masker.mask(Ext.getBody(), '..');
                }
            }
            
            function pur9060_unmaskWindow() {
                if ($('pur9060_vendor_modify_new')) {
                    Aurora.Masker.unmask($('pur9060_vendor_modify_new').wrap, '...');
                } else {
                    Aurora.Masker.unmask(Ext.getBody(), '..');
                }
            }
            
            function pur9060_vendor_oa_submit() {
                debugger;
                //OA提交 景旺事业部 不能为空
                var ds = $('pur9060_vendor_modify_header_ds');
                var record = ds.getCurrentRecord();
                var vendor_modify_id = record.get('vendor_modify_id');
                var data = record.data;
                data.action_code = 'OA_SUBMIT';
                //无变更项时抛出异常
                pur9060_maskWindow();
                Aurora.request({
                    url: $('pur_vendor_modify_oa_submit_link').getUrl(),
                    para: data,
                    success: function(res) {
                        Aurora.showMessage('${l:PROMPT}', '提交OA审批成功！', null);
                        pur9060_unmaskWindow();
                        pur9060_vendor_modify_back();
                    },
                    failure: function() {
                        pur9060_unmaskWindow();
                    },
                    error: function() {
                        pur9060_unmaskWindow();
                    },
                    scope: this
                });
            }
            
