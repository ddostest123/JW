                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}');
                    return;
                }
                new Aurora.Window({
                    url: $('pur5180_demandPoolAssignDetail_link').getUrl() + '?type=new',
                    title: '${l:PUR5180.ASSIGN_INQUIRY_STAFF}',
                    id: 'pur5180_demandPoolAssignDetail_window',
                    height: 160,
                    width: 420
                }).on('beforeclose', function() {
                    pur5180_demandPoolQueryFunction();
                    pur5180_requisitionQueryFunction();
                });
            }
            
            function pur5180_demandPoolDeassignAssignFunction() {
                var records = $('pur5180_demand_pool_assigned_result_ds').getSelected();
                if (records.length == 0) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}');
                    return;
                }
                
                   for (var i = 0;i < records.length;i++) {
                    var status = records[i].get('req_status');
                    if (status != 'ASSIGNED') {
                        var req_number =records[i].get('req_number');
                        var line_num =records[i].get('line_num');
                        Aurora.showMessage('${l:PROMPT}', '申请编号：'+req_number+' 行号：'+line_num+' 申请人已处理，无法重新分配！');
                        return;
                    }
                   
                }
                new Aurora.Window({
                    url: $('pur5180_demandPoolAssignDetail_link').getUrl() + '?type=repeat',
                    title: '${l:PUR5180.ASSIGN_INQUIRY_STAFF_AGAIN}',
                    id: 'pur5180_demandPoolDeassignAssignDetail_window',
                    height: 160,
                    width: 420
                }).on('close', function() {
                    pur5180_demandPoolQueryFunction();
                    pur5180_requisitionQueryFunction();
                    pur5180_demandPoolAssignedQueryFunction();
                });
            }
            
            function pur5180_demandPoolDeassignFunction() {
