            function pur5180_requisitionQueryFunction() {
                $('pur5180_requisition_result_ds').query();
            }
            
            function pur5180_requisitionCreateFunction() {
                new Aurora.Window({
                    url: $('pur5180_requisitionCreate_link').getUrl(),
                    title: '${l:PUR_REQUISITION_CREATION_TITLE}',
                    id: 'pur5180_requisitionCreate_window',
                    fullScreen: true
                });
            }
            
            function pur5180_requisitionNumRenderFunction(value, record, name) {
                var req_header_id = record.get('req_header_id');
                if (name == 'req_number') {
                    return '<a href="javascript:pur5180_requisitionEditFunction(' + req_header_id + ')">' + value + '</a>';
                }
            }
            
            function pur5180_requisitionEditFunction(req_header_id) {
                new Aurora.Window({
                    url: $('pur5180_requisitionCreate_link').getUrl() + '?req_header_id=' + req_header_id,
                    title: '${l:PUR_REQUISITION_MODIFIED_TITLE}',
                    id: 'pur5180_requisitionCreate_window',
                    fullScreen: true
                }).on('beforeclose', function() {
                    pur5180_requisitionQueryFunction();
                });
            }
            
            //操作记录
            
            function pur_recordRender(value, record, name) {
                if (record.isNew) {
                    return '${l:PUR5180.NO_OPERATION_RECORD}';
                }
                return '<a href="javascript:pur_openRecord(' + record.get('demand_pool_id') + ')">${l:PORMPT.OPERATION_RECORD}</a>';
            }
            
            function pur_openRecord(id) {
                new Aurora.Window({
                    id: 'pur5180_operation_record_window',
                    title: '${l:PROMPT.OPERATION_RECORD}',
                    url: $('bid5180_operation_record_link').getUrl() + '?operation_table=pur_demand_pool' + '&operation_table_id=' + id,
                    height: 450,
                    width: 600
                });
            }
            
            function pur5180_assign_pauseFunction() {
                var records = $('pur5180_demand_pool_assigned_result_ds').getSelected();
                if (records.length == 0) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}');
                    return;
                }
            
                new Aurora.Window({
                    url: $('pur5180_demand_pool_pause_comment_link').getUrl() + '?ds=pur5180_demand_pool_assigned_result_ds',
                    title: '${l:PUR5180.PENDING_DESCRIPTION}',
                    id: 'pur5180_demandPoolPauseComment_window',
                    height: 200,
                    width: 400
                }).on('beforeclose', function() {
                    pur5180_demandPoolQueryFunction();
                });
            
            
            
            }
            
            function pur5180_unassign_pauseFunction() {
                var records = $('pur5180_demand_pool_result_ds').getSelected();
                if (records.length == 0) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}');
                    return;
                }
                new Aurora.Window({
                    url: $('pur5180_demand_pool_pause_comment_link').getUrl() + '?ds=pur5180_demand_pool_result_ds',
                    title: '${l:PUR5180.PENDING_DESCRIPTION}',
                    id: 'pur5180_demandPoolPauseComment_window',
                    height: 200,
                    width: 400
                }).on('beforeclose', function() {
                    pur5180_demandPoolQueryFunction();
                });
            
            
            }
            
            function pur5180_assign_pause_backFunction() {
                var data = [];
                var line_data = [];
                var pool_pause_records = $('pur5180_demand_pool_pause_result_ds').getSelected();
                for (var i = 0;i < pool_pause_records.length;i++) {
                    line_data = {
                        'demand_pool_id': pool_pause_records[i].data.demand_pool_id,
                        '_status': 'update'
                    };
                    data.push(line_data);
                }
            
                Aurora.request({
                    url: $('pur5180_pur_demand_pool_pause_link').getUrl(),
                    para: data,
                    success: function(result) {
                        $('pur5180_demand_pool_assigned_result_ds').query();
                        $('pur5180_demand_pool_result_ds').query();
                        $('pur5180_demand_pool_pause_result_ds').query();
                        return;
                    },
                    failure: function() {
            
                        return;
                    },
                    error: function() {
            
                        return;
                    }
                });
            }
            
            function pur5180_demand_pool_result_grid_upload_render_fun(value, record, name) {
                if (name == 'atm_update') {
                    var demand_pool_id = record.get('demand_pool_id');
                    if (!record.isNew && demand_pool_id) {
                        return '<a href="javascript:pur5180_demand_pool_result_grid_upload_fun(' + demand_pool_id + ')">${l:PROMPT.UPLOAD_DOWNLOAD}</a>';
                    }
                } else if (name == 'atm_flag') {
                    var atm_show_flag = record.get('atm_show_flag');
                    if (atm_show_flag == 'Y') {
                        return '<div style="margin-top:3px;margin-left:auto;margin-right:auto;vertical-align:middle;height:20px;line-height:20px;"><img src="${/request/@context_path}/images/paperclip.png"/></div>';
            
                    } else {
                        return '';
                    }
                }
            }
            
            function pur5180_demand_pool_result_grid_upload_fun(id) {
                var url = "${/request/@context_path}/uploadFile.screen?table_name=PUR_DEMAND_POOL&header_id=" + id;
                new Aurora.Window({
                    url: url,
                    title: '${l:PROMPT.UPLOAD_DOWNLOAD}',
                    id: 'pur5180_demand_pool_result_grid_upload_window',
                    width: 850,
                    height: 500
                });
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="pur5180_rfx_types_ds" autoCreate="true" loadData="true">
                <a:datas dataSource="/model/pur5180_rfx_types_source"/>
            </a:dataSet>
            <a:dataSet id="pur5180_requisition_query_ds"/>
            <a:dataSet id="pur5180_status_ds" lookupCode="PUR_DEMAND_POOL_STATUS"/>
            <a:dataSet id="pur5180_assign_status_ds" loadData="true" model="pur.PUR5180.pur_demand_pool_status"/>
            <a:dataSet id="pur5180_data_source_ds" lookupCode="DATA_SOURCE"/>
            <a:dataSet id="pur5180_requisition_result_ds" autoCount="true" autoPageSize="true" model="pur.PUR5180.pur_requisition_header" queryDataSet="pur5180_requisition_query_ds" selectable="true"/>
            <a:dataSet id="pur5180_demand_pool_query_ds">
                <a:fields>
                    <a:field name="data_source" displayField="code_value_name" options="pur5180_data_source_ds" returnField="data_source" valueField="code_value"/>
                    <a:field name="req_status_code"/>
                    <a:field name="req_status_desc" displayField="code_value_name" options="pur5180_status_ds" returnField="req_status_code" valueField="code_value"/>
                    <a:field name="zero_flag" checkedValue="Y" defaultValue="N" uncheckedValue="N"/>
                    <!-- <a:field name="item_code"/> -->
                    <a:field name="item_code" autoComplete="true" autoCompleteField="code_name" lovGridHeight="320" lovHeight="480" lovService="public.mtl_system_items_b_mv_lov" lovWidth="500">
                        <a:mapping>
                            <!-- <a:map from="item_id" to="item_id"/>
                            <a:map from="item_code" to="item_code"/> -->
                            <a:map from="item_code" to="item_code"/>
                        </a:mapping>
                    </a:field>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="pur5180_demand_pool_result_ds" autoCount="true" autoPageSize="true" model="pur.PUR5180.pur_demand_pool" queryDataSet="pur5180_demand_pool_query_ds" queryUrl="${/request/@context_path}/autocrud/pur.PUR5180.pur_demand_pool/query?assigned_flag=N&amp;pause_flag=N" selectable="true"/>
            <a:dataSet id="pur5180_demand_pool_assigned_query_ds">
                <a:fields>
                    <a:field name="data_source" displayField="code_value_name" options="pur5180_data_source_ds" returnField="data_source" valueField="code_value"/>
                    <a:field name="rfx_type_code"/>
                    <a:field name="rfx_type_desc" displayField="rfx_type_desc" options="pur5180_rfx_types_ds" returnField="rfx_type_code" valueField="rfx_type_code">
                        <a:mapping>
                            <a:map from="rfx_type_code" to="rfx_type_code"/>
                            <a:map from="rfx_type_desc" to="rfx_type_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="req_status_code"/>
                    <a:field name="req_status_desc" displayField="code_value_name" options="pur5180_assign_status_ds" returnField="req_status_code" valueField="code_value"/>
                    <a:field name="zero_flag" checkedValue="Y" defaultValue="N" uncheckedValue="N"/>
                    <!-- <a:field name="item_code"/> -->
                    <a:field name="item_code" autoComplete="true" autoCompleteField="code_name" lovGridHeight="320" lovHeight="480" lovService="public.mtl_system_items_b_mv_lov" lovWidth="500">
                        <a:mapping>
                            <!-- <a:map from="item_id" to="item_id"/>
                            <a:map from="item_code" to="item_code"/> -->
                            <a:map from="item_code" to="item_code"/>
                        </a:mapping>
                    </a:field>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="pur5180_demand_pool_assigned_result_ds" autoCount="true" autoPageSize="true" model="pur.PUR5180.pur_demand_pool" queryDataSet="pur5180_demand_pool_assigned_query_ds" queryUrl="${/request/@context_path}/autocrud/pur.PUR5180.pur_demand_pool/query?ORDER_FIELD=assigned_date desc ,req_number,line_num&amp;assigned_flag=Y&amp;pause_flag=N" selectable="true"/>
            <a:dataSet id="pur5180_demand_pool_pause_query_ds">
                <a:fields>
                    <a:field name="vendor_flag" checkedValue="Y" defaultValue="Y" uncheckedValue="N"/>
                    <a:field name="data_source" displayField="code_value_name" options="pur5180_data_source_ds" returnField="data_source" valueField="code_value"/>
                    <a:field name="req_status_code"/>
                    <a:field name="req_status_desc" displayField="code_value_name" options="pur5180_status_ds" returnField="req_status_code" valueField="code_value"/>
                    <a:field name="rfx_type_code"/>
                    <a:field name="rfx_type_desc" displayField="rfx_type_desc" options="pur5180_rfx_types_ds" returnField="rfx_type_code" valueField="rfx_type_code">
                        <a:mapping>
                            <a:map from="rfx_type_code" to="rfx_type_code"/>
                            <a:map from="rfx_type_desc" to="rfx_type_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="item_code" autoComplete="true" autoCompleteField="code_name" lovGridHeight="320" lovHeight="480" lovService="public.mtl_system_items_b_mv_lov" lovWidth="500">
                        <a:mapping>
                            <!-- <a:map from="item_id" to="item_id"/>
                            <a:map from="item_code" to="item_code"/> -->
                            <a:map from="item_code" to="item_code"/>
                        </a:mapping>
                    </a:field>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="pur5180_demand_pool_pause_result_ds" autoCount="true" autoPageSize="true" model="pur.PUR5180.pur_demand_pool" queryDataSet="pur5180_demand_pool_pause_query_ds" queryUrl="${/request/@context_path}/autocrud/pur.PUR5180.pur_demand_pool/query?pause_flag=Y" selectable="true"/>
            <a:dataSet id="pur5180_agreement_delay_result_ds" autoCount="true" autoPageSize="true" autoQuery="true" model="pur.PUR5130.pur_agreement_delay_result" queryDataSet="pur5180_agreement_delay_query_ds" selectable="true"/>
