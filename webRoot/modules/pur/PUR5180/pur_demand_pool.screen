                        </a:mapping>
                    </a:field>
                </a:fields>
            </a:dataSet>
        </a:dataSets>
        <a:screenBody>
            <a:tabPanel marginHeight="80" marginWidth="100">
                <a:tabs>
                    <a:tab prompt="PUR5180.APPLY_FOR_CREATION" width="130">
                        <a:screenTopToolbar>
                            <a:gridButton click="pur5180_requisitionCreateFunction" text="PROMPT.INSERT" width="100"/>
                        </a:screenTopToolbar>
                        <a:queryForm bindTarget="pur5180_requisition_query_ds" resultTarget="pur5180_requisition_result_ds" style="border:none">
                            <a:formToolBar>
                                <a:hBox labelWidth="120">
                                    <a:textField name="req_number" prompt="PUR5180.APPLY_CODE">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_requisitionQueryFunction"/>
                                        </a:events>
                                    </a:textField>
                                </a:hBox>
                            </a:formToolBar>
                        </a:queryForm>
                        <a:grid id="pur5180_requisition_result_grid" autoAppend="false" bindTarget="pur5180_requisition_result_ds" marginHeight="200" marginWidth="100" navBar="true">
                            <a:columns>
                                <a:column name="req_number" prompt="PUR5180.APPLY_CODE" renderer="pur5180_requisitionNumRenderFunction" width="100"/>
                                <a:column name="type_lookup_desc" prompt="PUR_REQ_REF_TYPES.REQUISITION_TYPE_ID" width="50"/>
                                <a:column name="company_name" prompt="PUR_REQUISITION_HDS.COMPANY_ID" width="100"/>
                                <a:column name="business_unit_name" prompt="PUR_REQ_HEADERS.UNIT_ID" width="100"/>
                                <a:column name="unit_desc" prompt="PUR5120.UNIT" width="80"/>
                                <a:column name="pur_organization_name" prompt="PUR_REQUISITION_HDS.PUR_ORGANIZATION_ID" width="100"/>
                                <a:column name="project_name" prompt="PUR_REQUISITION_HDS.PROJECT_NAME" width="100"/>
                                <a:column name="association_agreement" prompt="PUR_REQUISITION_HDS.ASSOCIATION_AGREEMENT" width="100"/>
                            </a:columns>
                        </a:grid>
                    </a:tab>
                    <a:tab prompt="PUR5180.APPLICATION_TO_BE_ASSIGNED" selected="true" width="130">
                        <a:screenTopToolbar>
                            <a:gridButton click="pur5180_demandPoolAssignFunction" text="PUR5180.DISTRIBUTION" width="100"/>
                            <a:gridButton click="pur5180_unassign_pauseFunction" text="PUR5180.PENDING" width="100"/>
                            <a:gridButton bind="pur5180_demand_pool_result_grid" style="margin-left:5px;" type="excel" width="100"/>
                        </a:screenTopToolbar>
                        <a:queryForm bindTarget="pur5180_demand_pool_query_ds" resultTarget="pur5180_demand_pool_result_ds" style="border:none">
                            <a:formToolBar>
                                <a:hBox labelWidth="120">
                                    <a:textField name="req_number" prompt="PUR5180.APPLY_CODE">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:textField>
                                    <a:textField name="line_num" prompt="PUR5220.LINE_NUMBER">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:textField>
                                    <a:lov name="item_code" prompt="PUR_ITEM_PRICE_HEADERS.ITEM_CODE">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:lov>
                                </a:hBox>
                            </a:formToolBar>
                            <a:formBody>
                                <a:hBox labelWidth="120">
                                    <a:comboBox name="req_status_desc" prompt="PUR5710.STATUS_DESC">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:comboBox>
                                    <a:comboBox name="data_source" prompt="PUR_HEADERS_ALL.DATA_SOURCE">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:comboBox>
                                    <a:checkBox name="zero_flag" prompt="PUR5180.JUST_LOOK_AT_THE_PRICE">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:checkBox>
                                </a:hBox>
                                <a:hBox labelWidth="120">
                                    <a:datePicker name="need_by_date_from" prompt="PUR5120.DEMAND_DATE_FROM">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:datePicker>
                                    <a:datePicker name="need_by_date_to" prompt="PUR5120.DEMAND_DATE_TO">
                                        <a:events>
                                            <a:event name="enterdown" handler="pur5180_demandPoolQueryFunction"/>
                                        </a:events>
                                    </a:datePicker>
                                </a:hBox>
                            </a:formBody>
                        </a:queryForm>
                        <a:grid id="pur5180_demand_pool_result_grid" autoAppend="false" bindTarget="pur5180_demand_pool_result_ds" marginHeight="200" marginWidth="100" navBar="true">
                            <a:columns>
                                <a:column name="req_number" prompt="PUR5180.APPLY_CODE" width="100"/>
                                <a:column name="line_num" prompt="PUR_LINES_ALL.LINE_NUM" width="40"/>
                                <a:column name="req_status_desc" prompt="PUR_REQUISITION_LINES.REQUISITION_STATUS" width="60"/>
                                <a:column name="item_code" prompt="PUR_REQUISITION_LNS.ITEM_CODE" width="100"/>
                                <a:column name="item_desc" prompt="PUR_REQUISITION_LNS.ITEM_DESC" width="100"/>
                                <a:column name="quantity" align="right" prompt="PUR_REQUISITION_LNS.PRIMARY_QUANTITY" width="40"/>
                                <a:column name="unit_meas_lookup_code" align="right" prompt="PUR_REQUISITION_LNS.PRIMARY_UOM" width="40"/>
                                <a:column name="unit_price" align="right" prompt="PUR_REQUISITION_LNS.ENTERED_PRICE" width="50"/>
