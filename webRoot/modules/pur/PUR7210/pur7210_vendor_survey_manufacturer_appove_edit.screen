            function pur5730_vendor_survey_manufacturer_business_tab_ds_updateFun(value, record, name) {
            
                var min_lot_size_flag = record.get('min_lot_size_flag');
                var incoming_inspection_flag = record.get('incoming_inspection_flag');
                var ex_factory_inspection_flag = record.get('ex_factory_inspection_flag');
                var production_inspection_flag = record.get('production_inspection_flag');
                var sea_transport_flag = record.get('sea_transport_flag');
            
            
                if (name == "min_lot_size_flag") {
                    if (min_lot_size_flag == 'Y') {
                        record.getField('min_lot_size').setReadOnly(false);
            
                        record.getField('min_lot_size').setRequired(true);
                    } else if (min_lot_size_flag == 'N') {
                        record.set('min_lot_size', '');
                        record.getField('min_lot_size').setReadOnly(true);
                        record.getField('min_lot_size').setRequired(false);
                    }
                }
                if (name == "incoming_inspection_flag" || name == "ex_factory_inspection_flag" || name == "production_inspection_flag") {
                    if (incoming_inspection_flag == 'Y' || ex_factory_inspection_flag == 'Y' || production_inspection_flag == 'Y') {
                        record.getField('inspection_agency').setReadOnly(false);
                        record.getField('inspection_staffcount').setReadOnly(false);
            
                        record.getField('inspection_agency').setRequired(true);
                        record.getField('inspection_staffcount').setRequired(true);
                    } else {
                        record.set('inspection_agency', '');
                        record.set('inspection_staffcount', '');
                        record.getField('inspection_agency').setReadOnly(true);
                        record.getField('inspection_staffcount').setReadOnly(true);
                        record.getField('inspection_agency').setRequired(false);
                        record.getField('inspection_staffcount').setRequired(false);
                    }
                }
                if (name == "sea_transport_flag") {
                    if (sea_transport_flag == 'Y') {
                        record.getField('c_attribute1').setReadOnly(false);
            
                        record.getField('c_attribute1').setRequired(true);
                    } else if (sea_transport_flag == 'N') {
                        record.set('c_attribute1', '');
                        record.getField('c_attribute1').setReadOnly(true);
                        record.getField('c_attribute1').setRequired(false);
                    }
                }
            
            }
            
