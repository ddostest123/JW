    </a:init-procedure>
    <a:view>
        <a:link id="pur7210_vendor_survey_merchant_record_link" url="${/request/@context_path}/modules/pur/PUR7210/pur7210_vendor_supply_record.screen"/>
        <a:link id="pur7210_attachment_file_upload_link" url="${/request/@context_path}/modules/pur/PUR7270/attachment_file_view.screen"/>
