        <a:link id="pur7210_vendor_survey_merchant_vendor_detail_link" url="${/request/@context_path}/modules/pur/PUR5660/pur_com_supplier_detail.screen"/>
        <script><![CDATA[
            function pur7210_detail_return() {
                $('pur7210_vendor_survey_detail_win').close();
            }
            
            function pur7210_vendor_supply_record() {
                new Aurora.Window({
                    id: 'pur7210_vendor_survey_record_win',
                    title: '${l:PUR_VENDORS.OPERATION_RECORD}',
                    url: $('pur7210_vendor_survey_merchant_record_link').getUrl() + '?survey_header_id=${/parameter/@survey_header_id}',
                    width: 700,
                    height: 410
                });
            }
            
            function pur7210_vendor_survey_merchant_rendererFun(value, record, name) {
                var survey_header_id = record.get('survey_header_id');
                if (name == 'organization') {
	                return '<a style="color:blue" href="javascript:pur7210_vendor_survey_merchant_uploadFun(' + survey_header_id + ')">${l:PUR_VENDORS.VIEW_ATTACHMENT}</a>';
                }
            
                if (name == 'stock_right') {
                    return '<a style="color:blue" href="javascript:pur7210_vendor_survey_merchant_stock_uploadFun(' + survey_header_id + ')">${l:PUR_VENDORS.VIEW_ATTACHMENT}</a>';
                }
            }
            
            function pur7210_vendor_survey_merchant_uploadFun(id) {
                new Aurora.Window({
                    id: 'pur7210_attachment_file_upload_link_window',
                    title: '${l:PUR_VENDORS.VIEW_ATTACHMENT}',
                    url: $('pur7210_attachment_file_upload_link').getUrl() + '?pkvalue=' + id + '&table_name=PUR_VENDOR_SURVEY_BASIC_INFO_ME',
                    width: 610,
                    height: 500
                });
            }
            
            function pur7210_vendor_survey_merchant_stock_uploadFun(id) {
                new Aurora.Window({
                    id: 'pur7210_attachment_file_upload_link_window',
                    title: '${l:PUR_VENDORS.VIEW_ATTACHMENT}',
                    url: $('pur7210_attachment_file_upload_link').getUrl() + '?pkvalue=' + id + '&table_name=PUR_VENDOR_SURVEY_BASIC_INFO_STOCK',
                    width: 610,
                    height: 500
                });
            }
            //查看采购方附件
            function pur7210_survey_atm_upload_rendererFun_buyer(value, record, name) {
                if (name == 'attachment_id') {
                    //if ('${/parameter/@release_flag}' != 'Y') {
                        var attachment_id = record.get('attachment_id');
                        if (record.get('atm_count_buyer') == 0) {
                            return '<a style="color:blue" href="javascript:pur7210_vendor_survey_uploadFun_buyer(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                        } else {
                            return '<a style="color:blue" href="javascript:pur7210_vendor_survey_uploadFun_buyer(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>' + '<img id="pur5730_vendor_survey_manufacturer_uploadBtu_img_1" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                        }                    
                    //} else {
                     //   return '';
                    //}
                } else if (name == 'last_upload_date') {
                    if ('${/parameter/@release_flag}' != 'Y') {
                        return value;
                    } else {
                        return '';
                    }
                }
            }
            // 查看供应商附件
            function pur7210_survey_atm_upload_rendererFun(value, record, name) {
                if (name == 'attachment_id') {
                    if ('${/parameter/@release_flag}' != 'Y') {
                        var attachment_id = record.get('attachment_id');
                        if (record.get('atm_count_vendor') == 0) {
                            return '<a style="color:blue" href="javascript:pur7210_vendor_survey_uploadFun(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                        } else {
                            return '<a style="color:blue" href="javascript:pur7210_vendor_survey_uploadFun(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>' + '<img id="pur5730_vendor_survey_manufacturer_uploadBtu_img_2" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                        } 
                    } else {
                        return '';
                    }
                } else if (name == 'last_upload_date') {
                    if ('${/parameter/@release_flag}' != 'Y') {
                        return value;
                    } else {
                        return '';
                    }
                }
            }
            
            function pur7210_vendor_survey_uploadFun_buyer(id) {
                new Aurora.Window({
                    id: 'pur7210_attachment_file_upload_link_window',
                    title: '${l:PROMPT.BUYER_ATTACHMENT}',
                    url: $('pur7210_attachment_file_upload_link').getUrl() + '?pkvalue=' + id + '&table_name=PUR_VENDOR_SURVEY_ATTACHMENT_BUYER',
                    width: 610,
                    height: 500
                });
            }
            
            function pur7210_vendor_survey_uploadFun(id) {
                new Aurora.Window({
                    id: 'pur7210_attachment_file_upload_link_window',
                    title: '${l:PROMPT.VENDOR_ATTACHMENT}',
                    url: $('pur7210_attachment_file_upload_link').getUrl() + '?pkvalue=' + id + '&table_name=PUR_VENDOR_SURVEY_ATTACHMENT',
                    width: 610,
                    height: 500
                });
            }
            
            
	    //保存pdf
            function pur7210_survey_merchant_detail_export(){
                Aurora.request({
                    url: $('pur7210_vendor_survey_merchant_export_link').getUrl()+ '?survey_header_id=${/parameter/@survey_header_id}',
                    success: function(res) {
                        window.open("${/request/@context_path}" + res.result.fileName);
                    }
                });
            }
            
            function pur7210_survey_merchant_vendor_view() {
                var coop_company_id = $('pur7210_survey_merchant_headers_ds').getAt(0).get('supplier_company_id');
                new Aurora.Window({
                    url: $('pur7210_vendor_survey_merchant_vendor_detail_link').getUrl() + '?coop_company_id=' + coop_company_id,
                    id: 'pur5660_com_supplier_window',
                    fullScreen: true
                });
            }
        ]]></script>
