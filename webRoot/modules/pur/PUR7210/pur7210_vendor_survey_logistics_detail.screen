        <script><![CDATA[
            function diaplayTab(id) {
                var ui = document.getElementById(id);
                ui.style.display = "none";
            }
            
            function init() {
            
                var status = '${/parameter/@status}';
                //将审批意见隐藏
                if (status == '20_RELEASED' || status == '30_SUBMITTED') {
                    diaplayTab('pur7210_logistics_fieldSet_id');
                    diaplayTab('pur7210_logistics_approve_comments_id');
                }
            
                var tab_records = $('pur7210_vendor_survey_logistics_tabs_ds').getAll();
                var length = tab_records.length - 1;
                for (var i = 0;i <= length;i++) {
                    if (tab_records[length - i].get('survey_tab_code') == '90_ATTACHMENT') {
                        if (tab_records[length - i].get('enabled_flag') == 'N') {
                            diaplayTab("vendor_survey_logistics_attachment_tab_id");
                        }
                    } else if (tab_records[length - i].get('survey_tab_code') == '88_MANAGER') {
                        if (tab_records[length - i].get('enabled_flag') == 'N') {
                            diaplayTab("vendor_survey_logistics_manager_tab_id");
                        }
                    } else if (tab_records[length - i].get('survey_tab_code') == '87_TRANSACTION') {
                        if (tab_records[length - i].get('enabled_flag') == 'N') {
                            diaplayTab("vendor_survey_logistics_transaction_tab_id");
                        }
                    } else if (tab_records[length - i].get('survey_tab_code') == '86_COMPETITION') {
                        if (tab_records[length - i].get('enabled_flag') == 'N') {
                            diaplayTab("vendor_survey_logistics_competition_tab_id");
                        }
                    } else if (tab_records[length - i].get('survey_tab_code') == '85_FINANCE') {
                        if (tab_records[length - i].get('enabled_flag') == 'N') {
                            diaplayTab("vendor_survey_logistics_fin_tab_id");
                        }
                    } else if (tab_records[length - i].get('survey_tab_code') == '80_CUSTOMER') {
                        if (tab_records[length - i].get('enabled_flag') == 'N') {
                            diaplayTab("vendor_survey_logistics_custome_tab_id");
                        }
                    } else if (tab_records[length - i].get('survey_tab_code') == '70_SUBCONTRACT') {
                        if (tab_records[length - i].get('enabled_flag') == 'N') {
                            diaplayTab("vendor_survey_logistics_subcontract_tab_id");
