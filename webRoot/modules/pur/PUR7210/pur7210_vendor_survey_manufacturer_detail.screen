                    height: 410
                });
            }
            
            function pur7210_vendor_survey_manufacturer_rendererFun(value, record, name) {
                var survey_header_id = record.get('survey_header_id');
                if (name == 'organization') {
                    return '<a style="color:blue" href="javascript:pur7210_vendor_survey_manufacturer_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
            
                if (name == 'stock_right') {
                    return '<a style="color:blue" href="javascript:pur7210_vendor_survey_manufacturer_stock_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
            
                if (name == 'employment_qualifications') {
                    return '<a style="color:blue" href="javascript:employment_qualifications_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
                if (name == 'major_pro_capacity') {
                    return '<a style="color:blue" href="javascript:pur7210_pro_capacity_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
                if (name == 'major_equipment_attch') {
                    return '<a style="color:blue" href="javascript:pur7210_equipment_attch_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
            }
            
