    </a:init-procedure>
    <a:view>
        <a:link id="pur7130_rfx_details_link" url="${/request/@context_path}/modules/pur/PUR7130/pur_rfx_details.screen"/>
        <a:link id="pur7130_rfxResultView_link" url="${/request/@context_path}/modules/pur/PUR7130/pur_bidding_result_view.screen"/>
        <a:link id="pur7130_rfx_history_details_link" url="${/request/@context_path}/modules/pur/PUR7130/pur_rfx_history_details_vendor.screen"/>
        <script><![CDATA[
            function pur7130_query() {
                $('pur7130_rfx_header_result_ds').query();
            }
            
            function pur7130_rfx_render(value, record, name) {
                var rfx_header_id = record.get('rfx_header_id');
                var coop_company_id = record.get('coop_company_id');
                var atm_show_flag = record.get('atm_show_flag');
                var feedback_atm_counts = record.get('feedback_atm_counts');
                var prequal_atm_counts = record.get('prequal_atm_counts');
                if (name == 'rfx_number') {
                    return '<a href="javascript:pur7130_rfx_window(' + rfx_header_id + ',' + coop_company_id + ',' + feedback_atm_counts + ',' + prequal_atm_counts + ')">' + value + '</a>';
                } else if (name == 'result_publicity_view') {
                    var status = record.get('status');
                    if (status == 'FINISHED') {
                        return '<a href="javascript:pur7130_rfxResultViewFun(' + rfx_header_id + ')">${l:BID_RESULT_PUBLICITY_RULES.VIEW}</a>';
                    }
                }
                
                if(name == "atm_flag"){
                    if(atm_show_flag == 'Y'){
                        return '<div style="margin-top:3px;margin-left:auto;margin-right:auto;vertical-align:middle;height:20px;line-height:20px;"><img src="${/request/@context_path}/images/paperclip.png"/></div>';
                    }
                }
            }
            
            function pur7130_rfx_window(id, coop_company_id, atm_counts, prequal_atm_counts) {
                var window_id;
                var url;
                window_id = 'pur71301_rfx_details_window';
                url = $('pur7130_rfx_details_link').getUrl();
                url += '?rfx_header_id=' + id + '&coop_company_id=' + coop_company_id + '&atm_counts=' + atm_counts + '&prequal_atm_counts=' + prequal_atm_counts;
                new Aurora.Window({
                    id: window_id,
                    title: '${l:PUR_RFX_HEADERS.VENDOR_QUOTATION}',
                    url: url,
                    fullScreen: true
                }).on('beforeclose', pur7130_query);
            }
            
            
            //Query Ds Update Function
            
            function pur7130_onRfxFeedbackQueryDs_updateFun(ds, record, name, value, oldvalue) {
                if (name == 'company_id') {
                    record.set('business_unit_id', '');
                    record.set('business_unit_code', '');
                    record.set('business_unit_name', '');
                    if (Ext.isEmpty(value)) {
                        record.getField('business_unit_name').setReadOnly(true);
                        record.getField('business_unit_name').setLovPara('company_id', -1);
                    } else {
                        record.getField('business_unit_name').setReadOnly(false);
                        record.getField('business_unit_name').setLovPara('company_id', value);
                    }
                }
            }
            
            function pur7130_rfxResultViewFun(rfx_header_id) {
                new Aurora.Window({
                    id: 'pur7130_biddingResultPreview_window',
                    title: '${l:BID_RESULT_PUBLICITY_RULES.VIEW}',
                    url: $('pur7130_rfxResultView_link').getUrl() + '?rfx_header_id=' + rfx_header_id,
                    height: 400,
                    width: 800
                });
            }
            
              function pur7130_history_query() {
                $('pur7110_rfx_header_history_result_ds').query();
            }
                   function pur7130_rfx_history_render(value, record, name) {
                var rfx_header_id = record.get('rfx_header_id');
                var his_id = record.get('history_header_id');
                var atm_counts = record.get('atm_counts');
                var prequal_atm_counts = record.get('prequal_atm_counts');
            
                if (name == "rfx_number") {
                    return '<a href="javascript:pur7130_rfx_history_window(' + rfx_header_id + ',' + his_id + ',' + atm_counts + ',' + prequal_atm_counts + ')">' + value + '</a>';
                }
            
                if (name == "atm_flag") {
                    if (atm_counts > 0) {
                        return '<div style="margin-top:3px;margin-left:auto;margin-right:auto;vertical-align:middle;height:20px;line-height:20px;"><img src="${/request/@context_path}/images/paperclip.png"/></div>';
            
                    } else {
                        return '';
                    }
                }
            }
            
            function pur7130_rfx_history_window(id, his_id, atm_counts, prequal_atm_counts) {
                var window_id;
                var url;
                window_id = 'pur7110_rfx_history_details_window';
                url = $('pur7130_rfx_history_details_link').getUrl();
                url += '?rfx_header_id=' + id + '&history_header_id=' + his_id + '&atm_counts=' + atm_counts + '&prequal_atm_counts=' + prequal_atm_counts;
                new Aurora.Window({
                    id: window_id,
                    title: '${l:PUR7110.RFX_DETAIL}',
                    url: url,
                    fullScreen: true
                });
            }
        ]]></script>
