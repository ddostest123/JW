<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure>
        <a:model-query fetchAll="true" model="public.pur_rfx_types_list" rootPath="pur7130_rfx_types_source"/>
        <a:model-query fetchAll="true" model="public.csh_payment_methods_list" rootPath="pur7130_payment_methods_source"/>
        <a:model-query fetchAll="true" model="public.currency_list" rootPath="pur7130_currency_source"/>
