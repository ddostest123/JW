            //判断是否开启密封报价以及密封日期
            
            function pur5220_checkSealedFlag_fun() {
                var sealed_bid_flag = $('pur52202_rfx_headers_ds').getAt(0).get('sealed_bid_flag');
                var sealing_cutoff_date = $('pur52202_rfx_headers_ds').getAt(0).get('sealing_cutoff_date');
            
                if (sealed_bid_flag == 'Y') {
                    var sysdate = new Date();
            
                    if (typeof(sealing_cutoff_date) == 'string') {
                        sealing_cutoff_date = new Date(sealing_cutoff_date.replace(/-/g, "/"));
                    }
            
                    if (sealing_cutoff_date >= sysdate) {
                        return false;
                    }
                }
                return true;
            }
            
