                    lines.push(records[i].data);
                    
                }
                data['componentList'] = lines;
                Aurora.request({
                    url: $('PUR8010_vendor_list_save_link').getUrl(),
                    para: data,
                    success: function(result) {
                        $('pur8010_edit_save').enable();
                        $('pur8010_edit_submit').enable();
                        $('component_list_ds').query();
                    },
                    failure: function() {
                        $('pur8010_edit_save').enable();
                        $('pur8010_edit_submit').enable();
                    
                    },
                    error: function() {
                        $('pur8010_edit_save').enable();
                        $('pur8010_edit_submit').enable();
                    },
                    scope: this
                });  
            }
            
