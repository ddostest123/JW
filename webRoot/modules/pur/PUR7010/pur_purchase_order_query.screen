                        //订单关注
         
             
            function pur7010_purPoFocusFunction(pur_header_id) {
                new Aurora.Window({
                    id: 'pur7010_purPoFocus_window',
                   title: '订单关注',
                    url: $('pur7010_purPoFocus_link').getUrl(),
                    height: 150,
                    width: 400
                }).on('close', function() {
            
                    if (Aurora.isEmpty(pur7010_focus_type)) {
                        return;
                    }
            
                    var datas = [];
            
                    var data = {
                        pur_header_id: pur_header_id,
                        focus_type: pur7010_focus_type,
                        '_status': 'insert'
                    };
            
                    datas.push(data);
            
                    Aurora.Masker.mask(Ext.getBody());
                    Aurora.request({
                        url: $('pur7010_purPoFocusSave_link').getUrl(),
                        para: datas,
                        success: function(res) {
                            Aurora.Masker.unmask(Ext.getBody());
                            $('pur7010_purchase_order_header_result_ds').query();
                        },
                        failure: function() {
                            Aurora.Masker.unmask(Ext.getBody());
                        },
                        error: function() {
                            Aurora.Masker.unmask(Ext.getBody());
                        },
                        scope: this
                    });
                });
            }
            
            //取消关注
            
            
            function pur7010_purPoFocusCancelFunction(po_focus_id, focus_type) {
                var message = '';
                if (focus_type == 'ONCE') {
                    message = '当前关注模式为【仅第一次发货时通知】，是否确认取消关注？';
                } else if (focus_type == 'ALWAYS') {
                    message = '当前关注模式为【每次发货时都通知】，是否确认取消关注？';
                }
            
                Aurora.showConfirm("${l:PROMPT}", message, function() {
                    var datas = [];
                    var data = {
                        po_focus_id: po_focus_id,
                        '_status': 'delete'
                    };
                    datas.push(data);
                    Aurora.Masker.mask(Ext.getBody());
                    Aurora.request({
                        url: $('pur7010_purPoFocusSave_link').getUrl(),
                        para: datas,
                        success: function(res) {
                            Aurora.Masker.unmask(Ext.getBody());
                            $('pur7010_purchase_order_header_result_ds').query();
                        },
                        failure: function() {
                            Aurora.Masker.unmask(Ext.getBody());
                        },
                        error: function() {
                            Aurora.Masker.unmask(Ext.getBody());
                        },
                        scope: this
                    });
                }, function() {
                    return;
                });
            
            
