            //采购订单明细
            
            function pur7010_purchaseOrderHeaderRendererFun(value, record, name) {
                if (name == 'display_po_number') {
                    if (record.isNew) {
                        return '';
                    }
                    //addby liumeiqi 16049
                    if (record.get('po_11_item_value') == 'Y' && record.get('delivery_more_need_flag') == 'Y') {
                        return '<div style="background:#FF0000;"><a style="color:#FFFFFF;" href="javascript:pur7010_purchaseOrderHeaderRendereOpen(' + record.get('pur_header_id') + ')">' + value + '</a><div>';
                    }
                    return '<a href="javascript:pur7010_purchaseOrderHeaderRendereOpen(' + record.get('pur_header_id') + ')">' + value + '</a>';
                } else if (name == 'status_desc') {
                    if ('URGENT' == record.get('urgent_status')) {
                        return '<div class="status-text status-red">' + value + '</div>';
                    } else {
                        return value;
                    }
                } else if (name == 'po_focus_id') {
                    if (Aurora.isEmpty(value)) {
                        return '<a href="javascript:pur7010_purPoFocusFunction(' + record.get('pur_header_id') + ')">' + '<img class="po-focus-icon" alt="Y" src="${/request/@context_path}/images/White_Grey_heart.png"/>' + '</a>';
                    } else {
                        var focus_type_desc = '';
                        if (record.get('focus_type') == 'ONCE') {
                            focus_type_desc = '仅第一次发货时通知';
                        } else if (record.get('focus_type') == 'ALWAYS') {
                            focus_type_desc = '每次发货时都通知';
                        }
                        return '<a title="' + focus_type_desc + '" href="javascript:pur7010_purPoFocusCancelFunction(' + record.get('po_focus_id') + ',\'' + record.get('focus_type') + '\')">' + '<img class="po-focus-icon" alt="N" src="${/request/@context_path}/images/Red_Pink_heart.png"/>' + '</a>';
                    }
                }
            }
            
         
            
        
            
