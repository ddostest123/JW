            //勾选数据导出
            
            function pur7010_purchaseOrderHeaderExportSelectFunction() {
                var records = $('pur7010_purchase_order_header_result_ds').getSelected();
                if (records.length == 0) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}');
