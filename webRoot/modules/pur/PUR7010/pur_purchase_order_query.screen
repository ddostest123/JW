            function pur7010_purchaseOrderQueryMoneyRenderFun(value, record, name) {
                if (value == '***') {
                    return value;
                } else {
                    
                    //暂估标识
                    var provisional_estimate_flag = record.get('provisional_estimate_flag');
                    
                    if(name == 'unit_price'){
                        if(provisional_estimate_flag == 'Y'){
		                    return '<div style="color:#FF0000;font-weight:bold;">' + Aurora.formatNumber(value) + '</div>';
                        }else{
		                    return Aurora.formatNumber(value);
                        }
                    }else{
	                    return Aurora.formatNumber(value);
                    }
                }
            }
            
            function pur7010_treeGrid_renderer(value, record, name) {
                var index = $('pur7010_query_parameter_tree_ds').indexOf(record);
                var id = "pur7010_" + index;
                pur7010_tree_div.push(id);
                return '<div id="' + id + '" >' + value + '</div>';
            }
            
            //查找tree的父节点
            
            function pur7010_find_father(code) {
                var tree_ds = $('pur7010_query_parameter_tree_ds');
                var all = tree_ds.getAll();
                for (var i = 0;i < all.length;i++) {
                    if (all[i].data.query_code == code) {
                        return all[i].data.parent_query_code;
                    }
                }
            }
            
            //是否有子节点
            
            function pur7010_find_son(code) {
                var tree_ds = $('pur7010_query_parameter_tree_ds');
                var all = tree_ds.getAll();
                for (var i = 0;i < all.length;i++) {
                    if (all[i].data.parent_query_code == code) {
                        return false;
                    }
                }
                return true;
            }
            
            function pur7010_query_set_null() {
                var query_ds;
                if (pur7010_tab_index == 0) {
                    query_ds = $('pur7010_purchase_order_header_query_ds').getAt(0);
                } else if (pur7010_tab_index == 1) {
                    query_ds = $('pur7010_purchase_order_query_ds').getAt(0);
                }
                query_ds.set('urgent_status', '');
                query_ds.set('srm_status', '');
                query_ds.set('cancel_flag', '');
                query_ds.set('closed_flag', '');
                query_ds.set('closed_code', '');
                query_ds.set('already_shipped_flag', '');
                query_ds.set('ready_to_shipped_flag', '');
                query_ds.set('new_order', '');
                query_ds.set('updated_order', '');
            }
            
            function pur7010_tree_click(grid, row, record) {
                var query_ds;
                var i;
                var id;
                if (pur7010_tab_index == 0) {
                    query_ds = $('pur7010_purchase_order_header_query_ds').getAt(0);
                    pur7010_click_index_h = $('pur7010_query_parameter_tree_ds').indexOf(record);
                    id = "pur7010_" + pur7010_click_index_h;
                } else if (pur7010_tab_index == 1) {
                    query_ds = $('pur7010_purchase_order_query_ds').getAt(0);
                    pur7010_click_index_l = $('pur7010_query_parameter_tree_ds').indexOf(record);
                    id = "pur7010_" + pur7010_click_index_l;
                }
                //设置被选中div背景色为红色
                document.getElementById(id).style.color = "#FF0000";
                //设置没被选中div背景色为红色
                for (i = 0;i < pur7010_tree_div.length;i++) {
                    if (pur7010_tree_div[i] != id) {
                        document.getElementById(pur7010_tree_div[i]).style.color = "#000000";
                    }
                }
                
                
                pur7010_query_set_null();
                if (pur7010_find_son(record.get('query_code')) || record.get('query_code') == 'ALL' || record.get('query_code') == 'RELEASED') {
                    if (record.get('query_code') == 'ALL') {}
            
                    if (record.get('query_code') == 'URGENT') {
                        query_ds.set('urgent_status', 'URGENT');
                    }
                    if (pur7010_find_father(record.get('query_code')) == 'EXECUTING') {
                        if (record.get('query_code') == 'SEND_OUT') {
                            query_ds.set('already_shipped_flag', 'Y');
                        } else if (record.get('query_code') == 'READY_TO_SEND') {
                            query_ds.set('ready_to_shipped_flag', 'Y');
                        } else {
                            query_ds.set('srm_status', record.get('query_code'));
                        }
                    }
                    if (record.get('query_code') == 'FINISHED') {
                        query_ds.set('closed_code', 'FINALLY CLOSED');
                    }
                    if (record.get('query_code') == 'CANCLED') {
                        query_ds.set('cancel_flag', 'Y');
                    }
                    if (record.get('query_code') == 'CLOSED') {
                        query_ds.set('closed_flag', '1');
                    }
                    
                    //Add by Hunter.Wang at 2017-2-20 T811
                    if(pur7010_find_father(record.get('query_code')) == 'RELEASED'){
	                    if (record.get('query_code') == 'NEW_ORDER') {
	                        query_ds.set('new_order', '1');
	                    }else if (record.get('query_code') == 'UPDATE_ORDER') {
	                        query_ds.set('updated_order', '1');
	                    }
                    }
            
                    if (pur7010_tab_index == 0) {
                        $('pur7010_purchase_order_header_result_ds').query();
                    } else if (pur7010_tab_index == 1) {
                        $('pur7010_purchase_order_result_ds').query();
                    }
                    
                }
            }
            
            function pur7010_tabpanel_select(tabPanel, index, tab) {
                var click_index;
                if (index == 0) {
                    click_index = pur7010_click_index_h;
                } else if (index == 1) {
                    click_index = pur7010_click_index_l;
                }
                $('pur7010_query_parameter_tree_ds').locate(click_index + 1);
                //将所有非点击的div字体设置为黑色
                for (var i = 0;i < pur7010_tree_div.length;i++) {
                    if (pur7010_tree_div[i] != 'pur7010_' + click_index) {
                        document.getElementById(pur7010_tree_div[i]).style.color = "#000000";
                    }
                }
                //设置点击的div字体为红色
                if (click_index != '-1') {
                    document.getElementById('pur7010_' + click_index).style.color = "#FF0000";
                    //换页时锁定光标位置
                    $('pur7010_query_parameter_tree_ds').locate(click_index + 1);
                } else {
                    $('pur7010_query_parameter_tree_ds').locate(1);
                }
                pur7010_tab_index = index;
            }
            
            //Update Function
            
            function pur7010_onPurchaseOarderQueryDs_updateFun(ds, record, name, value, oldvalue) {
                if (name == 'vendor_id') {
                    if (Ext.isEmpty(value)) {
                        record.getField('vendor_site_code').setReadOnly(true);
                        record.getField('vendor_site_code').setLovPara('vendor_id', -1);
                        record.set('vendor_site_id', '');
                        record.set('vendor_site_code', '');
                    } else {
                        record.getField('vendor_site_code').setReadOnly(false);
                        record.getField('vendor_site_code').setLovPara('vendor_id', value);
                        record.set('vendor_site_id', '');
                        record.set('vendor_site_code', '');
                    }
                }
            }
            
