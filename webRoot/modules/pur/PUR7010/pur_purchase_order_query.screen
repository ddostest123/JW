            function pur7010_purchaseOrderDetailExportSelectFunction() {
                var records = $('pur7010_purchase_order_result_ds').getSelected();
                if (records.length == 0) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}');
