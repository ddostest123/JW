            <a:dataSet id="pur2200_req_edit_line_ds" autoQuery="true" bindName="pur2200_req_create_line_result_ds" bindTarget="pur2200_req_edit_header_ds" fetchAll="true" model="pur.PUR2200.pur_req_lines" queryUrl="${/request/@context_path}/autocrud/pur.PUR2200.pur_req_lines/query?req_header_id=${/parameter/@req_header_id}" selectable="true">
