<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure/>
    <a:view>
        <a:link id="pur2200_edit_requistion_link" url="${/request/@context_path}/modules/pur/PUR2200/pur_requisition_edit.screen"/>
        <a:link id="pur2200_edit_link" url="${/request/@context_path}/modules/pur/PUR2200/pur_req_create.svc"/>
        <a:link id="pur2200_edit_line_contract_delete_link" url="${/request/@context_path}/modules/pur/PUR2200/pur_req_contract_delete.svc"/>
        <a:link id="pur2200_edit_line_attachment_delete_link" url="${/request/@context_path}/modules/pur/PUR2200/pur_req_attachment_delete.svc"/>
        <a:link id="pur2200_edit_line_delete_link" url="${/request/@context_path}/modules/pur/PUR2200/pur_req_line_delete.svc"/>
        <a:link id="pur2200_edit_line_submit_link" url="${/request/@context_path}/modules/pur/PUR2200/pur_req_detail_submit.svc"/>
        <a:link id="pur2200_edit_line_cancel_link" url="${/request/@context_path}/modules/pur/PUR2200/pur_req_detail_cancel.svc"/>
        <script><![CDATA[
            var retrun_flag = 'N';
            var submit_flag = 'N';
            var cancel_flag = 'N';
            
            function pur2200_line_operation_record() {
                new Aurora.Window({
                    id: 'pur_req_operation_record_win',
                    url: $('pur2200_req_operation_record_link').getUrl() + '?operation_table_id=' + ${/parameter/@req_header_id} + '&operation_table=PUR_REQ_HEADERS',
                    title: '${l:ACP_BILL.OPERATION_RECORD}',
                    height: 450,
                    width: 600
                });
            }
            
            function pur2200_edit_return() {
                $('pur2200_header_edit_window').close();
            }
            
            function pur2200_header_update(ds, record, name, value, oldValue) {
                if (name == 'company_id') {
            
                    record.set('req_type_id', null);
                    record.set('req_type_name', null);
                    record.set('unit_id', null);
                    record.set('unit_name', null);
            
            
                    // 重置行上数据
                    var line_ds = $('pur2200_req_edit_line_ds');
                    var line_records = line_ds.getAll();
                    for (var i = 0;i < line_records.length;i++) {
                        // 物料品类
                        line_records[i].set('item_category_id', null);
                        line_records[i].set('item_category_code', null);
                        line_records[i].set('item_category_desc', null);
            
                        // 收货组织
                        line_records[i].set('ship_to_organization_id', null);
                        line_records[i].set('ship_to_organization_name', null);
            
                    }
            
            
                    var req_type_name_ds = record.getMeta().getField('req_type_name');
                    var unit_name_ds = record.getMeta().getField('unit_name');
            
                    if (!Aurora.isEmpty(value)) {
            
                        req_type_name_ds.setReadOnly(false);
                        unit_name_ds.setReadOnly(false);
            
                        $('pur2200_req_edit_type_ds').setQueryParameter('company_id', value);
                        $('pur2200_req_edit_type_ds').query();
            
                        var unit_url = 'public.exp_org_company_unit?company_id=' + value;
            
            
                        var lovField = record.getMeta().getField("unit_name");
                        lovField.setLovService(unit_url);
                    } else {
                        req_type_name_ds.setReadOnly(true);
                        unit_name_ds.setReadOnly(true);
                    }
                }
            
                if (name == 'tax_flag') {
                    record.set('tax_name', null);
                    record.set('tax_id', null);
            
                    var tax_name_ds = record.getMeta().getField('tax_name');
                    if (value == 'Y') {
                        tax_name_ds.setReadOnly(false);
                        tax_name_ds.setRequired(true);
            
                    } else {
                        tax_name_ds.setReadOnly(true);
                        tax_name_ds.setRequired(false);
                    }
                }
            
            
            }
            
            
            function pur2200_req_edit_save() {
            
                var headers_ds = $('pur2200_req_edit_header_ds');
                var lines_ds = $('pur2200_req_edit_line_ds');
                var contact_ds = $('pur2200_req_edit_line_contract_ds');
                var attachment_ds = $('pur2200_req_edit_attachment_ds');
            
                if (headers_ds.validate() && lines_ds.validate() && attachment_ds.validate()) {
                    headers_ds.setSubmitUrl($('pur2200_edit_link').getUrl());
                    headers_ds.submit();
            
            
                    var header_record = headers_ds.getCurrentRecord();
                    var datas;
                    datas = {
                        req_header_id: ${/parameter/@req_header_id}
                    };
            
            
                    if (submit_flag == 'Y') {
            
                        Aurora.request({
                            url: $('pur2200_edit_line_submit_link').getUrl(),
                            para: datas,
                            success: function(res) {
                                pur2200_edit_return();
                                pur2200_result_query();
                            },
                            failure: function(res) {
                                // Aurora.Masker.unmask($('acp5030_lines_window').wrap);
                                return;
            
                            },
                            error: function(res) {
                                return;
                                // Aurora.Masker.unmask($('acp5030_lines_window').wrap);
                            },
                            scope: this
                        });
            
            
                    }
                    if (cancel_flag == 'Y') {
            
            
                        Aurora.request({
                            url: $('pur2200_edit_line_cancel_link').getUrl(),
                            para: datas,
                            success: function(res) {
                                pur2200_edit_return();
                                pur2200_result_query();
                            },
                            failure: function(res) {
                                // Aurora.Masker.unmask($('acp5030_lines_window').wrap);
                                return;
            
                            },
                            error: function(res) {
                                return;
                                // Aurora.Masker.unmask($('acp5030_lines_window').wrap);
                            },
                            scope: this
                        });
            
            
                    }
                    attachment_ds.query();
            
            
            
                }
            
            
            }
            // 提交动作
            
            function pur2200_req_edit_submit() {
                submit_flag = 'Y';
                return_flag = 'Y';
                pur2200_req_edit_save();
            }
            // 取消动作
            
            function pur2200_req_edit_cancel() {
            
                cancel_flag = 'Y';
                return_flag = 'Y';
                pur2200_req_edit_save();
            
            }
            
            function pur2200_hd_submitsuccess(ds, res) {
                Aurora.Masker.unmask($('pur2200_edit_window').wrap);
                var pur_requisition_header_id = res.result.pur_requisition_header_id;
                new Aurora.Window({
                    url: $('pur2200_edit_requistion_link').getUrl() + '?pur_requisition_header_id=' + pur_requisition_header_id,
                    title: '${l:PUR_REQ_HEADERS.PUR_REQ_MAINTAIN}',
                    id: 'pur2200_edit_window',
                    fullScreen: true
                }).on('beforeclose', pur2200_result_query);
                pur2200_edit_return();
            }
            
            function pur2200_hd_submitfailed(ds, res) {
                Aurora.Masker.unmask($('pur2200_edit_window').wrap);
            }
            
            // 采购品类
            
            function pur2220_edit_cellClick(grid, row, name, record) {
                var headers_ds = $('pur2200_req_edit_header_ds');
                var header_record = headers_ds.getCurrentRecord();
            
            
                if (name == 'item_category_desc') {
                    if (Ext.isEmpty(header_record.get('company_id'))) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PUR_REQ_HEADERS.PLEASE_SELECT_COMPANY}!');
                        return;
                    } else {
                        url = 'pur.PUR2200.pur_purchase_category_lov?company_id=' + header_record.get('company_id');
                    }
            
                    var lovField = record.getMeta().getField("item_category_desc");
                    lovField.setLovService(url);
                    return 'item_category_desc_lov';
                }
            
                if (name == 'item_code') {
                    if (Ext.isEmpty(record.get('item_category_id'))) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PUR_REQ_HEADERS.PLEASE_SELECT_PURCHASE_CATEGORY}!');
                        return;
                    } else {
                        url = 'pur.PUR2200.inv_system_item_lov?purchase_category_id=' + record.get('item_category_id');
                    }
                    var item_lovField = record.getMeta().getField("item_code");
                    item_lovField.setLovService(url);
                    return 'item_code_lov';
                }
            
                if (name == 'ship_to_organization_name') {
                    if (Ext.isEmpty(header_record.get('company_id'))) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PUR_REQ_HEADERS.PLEASE_SELECT_COMPANY}!');
                        return;
                    } else {
                        url = 'public.inv_organizations_lov?company_id=' + header_record.get('company_id');
                    }
                    var organization_lovField = record.getMeta().getField("ship_to_organization_name");
                    organization_lovField.setLovService(url);
                    return 'ship_to_organization_name_lov';
                }
            
            }
            
            
            function pur2220_edit_contact_cellClick(grid, row, name, record) {
                if (name == 'vendor_site_name') {
                    if (Ext.isEmpty(record.get('vendor_id'))) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PUR_REQ_HEADERS.PLEASE_SELECT_VENDOR}!');
                        return;
                    } else {
                        url = 'public.po_vendor_sites_all_lov?vendor_id=' + record.get('vendor_id');
                    }
                    var vendor_site_name_lovField = record.getMeta().getField("vendor_site_name");
                    vendor_site_name_lovField.setLovService(url);
                    return 'vendor_site_name_lov';
                }
                if (name == 'contact_persion') {
                    if (Ext.isEmpty(record.get('vendor_id'))) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PUR_REQ_HEADERS.PLEASE_SELECT_VENDOR}!');
                        return;
                    } else {
                        url = 'public.pur_sys_vendor_cantacts_lov?vendor_id=' + record.get('vendor_id');
                    }
                    var contact_persion_lovField = record.getMeta().getField("contact_persion");
                    contact_persion_lovField.setLovService(url);
                    return 'contact_persion_lov';
                }
            }
            // 行更新动作
            
            function pur2200_grid_update(dataSet, record, name, value, oldvalue) {
            
                if (name == 'tax_flag') {
            
                    var tax_ds = record.getMeta().getField('tax');
            
                    if (value == 'Y') {
            
                        tax_ds.setReadOnly(false);
                        tax_ds.setRequired(true);
            
                    } else {
                        tax_ds.setReadOnly(true);
                        tax_ds.setRequired(false);
                        record.set('tax_id', null);
                        record.set('tax', null);
            
                    }
                }
            
            
            
                //物料品类改变
                if (name == 'item_category_desc') {
                    if (Ext.isEmpty(value)) {
                        record.set('item_id', null);
                        record.set('item_code', null);
                        record.set('item_desc', null);
                    }
                }
            
            
                // 税率改变
                if (name == 'tax_id') {
                    if (!Ext.isEmpty(value)) {
                        if (!Ext.isEmpty(record.get('tax_type_rate'))) {
            
                            if (!Ext.isEmpty(record.get('quantity')) && !Ext.isEmpty(record.get('budget_price'))) {
                                var line_amount = record.get('quantity') * record.get('budget_price') * 1;
                                record.set('tax_amount', Math.round((line_amount - line_amount / (1 + record.get('tax_type_rate') / 100) * 1) * 100) / 100);
                            } else {
                                record.set('tax_amount', 0);
                            }
                        }
                    } else {
                        record.set('tax_amount', 0);
                    }
                }
            
            
            
                // 数量改变
                if (name == 'quantity') {
                    if (!Ext.isEmpty(value) && !Ext.isEmpty(record.get('budget_price'))) {
                        var line_amount = value * record.get('budget_price') * 1;
                        record.set('line_amount', line_amount);
            
                        // 税率判断
                        if (!Ext.isEmpty(record.get('tax_type_rate'))) {
                            var tax_amount = line_amount - line_amount / (1 + record.get('tax_type_rate') / 100) * 1;
                            record.set('tax_amount', Math.round(tax_amount * 100) / 100);
                        } else {
                            record.set('tax_amount', 0);
                        }
                        // 汇率判断
                        if (!Ext.isEmpty(record.get('exchange_rate'))) {
                            record.set('functional_amount', line_amount * record.get('exchange_rate') * 1);
                        } else {
            
                            record.set('functional_amount', 0);
                        }
            
                    }
            
                }
                // 价格改变
                if (name == 'budget_price') {
                    if (!Ext.isEmpty(value) && !Ext.isEmpty(record.get('quantity'))) {
                        var line_amount = value * record.get('quantity') * 1;
                        record.set('line_amount', line_amount);
                        // 税率判断
                        if (!Ext.isEmpty(record.get('tax_type_rate'))) {
                            var tax_amount = line_amount - line_amount / (1 + record.get('tax_type_rate') / 100) * 1;
                            record.set('tax_amount', Math.round(tax_amount * 100) / 100);
                        } else {
                            record.set('tax_amount', 0);
                        }
                        // 汇率判断
                        if (!Ext.isEmpty(record.get('exchange_rate'))) {
                            record.set('functional_amount', line_amount * record.get('exchange_rate') * 1);
                        } else {
            
                            record.set('functional_amount', 0);
                        }
            
                    }
            
                }
                // 汇率改变
                if (name == 'exchange_rate') {
                    if (!Ext.isEmpty(value) && !Ext.isEmpty(record.get('budget_price')) && !Ext.isEmpty(record.get('quantity'))) {
                        record.set('functional_amount', value * record.get('budget_price') * record.get('quantity') * 1);
                    } else {
            
                        record.set('functional_amount', 0);
                    }
            
                }
            
            
            }
            
            
            // 添加附件行
            
            function pur2220_edit_attachment_add(dataSet, record, index) {
                // 生成行号
                records = dataSet.getAll();
                var attachment_num = 0;
                if (records.length > 1) {
                    for (var t = 0;t < records.length;t++) {
                        if (attachment_num < records[t].get('attachment_num')) {
                            attachment_num = records[t].get('attachment_num');
                        }
                    }
                }
                record.set('attachment_num', attachment_num * 1 + 1);
            }
            // 合同删除
            
            function pur2220_edit_contract_delete(dataSet) {
            
                dataSet.setSubmitUrl($('pur2200_edit_line_contract_delete_link').getUrl());
                dataSet.submit();
            }
            // 附件删除
            
            function pur2220_edit_attachment_delete(dataSet) {
            
                dataSet.setSubmitUrl($('pur2200_edit_line_attachment_delete_link').getUrl());
                dataSet.submit();
            }
            
            
            
            // 行删除
            
            function pur2200_edit_line_delete(dataSet) {
                dataSet.setSubmitUrl($('pur2200_edit_line_delete_link').getUrl());
                dataSet.submit();
            
            }
            
            
            // 添加行动作
            
            function pur2220_edit_add(dataSet, record, index) {
                var headers_ds = $('pur2200_req_edit_header_ds');
                var header_record = headers_ds.getCurrentRecord();
                // 生成行号
                records = dataSet.getAll();
                var line_num = 0;
                if (records.length > 1) {
                    for (var t = 0;t < records.length;t++) {
            
            
            
                        if (line_num < records[t].get('line_num')) {
                            line_num = records[t].get('line_num');
                        }
                    }
                }
                record.set('line_num', line_num * 1 + 1);
                record.set('req_header_id', header_record.get('req_header_id'));
            
                // 默认税率
                var tax_ds = record.getMeta().getField('tax');
            
                if (!Ext.isEmpty(header_record.get('tax_id'))) {
            
                    tax_ds.setReadOnly(false);
                    record.set('tax_flag', header_record.get('tax_flag'));
                    record.set('tax_id', header_record.get('tax_id'));
                    record.set('tax', header_record.get('tax_name'));
                    record.set('tax_type_rate', header_record.get('tax_type_rate'));
            
                } else {
                    tax_ds.setReadOnly(true);
            
                }
            
            
            
                // 默认币种
                if (!Ext.isEmpty(header_record.get('currency_code'))) {
                    record.set('currency_code', header_record.get('currency_code'));
                    record.set('currency_name', header_record.get('currency_name'));
                }
            
                // 默认汇率
                if (!Ext.isEmpty(header_record.get('exchange_rate'))) {
                    record.set('exchange_rate', header_record.get('exchange_rate'));
                }
            
            }
            
            function pur2200_header_load() {
                var headers_ds = $('pur2200_req_edit_header_ds');
                var header_record = headers_ds.getCurrentRecord();
                if (header_record.get('tax_flag') == 'Y') {
                    var tax_ds = header_record.getMeta().getField('tax_name');
                    tax_ds.setReadOnly(false);
                    tax_ds.setRequired(true);
            
                }
            }
            
            function pur2200_line_load() {
                var line_ds = $('pur2200_req_edit_line_ds');
                var line_records = line_ds.getAll();
                for (var j = 0;j < line_records.length;j++) {
                    if (line_records[j].get('tax_flag') == 'Y') {
                        var tax_ds = line_records[j].getMeta().getField('tax');
                        tax_ds.setReadOnly(false);
                        tax_ds.setRequired(true);
            
                    }
                }
            
            
            }
            
            
            
            function pur2200_edit_Upload(value, record, name) {
                var req_attachment_id = record.get('req_attachment_id');
                if (record.get('req_attachment_id')) {
                    return '<a href="javascript:pur2200_edit_uploadFile(' + req_attachment_id + ')">' + '附件上传' + '</a>';
                }
            }
            
            
            function pur2200_edit_uploadFile(req_attachment_id) {
                if (!Ext.isEmpty(req_attachment_id)) {
                    var url = "${/request/@context_path}/uploadFile.screen?table_name=PUR_REQ_ATTACHMENT&header_id=" + req_attachment_id;
                    new Aurora.Window({
                        url: url,
                        title: '${l:ATM.UPLOAD_ATTACHMENT}',
                        id: 'pur2200_req_edit_file_window',
                        width: 600,
                        height: 300
                    });
                }
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="pur2200_req_edit_type_ds" autoQuery="true" loadData="true" model="pur.public.pur_req_type"/>
            <a:dataSet id="pur2200_req_edit_currency_ds" autoQuery="true" loadData="true" model="public.currency_list"/>
            <a:dataSet id="pur2200_req_edit_tax_ds" autoQuery="true" loadData="true" model="public.fnd_tax_type_code_lov"/>
            <a:dataSet id="pur2200_req_edit_line_type_ds" autoQuery="true" loadData="true" model="pur.public.pur_line_type"/>
            <a:dataSet id="pur2200_req_edit_pur_method_ds" loadData="true" lookupCode="PUR_METHOD"/>
            <a:dataSet id="pur2200_req_edit_pur_attachment_ds" loadData="true" lookupCode="PUR_ATTACHMENT_TYPE"/>
            <a:dataSet id="pur2200_req_edit_header_ds" autoCreate="true" autoQuery="true" model="pur.PUR2200.pur_req_headers" queryUrl="${/request/@context_path}/autocrud/pur.PUR2200.pur_req_headers/query?req_header_id=${/parameter/@req_header_id}">
                <a:fields>
                    <a:field name="req_header_id"/>
