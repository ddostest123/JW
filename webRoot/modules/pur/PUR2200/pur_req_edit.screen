                        <a:mapping>
                            <a:map from="company_id" to="company_id"/>
                            <a:map from="company_short_name" to="company_name"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="req_type_name" displayField="description" options="pur2200_req_edit_type_ds" required="true" returnField="req_type_id" valueField="requisition_type_id"/>
                    <a:field name="currency_name" displayField="currency_name" options="pur2200_req_edit_currency_ds" required="true" returnField="currency_code" valueField="currency_code"/>
                    <a:field name="tax_name" displayField="description" options="pur2200_req_edit_tax_ds" readOnly="true" returnField="tax_id" valueField="tax_type_id">
