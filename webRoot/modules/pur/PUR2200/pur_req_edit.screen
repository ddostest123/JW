                        <a:mapping>
                            <a:map from="description" to="item_desc"/>
                            <a:map from="item_code" to="item_code"/>
                            <a:map from="item_id" to="item_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="pro_mode_name" displayField="code_value_name" options="pur2200_req_edit_pur_method_ds" required="true" returnField="pro_mode_code" valueField="code_value"/>
                    <a:field name="approve_number"/>
                    <a:field name="quantity" required="true"/>
                    <a:field name="unit_meas_lookup_code"/>
                    <a:field name="need_by_date" required="true"/>
                    <a:field name="ship_to_organization_name" lovGridHeight="350" lovHeight="500" lovWidth="500" required="true" title="PUR_REQ_LINES.SHIP_TO_ORGANIZATION_ID">
                        <a:mapping>
                            <a:map from="organization_name" to="ship_to_organization_name"/>
                            <a:map from="organization_id" to="ship_to_organization_id"/>
                        </a:mapping>
                    </a:field>
