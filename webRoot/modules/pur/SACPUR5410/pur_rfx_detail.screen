            function bid6010Submit() {
                Aurora.showConfirm('${l:PROMPT}', '${l:PROMPT.CONFIRM_SUBMIT}？', function() {
            
                    var datas = $('pur51303_rfx_headers_ds').getCurrentRecord().data;
                    var score_headers = [];
                    var docm_header_records = $('bid6010_bidding_docm_header_ds').getAll();
                    for (var k = 0;k < docm_header_records.length;k++) {
                        score_headers.push(docm_header_records[k].data);
                    }
            
                    datas['score_headers'] = score_headers;
                    Aurora.Masker.mask($('sacpur5410_rfxNumberDetail_window').wrap, '${l:LOADING}');
                    Aurora.request({
                        url: $('bid6010_submit_bidding_score_link').getUrl(),
                        para: datas,
                        success: function() {
                            Aurora.Masker.unmask($('sacpur5410_rfxNumberDetail_window').wrap);
                            Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SUBMITED}', function() {
                                sacpur5410_rfx_header_result_query();
                                $('sacpur5410_rfxNumberDetail_window').close();
                            });
                        },
                        failure: function() {
                            Aurora.Masker.unmask($('sacpur5410_rfxNumberDetail_window').wrap);
                            return;
                        },
                        error: function() {
                            Aurora.Masker.unmask($('sacpur5410_rfxNumberDetail_window').wrap);
                            return;
                        },
                        scope: this
                    });
            
            
                });
            }
            
            function formatNumber2(value, decimalprecision) {
                if (Ext.isEmpty(value)) return '';
            
                value = String(value).replace(/,/g, '');
                if (isNaN(value)) return '';
                if (decimalprecision || decimalprecision === 0)
            
            
                var point_value = value.split('.');
                var in_length = (point_value.length == 2) ? '.' + point_value[1] : '';
            
            
                if (in_length.length - 1 < decimalprecision) {
            
                    decimalprecision = in_length.length - 1
                };
                if (in_length.length == 0) {
                    decimalprecision = 0;
                }
            
                value = Number(value).toFixed(decimalprecision);
            
                var ps = value.split('.');
                var sub = (ps.length == 2) ? '.' + ps[1] : '';
            
            
                var whole = ps[0];
                var r = /(\d+)(\d{3})/;
                while (r.test(whole)) {
                    whole = whole.replace(r, '$1' + ',' + '$2');
                }
            
                Math.round()
                v = whole + sub;
                return v;
            }
            
            function pur5410_formatNumber_render(value, record, name) {
                var business_flag = record.get('business_flag');
                if(business_flag == 'Y'){
	                return formatNumber2(value, 8);
                }else{
                    return '***';
                }
            }
            
