                                } else {
                                    return '<a href="javascript:bid6010BiddingScore(' + score_header_id + "," + '${/parameter/@rfx_header_id}' + "," + record.get('bid_header_id') + ',\'' + team + '\',' + sequence_num + ');">${l:PROMPT.EVALUATE}</a>';
                                }
                            }
