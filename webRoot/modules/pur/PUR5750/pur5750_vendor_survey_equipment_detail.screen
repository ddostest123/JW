            function pur5750_survey_equipment_vendor_view() {
                var coop_company_id = $('pur5750_survey_equipment_headers_ds').getAt(0).get('supplier_company_id');
                new Aurora.Window({
                    url: $('pur5750_vendor_survey_equipment_vendor_detail_link').getUrl() + '?coop_company_id=' + coop_company_id,
                    id: 'pur5660_com_supplier_window',
                    fullScreen: true
                });
            }
            //查看采购方附件
            
            function pur5750_survey_atm_upload_rendererFun_buyer(value, record, name) {
                if (name == 'attachment_id') {
                    if ('${/parameter/@release_flag}' != 'Y') {
                        var attachment_id = record.get('attachment_id');
                        if (record.get('atm_count_buyer') == 0) {
                            return '<a style="color:blue" href="javascript:pur5750_vendor_survey_uploadFun_buyer(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                        } else {
                            return '<a style="color:blue" href="javascript:pur5750_vendor_survey_uploadFun_buyer(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>' + '<img id="pur5730_vendor_survey_manufacturer_uploadBtu_img_1" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                        }
                    } else {
                        return '';
                    }
                } else if (name == 'last_upload_date') {
                    if ('${/parameter/@release_flag}' != 'Y') {
                        return value;
                    } else {
                        return '';
                    }
                }
            }
            // 查看供应商附件
            
            function pur5750_survey_atm_upload_rendererFun(value, record, name) {
                if (name == 'attachment_id') {
                    if ('${/parameter/@release_flag}' != 'Y') {
                        var attachment_id = record.get('attachment_id');
                        if (record.get('atm_count_vendor') == 0) {
                            return '<a style="color:blue" href="javascript:pur5750_vendor_survey_uploadFun(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                        } else {
                            return '<a style="color:blue" href="javascript:pur5750_vendor_survey_uploadFun(' + attachment_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>' + '<img id="pur5730_vendor_survey_manufacturer_uploadBtu_img_2" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                        }
                    } else {
                        return '';
                    }
                } else if (name == 'last_upload_date') {
                    if ('${/parameter/@release_flag}' != 'Y') {
                        return value;
                    } else {
                        return '';
                    }
                }
            }
            
