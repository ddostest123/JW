        <script><![CDATA[
            function pur5750_detail_equipment_returnFun() {
                $('pur5750_vendor_survey_detail_win').close();
            }
            
            function pur5750_vendor_supply_record() {
                new Aurora.Window({
                    id: 'pur7210_vendor_survey_record_win',
                    title: '${l:PROMPT.OPERATION_RECORD}',
                    url: $('pur5750_vendor_survey_equipment_record_link').getUrl() + '?survey_header_id=${/parameter/@survey_header_id}',
                    width: 700,
                    height: 410
                });
            }
            
            function pur5750_vendor_survey_equipment_rendererFun(value, record, name) {
                var survey_header_id = record.get('survey_header_id');
                if (name == 'sale_situation') {
                    return '<a style="color:blue" href="javascript:pur5750_vendor_survey_equipment_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
                if (name == 'organization') {
                    return '<a style="color:blue" href="javascript:pur5750_vendor_survey_manufacturer_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
            
                if (name == 'stock_right') {
                    return '<a style="color:blue" href="javascript:pur5750_vendor_survey_manufacturer_stock_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
                if (name == 'employment_qualifications') {
                    return '<a style="color:blue" href="javascript:employment_qualifications_uploadFun(' + survey_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                }
            }
            
            function employment_qualifications_uploadFun(id) {
                new Aurora.Window({
                    id: 'employment_qualifications_upload_link_window',
                    title: '${l:VENDOR_SURVEY_TAB.EMPLOYMENT_QUALIFICATIONS}',
                    url: $('pur5750_attachment_file_upload_link').getUrl() + '?pkvalue=' + id + '&table_name=PUR_VENDOR_SURVEY_BASIC_INFO_QUA',
                    width: 610,
                    height: 500
                });
            }
            
            function pur5750_vendor_survey_manufacturer_uploadFun(id) {
                new Aurora.Window({
                    id: 'pur7210_attachment_file_upload_link_window',
                    title: '${l:PROMPT.VIEW_ATTACHMENT}',
                    url: $('pur5750_attachment_file_upload_link').getUrl() + '?pkvalue=' + id + '&table_name=PUR_VENDOR_SURVEY_BASIC_INFO_ME',
                    width: 610,
                    height: 500
                });
            }
            
            
            function pur5750_vendor_survey_manufacturer_stock_uploadFun(id) {
                new Aurora.Window({
                    id: 'pur7210_attachment_file_upload_link_window',
