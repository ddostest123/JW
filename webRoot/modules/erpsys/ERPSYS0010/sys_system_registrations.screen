            function erpsys_0010_mouse_over(id) {
                var msg = '';
                if (id == 'erpsys_0010_tax_nature_img') {
                    msg = '${l:ERP_SYS.MESSAGE2}';
                    $A.ToolTip.show(Ext.get(id), msg);
                } else if (id == 'erpsys_0010_tax_included_flag_img') {
                    msg = '${l:ERP_SYS.MESSAGE3}';
                    $A.ToolTip.show(Ext.get(id), msg);
                } else if (id == 'erpsys_0010_functional_currency_img') {
                    msg = '${l:ERP_SYS.MESSAGE4}';
                    $A.ToolTip.show(Ext.get(id), msg);
                } else if (id == 'erpsys_0010_system_mode_img') {
                    msg = '${l:ERP_SYS.MESSAGE5}';
                    $A.ToolTip.show(Ext.get(id), msg);
                }
            }
             */
            function erpsys0010_rule_check(record, name, value) {
                var patrn = '';
                var default_password = '';
                if (name == 'company_code') {
                    patrn = /^[A-Z0-9-]{4,20}$/;
                    if (!patrn.exec(value)) {
                        Ext.get('erpsys_0010_company_code_info_div').setStyle({
                            color: "red"
                        });
                        return '${l:ERPFND_COMPANIES.ILLEGAL_ENTRY}';
                    } else {
                        Ext.get('erpsys_0010_company_code_info_div').setStyle({
                            color: "black"
                        });
                        return true;
                    }
                }
            
                if (name == 'default_password') {
                    patrn = /^(?![a-zA-Z]+$)(?!\d+$)[a-zA-Z0-9]{6,16}$/;
                    if (!patrn.exec(value)) {
                        Ext.get('sys_0010_default_password_div').setStyle({
                            color: "red"
                        });
                        return '${l:ERPFND_COMPANIES.ILLEGAL_ENTRY}';
                    } else {
                        Ext.get('sys_0010_default_password_div').setStyle({
                            color: "black"
                        });
                        return true;
                    }
                }
            
                if (name == 'confirm_password') {
                    default_password = record.get("default_password");
            
                    if (default_password != value) {
                        Ext.get("confirm_password_img").setStyle({
                            display: "block"
                        });
                        Ext.get("confirm_password_div").setStyle({
                            display: "block"
                        });
                        return '${l:ERPFND_COMPANIES.ILLEGAL_ENTRY}';
                    } else {
                        Ext.get("confirm_password_img").setStyle({
                            display: "none"
                        });
                        Ext.get("confirm_password_div").setStyle({
                            display: "none"
                        });
                        return true;
                    }
                }
            
                if (name == 'openning_period_year') {
                    if (value.toString().length != 4 || value.toString().substr(0, 1) == '0') {
                        var date = new Date();
                        var year = date.getFullYear();
                        record.set("openning_period_year", year);
                        return true;
                    } else {
                        return true;
                    }
                }
            }
            
            function erpsys0010_system_registrations_upload(title, table_name) {
                erpsys0010_attachment_type = table_name;
                var url = $('erpsys_system_registrations_upload_link').getUrl() + '?table_name=' + table_name + '&header_id=${/model/erpsys0010_sys_system_regist_table_pk_path/record/@header_id}';
                new Aurora.Window({
                    url: url,
                    title: title,
                    id: 'sys_system_registrations_upload_window',
                    width: 600,
                    height: 400
                }).on('beforeclose', erpsys0010_system_registrations_upload_query);
            }
            
            function erpsys0010_system_registrations_upload_query() {
                var ds = '';
                if (erpsys0010_attachment_type == 'ERPSYS0010_BUSINESS_LICENSE') {
                    ds = $('erpsys0010_business_license_ds');
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_TAX_REGISTRATION_CERTIFICTE') {
                    ds = $('erpsys0010_tax_registration_certificte_ds');
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_ORGANIZATION_CODE_CERTIFICATE') {
                    ds = $('erpsys0010_organization_code_certificate_ds');
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_BANK_ACCOUNT_CERTIFICATE') {
                    ds = $('erpsys0010_bank_account_certificate_ds');
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_LEGAL_REPRESENTATIVE_ID') {
                    ds = $('erpsys0010_legal_representative_id_ds');
                }
                ds.setQueryParameter('table_name', erpsys0010_attachment_type);
                ds.setQueryParameter('table_pk_value', '${/model/erpsys0010_sys_system_regist_table_pk_path/record/@header_id}');
                ds.query();
            }
            
            function erpsys0010_business_license_upload() {
                erpsys0010_system_registrations_upload('${l:ERPFND_COMPANIES.BUSINESS_LICENSE}', 'ERPSYS0010_BUSINESS_LICENSE');
            }
            
            function erpsys0010_tax_registration_certificte_upload() {
                erpsys0010_system_registrations_upload('${l:ERPFND_COMPANIES.TAX_REGISTRATION_CERTIFICTE}', 'ERPSYS0010_TAX_REGISTRATION_CERTIFICTE');
            }
            
            function erpsys0010_organization_code_certificate_upload() {
                erpsys0010_system_registrations_upload('${l:ERPFND_COMPANIES.ORGANIZATION_CODE_CERTIFICATE}', 'ERPSYS0010_ORGANIZATION_CODE_CERTIFICATE');
            }
            
            function erpsys0010_bank_account_certificate_upload() {
                erpsys0010_system_registrations_upload('${l:ERPFND_COMPANIES.BANK_ACCOUNT_CERTIFICATE}', 'ERPSYS0010_BANK_ACCOUNT_CERTIFICATE');
            }
            
            function erpsys0010_legal_representative_id_upload() {
                erpsys0010_system_registrations_upload('${l:ERPFND_COMPANIES.LEGAL_REPRESENTATIVE_ID}', 'ERPSYS0010_LEGAL_REPRESENTATIVE_ID');
            }
            
            function erpsys0010_onload(ds) {
                var records = ds.getAll();
                var file_record = $('erpsys0010_uploadfile_ds').getAt(0);
                var file_name = '';
                for (var i = 0;i < records.length;i++) {
                    if (i < 1) {
                        file_name = '<a href="${/request/@context_path}/atm_download.svc?attachment_id=' + records[i].get('attachment_id') + '&table_name=' + erpsys0010_attachment_type + '&table_pk_value=' + '${/model/erpsys0010_sys_system_regist_table_pk_path/record/@header_id}' + '">' + records[i].get('file_name') + '</a><br>';
                    } else if (i == 1) {
                        file_name = file_name + '<a href="${/request/@context_path}/atm_download.svc?attachment_id=' + records[i].get('attachment_id') + '&table_name=' + erpsys0010_attachment_type + '&table_pk_value=' + '${/model/erpsys0010_sys_system_regist_table_pk_path/record/@header_id}' + '">' + records[i].get('file_name') + '</a>';
                    }
                }
                if (erpsys0010_attachment_type == 'ERPSYS0010_BUSINESS_LICENSE') {
                    if (records.length > 2) {
                        file_name = file_name + '[' + '<a href="javascript:erpsys0010_business_license_upload();">${l:ERP_FND.VIEW_MORE}</a>' + ']';
                    }
                    file_record.set('business_license', file_name);
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_TAX_REGISTRATION_CERTIFICTE') {
                    if (records.length > 2) {
                        file_name = file_name + '[' + '<a href="javascript:erpsys0010_tax_registration_certificte_upload();">${l:ERP_FND.VIEW_MORE}</a>' + ']';
                    }
                    file_record.set('tax_registration_certificte', file_name);
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_ORGANIZATION_CODE_CERTIFICATE') {
                    if (records.length > 2) {
                        file_name = file_name + '[' + '<a href="javascript:erpsys0010_organization_code_certificate_upload();">${l:ERP_FND.VIEW_MORE}</a>' + ']';
                    }
                    file_record.set('organization_code_certificate', file_name);
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_BANK_ACCOUNT_CERTIFICATE') {
                    if (records.length > 2) {
                        file_name = file_name + '[' + '<a href="javascript:erpsys0010_bank_account_certificate_upload();">${l:ERP_FND.VIEW_MORE}</a>' + ']';
                    }
                    file_record.set('bank_account_certificate', file_name);
                } else if (erpsys0010_attachment_type == 'ERPSYS0010_LEGAL_REPRESENTATIVE_ID') {
                    if (records.length > 2) {
                        file_name = file_name + '[' + '<a href="javascript:erpsys0010_legal_representative_id_upload();">${l:ERP_FND.VIEW_MORE}</a>' + ']';
                    }
                    file_record.set('legal_representative_id', file_name);
                }
            }
            
            function erpsys0010_uploadfile_ds_validator(record, name, value) {
                if (Aurora.isEmpty(value)) {
                    return '${l:ERPFND_COMPANIES.UPLOAD_COMPANY_FILE}';
                }
                return true;
            }
            
            function erpsys0010_show() {
                var url = $('erpsys0010_service_declare_link').getUrl();
                window.open(url);
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="erpsys0010_business_license_ds" model="erpsys.ERPSYS0010.sys_system_register_attachment">
                <a:events>
                    <a:event name="load" handler="erpsys0010_onload"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="erpsys0010_tax_registration_certificte_ds" model="erpsys.ERPSYS0010.sys_system_register_attachment">
                <a:events>
                    <a:event name="load" handler="erpsys0010_onload"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="erpsys0010_organization_code_certificate_ds" model="erpsys.ERPSYS0010.sys_system_register_attachment">
                <a:events>
                    <a:event name="load" handler="erpsys0010_onload"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="erpsys0010_bank_account_certificate_ds" model="erpsys.ERPSYS0010.sys_system_register_attachment">
                <a:events>
                    <a:event name="load" handler="erpsys0010_onload"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="erpsys0010_legal_representative_id_ds" model="erpsys.ERPSYS0010.sys_system_register_attachment">
                <a:events>
                    <a:event name="load" handler="erpsys0010_onload"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="erpsys0010_uploadfile_ds" autoCreate="true">
                <a:fields>
                    <a:field name="business_license" validator="erpsys0010_uploadfile_ds_validator"/>
                    <a:field name="tax_registration_certificte" validator="erpsys0010_uploadfile_ds_validator"/>
                    <a:field name="organization_code_certificate" validator="erpsys0010_uploadfile_ds_validator"/>
                    <a:field name="bank_account_certificate" validator="erpsys0010_uploadfile_ds_validator"/>
                    <a:field name="legal_representative_id" validator="erpsys0010_uploadfile_ds_validator"/>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="erpsys0010_taxpayer_status_ds" lookupCode="TAXPAYER_STATUS"/>
            <a:dataSet id="erpsys0010_system_registrations_ds" autoCreate="true">
                <a:fields>
                    <a:field name="company_code" required="true" validator="erpsys0010_rule_check"/>
                    <a:field name="telephone" required="true"/>
                    <a:field name="company_short_name" required="true"/>
                    <a:field name="e_mail" required="true"/>
                    <a:field name="company_full_name"/>
                    <a:field name="tax_nature" defaultValue="GENERAL_TAXPAYERS" required="true"/>
                    <a:field name="tax_nature_desc" displayField="code_value_name" options="erpsys0010_taxpayer_status_ds" required="true" returnField="tax_nature" valueField="code_value"/>
                    <a:field name="tax_rate" defaultValue="17" required="true"/>
                    <a:field name="tax_included_flag" checkedValue="Y" defaultValue="Y" required="true" uncheckedValue="N"/>
                    <a:field name="functional_currency_code"/>
                    <a:field name="functional_currency_code_desc" autoComplete="true" autoCompleteField="code_desc" autoCompleteSize="1" lovGridHeight="325" lovHeight="450" lovService="erpsys.sys_currency_query_lov" lovWidth="500" prompt="ERP_FND_USERS.EMPLOYEE_CODE" required="true" title="ERP_SYS.CURRENCY">
                        <a:mapping>
                            <a:map from="currency_code" to="functional_currency_code"/>
                            <a:map from="code_desc" to="functional_currency_code_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="openning_period_year" required="true" validator="erpsys0010_rule_check"/>
                    <a:field name="openning_period_month" required="true"/>
                    <a:field name="period_num" defaultValue="12" required="true"/>
                    <a:field name="accounting_series" defaultValue="3" required="true"/>
                    <a:field name="accounting_code_1" defaultValue="4" readOnly="true" required="true"/>
                    <a:field name="accounting_code_2" defaultValue="2" required="true"/>
                    <a:field name="accounting_code_3" defaultValue="2" required="true"/>
                    <a:field name="accounting_code_4" defaultValue="2"/>
                    <a:field name="accounting_code_5"/>
                    <a:field name="system_mode" defaultValue="4" required="true"/>
                    <a:field name="default_user_name" defaultValue="ADMIN" readOnly="true" required="true"/>
                    <a:field name="default_password" required="true" validator="erpsys0010_rule_check"/>
                    <a:field name="confirm_password" required="true" validator="erpsys0010_rule_check"/>
                    <a:field name="check_code" required="true"/>
                    <a:field name="table_pk_value" defaultValue="${/model/erpsys0010_sys_system_regist_table_pk_path/record/@header_id}"/>
                </a:fields>
                <a:events>
                    <a:event name="update" handler="erpsys0010_system_registrations_ds_update"/>
                </a:events>
            </a:dataSet>
        </a:dataSets>
