        <script><![CDATA[
            function load() {
                var record = $("erpsys0010_system_registrations_ds").getAt(0);
            
                var date = new Date();
                var year = date.getFullYear();
                var month = date.getMonth() + 1;
                record.set("openning_period_year", year);
                record.set("openning_period_month", month);
            
                record.set('functional_currency_code', 'CNY');
                record.set('functional_currency_code_desc', '${l:ERP_SYS.CNY_MONEY}');
            
                record.set('accounting_code_4', '');
            /* 
                var showCC = Aurora.getCookie('showCheckCode');
                if (showCC == 'true') {
                    showCheckCode();
                } */
            }
            Aurora.onReady(function() {
                load();
            });
        ]]></script>
