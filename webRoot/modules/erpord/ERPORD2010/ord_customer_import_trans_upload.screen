                    <a:field name="line_number" required="true"/>
                    <a:field name="session_id" defaultValue="${/parameter/@header_id}"/>
                    <a:field name="partner_code" prompt="客户代码" required="true" typeCase="upper" validator="erpord2010_import_partner_code_validator"/>
                    <a:field name="vender_customer_flag" checkedValue="Y" defaultValue="N" prompt="既是供应商又是客户" uncheckedValue="N"/>
                    <a:field name="partner_name" prompt="客户名称" required="true"/>
                    <a:field name="province" autoComplete="true" autoCompleteField="province_code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovService="erpsys.public.sys_region_province_lov" lovWidth="450" prompt="省" title="省信息">
                        <a:mapping>
                            <a:map from="province_name" to="province"/>
                            <a:map from="province_id" to="province_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="city" autoComplete="true" autoCompleteField="city_code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovService="erpsys.public.sys_region_city_lov" lovWidth="450" prompt="市" title="市信息">
                        <a:mapping>
                            <a:map from="city_name" to="city"/>
                            <a:map from="city_id" to="city_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="district" autoComplete="true" autoCompleteField="district_code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovService="erpsys.public.sys_region_district_lov" lovWidth="500" prompt="县" title="县信息">
                        <a:mapping>
                            <a:map from="district_name" to="district"/>
                            <a:map from="district_id" to="district_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="detail_address" prompt="客户通讯地址"/>
                    <a:field name="legal_representative" prompt="法人代表"/>
                    <a:field name="tax_number" prompt="税务登记号"/>
                    <a:field name="bank_name" prompt="开户银行"/>
                    <a:field name="bank_account" prompt="银行账号"/>
                    <a:field name="homepage" prompt="公司主页"/>
                    <a:field name="enabled_flag" checkedValue="Y" defaultValue="N" prompt="启用" uncheckedValue="N"/>
                    <a:field name="default_flag" checkedValue="Y" defaultValue="N" prompt="设为默认联系人" uncheckedValue="N"/>
                    <a:field name="contact_name" prompt="联系人"/>
                    <a:field name="contact_province" autoComplete="true" autoCompleteField="province_code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovService="erpsys.public.sys_region_province_lov" lovWidth="450" prompt="省" title="省信息">
                        <a:mapping>
                            <a:map from="province_name" to="contact_province"/>
                            <a:map from="province_id" to="contact_province_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="contact_city" autoComplete="true" autoCompleteField="city_code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovService="erpsys.public.sys_region_city_lov" lovWidth="450" prompt="市" title="市信息">
                        <a:mapping>
                            <a:map from="city_name" to="contact_city"/>
                            <a:map from="city_id" to="contact_city_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="contact_district" autoComplete="true" autoCompleteField="district_code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovService="erpsys.public.sys_region_district_lov" lovWidth="500" prompt="县" title="县信息">
                        <a:mapping>
                            <a:map from="district_name" to="contact_district"/>
                            <a:map from="district_id" to="contact_district_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="contact_address" prompt="FND_ADDRESSES.ADDRESS"/>
                    <a:field name="post_code" prompt="邮政编码"/>
                    <a:field name="phone_number" prompt="电话号码"/>
                    <a:field name="mobile_phone" prompt="手机号码"/>
                    <a:field name="fax_number" prompt="传真号码"/>
                    <a:field name="email" prompt="E_mail"/>
                    <a:field name="sys_customer_desc" autoComplete="true" autoCompleteField="code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovService="erpsys.ERPSYS2010.sys_customer_basics?enabled_flag=Y" lovWidth="500" prompt="系统级大客户" title="系统级大客户">
                        <a:mapping>
                            <a:map from="code_name" to="sys_customer_desc"/>
                            <a:map from="customer_code" to="sys_customer_code"/>
                            <a:map from="customer_id" to="sys_customer_id"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="customer_type_desc" autoComplete="true" autoCompleteField="code_name" autoCompleteSize="1" lovGridHeight="350" lovHeight="500" lovLabelWidth="100" lovService="erpord.ERPORD1010.fnd_business_partner_types?enabled_flag=Y" lovWidth="550" prompt="客户类型" title="客户类型">
                        <a:mapping>
                            <a:map from="code_name" to="customer_type_desc"/>
                            <a:map from="partner_type_code" to="partner_type_code"/>
