                    }
                }
            }
            
            //勾选数据导出
            function inv7010_asnHeaderExportSelectFunction() {
                var records = $('inv7010_asn_header_result_ds').getSelected();
                if (records.length == 0) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}');
                    return;
                } else {
                    new Aurora.Window({
                        id: 'inv7010_asnHeaderExport_window',
