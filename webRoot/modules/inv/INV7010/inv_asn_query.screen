            function inv7010_asnHeaderGridQueryRender(grid) {
                var ds = $('inv7010_asn_header_result_ds');
                var list_ds = $('inv7010_asn_header_query_result_selected_ds');
                var records = list_ds.getAll();
                for (var i = 0;i < records.length;i++) {
                    var asn_header_id = records[i].get('asn_header_id');
                    var result_record = ds.find('asn_header_id', asn_header_id);
