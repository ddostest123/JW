                }
            }
            
            //送货单头查询
            
            function inv7010_asnHeader_query() {
                $('inv7010_asn_header_result_ds').query();
            }
            
            //送货单
            
            function inv7010_asnHeaderGridRenderer(value, record, name) {
                if (name == 'asn_number') {
                    return '<a href="javascript:inv7010_asnDocumentView_open(' + record.get('asn_header_id') + ');">' + value + '</a>';
                }
            
                if (name == 'process_status_name') {
                    if (record.get('process_status') == 'SUCCESSED') {
                        return '<div style="background:#00DD00;">' + value + '</div>';
            
                    } else if (record.get('process_status') == 'FAILED') {
                        return '<div style="background:#FF0000;">' + value + '</div>';
            
                    } else {
                        return '<div>' + value + '</div>';
                    }
                }
            
                if (name == 'warning' && (record.get('process_status') == 'FAILED' || record.get('cancel_process_status') == 'FAILED')) {
                    var header_id = record.get('asn_header_id');
                    return '<div style="font-size: 16px;color: #e74c3c;"><a href="javascript:inv7010_showMessage(' + header_id + ')" style="font-size: 16px;color: #e74c3c;"><i class="fa fa-warning"></i></><div>';
                }
            }
            
            function inv7010_showMessage(header_id) {
                var result;
                var process_status;
                var cancel_process_status;
                var error_message;
                var cancel_message;
                if (inv7010_tab_index == 0) {
                    result = $('inv7010_asn_header_result_ds').getAll();
                } else if (inv7010_tab_index == 1) {
                    result = $('inv7010_asn_query_result_ds').getAll();
                }
                for (var i = 0;i < result.length;i++) {
                    if ((result[i].get('asn_header_id') == header_id && inv7010_tab_index == 0) || (result[i].get('asn_line_id') == header_id && inv7010_tab_index == 1)) {
                        process_status = result[i].get('process_status');
                        cancel_process_status = result[i].get('cancel_process_status');
                        error_message = result[i].get('process_message');
                        cancel_message = result[i].get('cancel_process_message');
                    }
                }
                if (Aurora.isEmpty(error_message)) {
                    error_message = '';
                }
                if (Aurora.isEmpty(cancel_message)) {
                    cancel_message = '';
                }
                if (process_status == 'FAILED') {
                    Aurora.showMessage('${l:PROMPT}', error_message, null, 600, 300);
                } else if (cancel_process_status == 'FAILED') {
                    Aurora.showMessage('${l:PROMPT}', cancel_message, null, 600, 300);
                }
            }
            
            function inv7010_asnDocumentView_open(asn_header_id) {
                new Aurora.Window({
                    id: 'inv_asn_document_view_window',
                    title: '${l:INV_ASN_HEDER_DETAIL_TITLE}',
                    url: $('inv7010_asnDocumentView_link').getUrl() + '?asn_header_id=' + asn_header_id,
                    fullScreen: true
                });
            }
            
            function inv7010_asnQueryGridRenderer(value, record, name) {
                if (name == 'process_status_name') {
                    if (record.get('process_status') == 'SUCCESSED') {
                        return '<div style="background:#00FF00;">' + value + '</div>';
            
                    } else if (record.get('process_status') == 'FAILED') {
                        return '<div style="background:#FF0000;">' + value + '</div>';
            
                    } else {
                        return '<div>' + value + '</div>';
                    }
                }
                if (name == 'warning' && (record.get('process_status') == 'FAILED' || record.get('cancel_process_status') == 'FAILED')) {
                    var line_id = record.get('asn_line_id');
                    return '<div style="font-size: 16px;color: #e74c3c;"><a href="javascript:inv7010_showMessage(' + line_id + ')" style="font-size: 16px;color: #e74c3c;"><i class="fa fa-warning"></i></><div>';
                }
            }
            //查找tree的父节点
            
            function inv7010_find_father(code) {
                var tree_ds = $('inv7010_asn_query_parameter_tree_ds');
                var all = tree_ds.getAll();
                for (var i = 0;i < all.length;i++) {
                    if (record.get('query_code') == code) {
                        return all[i].data.parent_query_code;
                    }
                }
            }
            
            //是否有子节点
            
            function inv7010_find_son(code) {
                var tree_ds = $('inv7010_asn_query_parameter_tree_ds');
                var all = tree_ds.getAll();
                for (var i = 0;i < all.length;i++) {
                    if (all[i].data.parent_query_code == code) {
                        return false;
                    }
                }
                return true;
            }
            
            function inv7010_query_set_null() {
                var query_ds;
                if (inv7010_tab_index == 0) {
                    query_ds = $('inv7010_asn_header_query_ds').getAt(0);
                } else if (inv7010_tab_index == 1) {
                    query_ds = $('inv7010_asn_query_ds').getAt(0);
                }
                query_ds.set('status', '');
                query_ds.set('process_status', '');
                query_ds.set('cancel_process_status', '');
                query_ds.set('cancel_process_status1', '');
                query_ds.set('can_print', '');
                query_ds.set('close_flag', '');
            }
            //显示隐藏打印按钮
            
            function inv7010_diable_or_enable_print_btn(mode) {
                //显示
                if (mode == 1) {
                    if (inv7010_tab_index == 0) {
                        $('inv7010_asn_header_print_btn').enable();
                    } else if (inv7010_tab_index == 1) {
                        $('inv7010_asn_line_print_btn').enable();
                    }
                } else if (mode == 2) {
                    if (inv7010_tab_index == 0) {
                        $('inv7010_asn_header_print_btn').disable();
                    } else if (inv7010_tab_index == 1) {
                        $('inv7010_asn_line_print_btn').disable();
                    }
                }
            }
            
            function inv7010_treeGrid_renderer(value, record, name) {
                var index = $('inv7010_asn_query_parameter_tree_ds').indexOf(record);
                var id = "inv7010_" + index;
                inv7010_tree_div.push(id);
                return '<div id="' + id + '" >' + value + '</div>';
            }
            
            function inv7010_tree_click(grid, row, record) {
                var can_click = record.get('can_click');
                var query_code = record.get('query_code');
                if (can_click == 'Y') {
                    $('inv7010_asn_header_result_ds').setQueryParameter('query_code', query_code);
                    $('inv7010_asn_header_result_ds').query();
                }
            }
            
