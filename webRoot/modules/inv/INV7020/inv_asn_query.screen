            //日期比较
            
            function inv7020_compareDate(start, end) {
                if (start > end) {
                    return false;
                }
                return true;
            }
            
            //日期校验
            
            function inv7020_expected_dateValidator(record, name, value) {
                if (name == 'expected_date_from' || name == 'expected_date_to') {
                    var start = record.get('expected_date_from');
                    var end = record.get('expected_date_to');
            
                    if ( !! end) {
                        if (!inv7020_compareDate(start, end)) {
                            return '${l:START_GREATER_THAN_END}';
                        }
                    }
                    return true;
                }
            }
            
            function inv7020_ship_dateValidator(record, name, value) {
                if (name == 'ship_date_from' || name == 'ship_date_to') {
                    var start = record.get('ship_date_from');
                    var end = record.get('ship_date_to');
            
                    if ( !! end) {
                        if (!inv7020_compareDate(start, end)) {
                            return '${l:START_GREATER_THAN_END}';
                        }
                    }
                    return true;
                }
            }
            
            function inv7020_creation_dateValidator(record, name, value) {
                if (name == 'creation_date_from' || name == 'creation_date_to') {
                    var start = record.get('creation_date_from');
                    var end = record.get('creation_date_to');
            
                    if ( !! end) {
                        if (!inv7020_compareDate(start, end)) {
                            return '${l:START_GREATER_THAN_END}';
                        }
                    }
                    return true;
                }
            }
            
            function inv7020_query() {
                var ds = $('inv7020_asn_query_result_ds');
                ds.query();
            }
            
            function inv7020_operation_records_render(value, record, name) {
                var asn_header_id = record.get('asn_header_id');
                var asn_line_id = record.get('asn_line_id');
                if (Aurora.isEmpty(record.get('asn_line_id'))) {
                    asn_line_id = -1;
                }
                return '<a href="javascript:inv7020_go_operation_records(' + asn_header_id + ',' + asn_line_id + ');">${l:PROMPT.OPERATION_RECORD}</a>';
            }
            
            function inv7020_go_operation_records(asn_header_id, asn_line_id) {
                new Aurora.Window({
                    id: 'inv_asn_operation_record_window',
                    title: '${l:PROMPT.OPERATION_RECORD}',
                    url: $('inv7020_asn_operation_record_link').getUrl() + '?asn_header_id=' + asn_header_id + '&asn_line_id=' + asn_line_id,
                    height: 450,
                    width: 600
                });
            
            }
            
            //按单打印操作
            
            function inv7020_asn_header_query_print() {
                var ds = $('inv7020_asn_header_query_result_selected_ds');
                var records = ds.getAll();
                if (records.length == 0) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SELECT_ONE_RECORD_AT_LEAST}!');
                    return false;
                }
                var datas = [];
                for (var i = 0;i < records.length;i++) {
                    datas.push(records[i].data);
                }
                Aurora.Masker.mask(Ext.getBody());
                Aurora.request({
                    url: $('inv7020_asn_print_insert_link').getUrl(),
                    para: datas,
                    success: function(res) {
                        Aurora.Masker.unmask(Ext.getBody());
                        window.open($('inv7020_asn_print_link').getUrl(), '_blank');
                        ds.removeAll();
                        var header_records = $('inv7020_asn_header_result_ds').getAll();
                        for (var j = 0;j < header_records.length;j++) {
                            $('inv7020_asn_header_result_ds').unSelect(header_records[j]);
                        }
                    },
                    failure: function(res) {
                        Aurora.Masker.unmask(Ext.getBody());
                    },
                    error: function(res) {
                        Aurora.Masker.unmask(Ext.getBody());
                    },
                    scope: this
                });
            }
            
            //按单查询，勾选时的操作
            
            function inv7020_asn_header_query_result_select(ds, record) {
                var select_ds = $('inv7020_asn_header_query_result_selected_ds');
                var result_record = select_ds.find('asn_header_id', record.get('asn_header_id'));
                if (Aurora.isEmpty(result_record)) {
                    select_ds.create();
                    var select_record = select_ds.getCurrentRecord();
                    select_record.set('asn_header_id', record.get('asn_header_id'));
                }
            }
            
            //按单查询，去掉选时的操作
            
            function inv7020_asn_header_query_result_unselect(ds, record) {
                var select_ds = $('inv7020_asn_header_query_result_selected_ds');
                var result_record = select_ds.find('asn_header_id', record.get('asn_header_id'));
                if (!Aurora.isEmpty(result_record)) {
                    select_ds.remove(result_record);
                }
            }
            
            //按明细行查询，勾选时的操作
            
