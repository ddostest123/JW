            function inv7020_tree_click(grid, row, record) {
                var can_click = record.get('can_click');
                var query_code = record.get('query_code');
                if (can_click == 'Y') {
                    $('inv7020_asn_header_result_ds').setQueryParameter('query_code', query_code);
                    $('inv7020_asn_header_result_ds').query();
                }
