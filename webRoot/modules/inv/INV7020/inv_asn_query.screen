            //送货单
            
            function inv7020_asnHeaderGridRenderer(value, record, name) {
                if (name == 'asn_number') {
                    return '<a href="javascript:inv7020_asnDocumentView_open(' + record.get('asn_header_id') + ');">' + value + '</a>';
                }
            
                if (name == 'process_status_name') {
                    if (record.get('process_status') == 'SUCCESSED') {
                        return '<div style="background:#00DD00;">' + value + '</div>';
                    } else {
                        return '<div>' + value + '</div>';
                    }
                }
                if (name == 'warning' && (record.get('process_status') == 'FAILED' || record.get('cancel_process_status') == 'FAILED')) {
                    var header_id = record.get('asn_header_id');
                    return '<div style="font-size: 16px;color: #e74c3c;"><a href="javascript:inv7020_showMessage(' + header_id + ')" style="font-size: 16px;color: #e74c3c;"><i class="fa fa-warning"></i></><div>';
                }
            }
            
            function inv7020_showMessage(header_id) {
                //if(record.get('')){}
                var result = $('inv7020_asn_header_result_ds').getAll();
                var process_status;
                var cancel_process_status;
                var error_message;
                var cancel_message;
                for (var i = 0;i < result.length;i++) {
                    if (result[i].get('asn_header_id') == header_id) {
                        process_status = result[i].get('process_status');
                        cancel_process_status = result[i].get('cancel_process_status');
                        error_message = result[i].get('process_message');
                        cancel_message = result[i].get('cancel_process_message');
                    }
                }
                if (Aurora.isEmpty(error_message)) {
                    error_message = '';
                }
                if (Aurora.isEmpty(cancel_message)) {
                    cancel_message = '';
                }
                if (process_status == 'FAILED') {
                    Aurora.showMessage('${l:PROMPT}', error_message, null, 600, 300);
                } else if (cancel_process_status == 'FAILED') {
                    Aurora.showMessage('${l:PROMPT}', cancel_message, null, 600, 300);
                }
            }
            
            function inv7020_asnDocumentView_open(asn_header_id) {
                new Aurora.Window({
                    id: 'inv_asn_document_view_window',
