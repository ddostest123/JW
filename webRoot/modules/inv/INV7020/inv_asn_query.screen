                    fullScreen: true
                });
            }
            
            //查找tree的父节点
            
            function inv7020_find_father(code) {
                var tree_ds = $('inv7020_asn_query_parameter_tree_ds');
                var all = tree_ds.getAll();
                for (var i = 0;i < all.length;i++) {
                    if (all[i].data.query_code == code) {
                        return all[i].data.parent_query_code;
                    }
                }
            }
            
            //是否有子节点
            
            function inv7020_find_son(code) {
                var tree_ds = $('inv7020_asn_query_parameter_tree_ds');
                var all = tree_ds.getAll();
                for (var i = 0;i < all.length;i++) {
                    if (all[i].data.parent_query_code == code) {
                        return false;
                    }
                }
                return true;
            }
            
            function inv7020_query_set_null() {
                var query_ds;
                if (inv7020_tab_index == 0) {
                    query_ds = $('inv7020_asn_header_query_ds').getAt(0);
                } else if (inv7020_tab_index == 1) {
                    query_ds = $('inv7020_asn_query_ds').getAt(0);
                }
                query_ds.set('status', '');
                query_ds.set('process_status', '');
                query_ds.set('cancel_process_status', '');
                query_ds.set('cancel_process_status1', '');
                query_ds.set('can_print', '');
                query_ds.set('close_flag', '');
            }
            
            //显示隐藏打印按钮
            
            function inv7020_diable_or_enable_print_btn(mode) {
                //显示
                if (mode == 1) {
                    if (inv7020_tab_index == 0) {
                        $('inv7020_asn_header_print_btn').enable();
                    } else if (inv7020_tab_index == 1) {
                        $('inv7020_asn_line_print_btn').enable();
                    }
                } else if (mode == 2) {
                    if (inv7020_tab_index == 0) {
                        $('inv7020_asn_header_print_btn').disable();
                    } else if (inv7020_tab_index == 1) {
                        $('inv7020_asn_line_print_btn').disable();
                    }
                }
            }
            
            function inv7020_treeGrid_renderer(value, record, name) {
                var index = $('inv7020_asn_query_parameter_tree_ds').indexOf(record);
                var id = "inv7020_" + index;
