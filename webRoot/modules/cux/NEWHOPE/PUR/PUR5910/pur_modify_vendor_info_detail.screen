            }
            
            //附件上传和下载
            
            function fnd9220_upload_renderer(value, record, name) {
                var attachment_id = record.get('attachment_id'); //附件ID
                if (attachment_id) {
                    return '<a href="javascript:fnd9220_attachment_add_fun(' + attachment_id + ')">上传/下载</a>';
