            <a:dataSet id="fnd5730_sample_certify_structrue_tab_ds" autoQuery="true" bindName="structrue" bindTarget="fnd5730_sample_certify_headers_ds" model="cux.KINWONG.fnd.fnd7210.fnd7210_sample_certify_structure_tab" queryUrl="${/request/@context_path}/autocrud/cux.KINWONG.fnd.fnd7210.fnd7210_sample_certify_structure_tab/query?sample_certification_header_id=${/parameter/@sample_certification_header_id}" selectable="true">
