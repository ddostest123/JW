            </a:dataSet>
            <a:dataSet id="fnd5730_exception_record_ds">
                <a:datas dataSource="fnd5730_exception_record"/>
            </a:dataSet>
            <a:dataSet id="fnd5730_sample_certify_headers_ds" autoQuery="true" model="cux.KINWONG.fnd.fnd5730.fnd5730_sample_certify_query" queryUrl="${/request/@context_path}/autocrud/cux.KINWONG.fnd.fnd5730.fnd5730_sample_certify_query/query?sample_certification_header_id=${/parameter/@sample_certification_header_id}" submitUrl="${/request/@context_path}/modules/cux/KINWONG/fnd/fnd5730/fnd5730_sample_certify_receive_save.svc">
                <a:fields>
                    <!-- <a:field name="szpcb_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="szfpc_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="lcpcb_falg" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="lcfpc_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="lcmpcb_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="jxpcb_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/> -->
                    <a:field name="market_time" required="true"/>
