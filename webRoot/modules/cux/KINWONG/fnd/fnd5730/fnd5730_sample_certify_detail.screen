<a:screen xmlns:a="http://www.aurora-framework.org/application" trace="true">
    <a:init-procedure>
        <a:model-query defaultWhereClause="flex_value_set_code = &apos;KW_ATTACH_TYPE&apos; and flex_value != &apos;40_BIDIRECTIONAL&apos;" fetchAll="true" model="public.fnd_flex_value_v_lov" rootPath="fnd5730_sample_certify_attach_type"/>
        <a:model-query defaultWhereClause="flex_value_set_code = &apos;KW_CHEMICAL_FLAG&apos;" fetchAll="true" model="public.fnd_flex_value_v_lov" rootPath="fnd5730_chemical_flag"/>
        <a:model-query defaultWhereClause="flex_value_set_code = &apos;KW_CHEMICAL_RANGE&apos;" fetchAll="true" model="public.fnd_flex_value_v_lov" rootPath="fnd5730_chemical_range"/>
        <a:model-query defaultWhereClause="flex_value_set_code = &apos;KW_EXCEPTION_RECORD&apos;" fetchAll="true" model="public.fnd_flex_value_v_lov" rootPath="fnd5730_exception_record"/>
    </a:init-procedure>
    <a:view>
        <a:link id="fnd5730_sample_certify_submit_link" model="cux.KINWONG.fnd.fnd5730.fnd5730_sample_certify_query" modelaction="execute"/>
        <a:link id="fnd5750_sgs_report_add_link" url="${/request/@context_path}/modules/cux/KINWONG/qms/qms2110/qms2110_env_material_select.screen"/>
        <a:link id="qms2010_env_material_reprot_readOnly_link" url="${/request/@context_path}/modules/cux/KINWONG/qms/qms2010/qms2010_env_material_report_readonly.screen"/>
        <script><![CDATA[
            function init() {
                var status = '${/parameter/@status}';
                var hBox = document.getElementById('fnd5730_return_reason_id');
                if (status == '50_RETURNED') {
                    hBox.style.display = 'block';
                } else {
                    hBox.style.display = 'none';
                }
            }
            
            function pur51401_compareDate(start, end) {
                if (typeof(start) == 'string') {
                    start = new Date(start.replace(/-/g, "/"));
                }
            
                if (typeof(end) == 'string') {
                    end = new Date(end.replace(/-/g, "/"));
                }
            
                if (start > end) {
                    return false;
                }
                return true;
            }
            
            function fnd5730_sample_certify_returnFun() {
            
                $('fnd5730_sample_certify_detail_win').close();
            }
            //成立日期校验
            
            function pur51401_founding_dateValidator(record, name, value) {
                if (name == 'c_attribute3') {
                    var c_attribute3 = record.get('c_attribute3');
                    var end = new Date();
            
                    if (!pur51401_compareDate(c_attribute3, end)) {
                        return '成立日期不能晚于当前日期！';
                    }
                    return true;
                }
            }
            
            function fnd5730_sample_certify_upload_rendererFun(value, record, name) {
                debugger;
                var ds = $('fnd5730_sample_certify_headers_ds');
                var sample_certification_header_id = ds.getCurrentRecord().get('sample_certification_header_id');
            
                if (name == 'market_time_att') {
                    if (record.get('atm_count_mt') == 0) {
                        return '<a style="color:blue" href="javascript:market_time_uploadFun(' + sample_certification_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                    } else {
                        return '<a style="color:blue" href="javascript:market_time_uploadFun(' + sample_certification_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>' + '<img id="fnd5750_sample_certify_downloadBtu_img_3" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                    }
                }
            
                if (name == 'exception_record_att') {
                    if (record.get('atm_count_er') == 0) {
                        return '<a style="color:blue" href="javascript:exception_record_uploadFun(' + sample_certification_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>';
                    } else {
                        return '<a style="color:blue" href="javascript:exception_record_uploadFun(' + sample_certification_header_id + ')">${l:PROMPT.VIEW_ATTACHMENT}</a>' + '<img id="fnd5750_sample_certify_downloadBtu_img_4" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                    }
                }
            }
            
            function market_time_uploadFun(id) {
                var url = "${/request/@context_path}/attachment_file_upload.screen?table_name=KW_FND_SAMPLE_CERTIFY_MT&pkvalue=" + id;
                new Aurora.Window({
                    url: url,
                    title: '物料上市时间',
                    id: 'market_time_upload_window',
                    width: 500,
                    height: 500
                });
            }
            
            function exception_record_uploadFun(id) {
                var url = "${/request/@context_path}/attachment_file_upload.screen?table_name=KW_FND_SAMPLE_CERTIFY_ER&pkvalue=" + id;
                new Aurora.Window({
                    url: url,
                    title: '异常记录情况',
                    id: 'exception_record_upload_window',
                    width: 500,
                    height: 500
                });
            }
            
            function fnd5730_sample_certify_sgs_tab_ds_submitsuccessFun() {
                $('fnd5730_sample_certify_sgs_tab_ds').query();
            }
            
            function fnd5730_sample_certify_structrue_tab_ds_submitsuccessFun() {
                $('fnd5730_sample_certify_structrue_tab_ds').query();
            }
            
            function fnd5730_sample_certify_attachment_tab_ds_submitsuccessFun() {
                $('fnd5730_sample_certify_attachment_tab_ds').query();
            }
            
            function fnd5730_statusDisplayRenderer(value, record, name) {
                debugger;
            
                if (name == 'status_display') {
                    var disabled_date = record.get('disabled_date');
                    if (!Aurora.isEmpty(disabled_date)) {
                        var days = record.get('days');
                        if (days > 0 && days <= 7) {
                            return '<div style="background-color:yellow">' + '临近过期' + '</div>';
                        } else if (days <= 0) {
                            return '<div style="background-color:red">' + '已过期' + '</div>';
                        } else {
                            return '正常';
                        }
                    } else {
                        return null;
                    }
                }
            
            
                if (name == 'operation') {
                    var env_materials_id = record.get('env_materials_id');
                    return '<a href="javascript:fnd5730_envmsdl_report_detail(' + env_materials_id + ')">' + '查看' + '</a>';
                }
            }
            
            function fnd5730_envmsdl_report_detail(env_materials_id) {
                new Aurora.Window({
                    id: 'fnd5730_envmsdl_report_detail_win',
                    title: '环保报告',
                    url: $('qms2010_env_material_reprot_readOnly_link').getUrl() + '?env_materials_id=' + env_materials_id,
                    width: 850,
                    height: 600
                });
            }
            //sgs附件
            
            function fnd5730_sample_sgsatm_upload_rendererFun(value, record, name) {
                var sgs_report_id = record.get('sgs_report_id');
                if (record.isNew) {
                    return '';
                }
                if (record.get('atm_count_sgs') == 0) {
                    return '<a style="color:blue" href="javascript:fnd5730_sample_certify_sgs_uploadFun(' + sgs_report_id + ')">${l:ATM.UPLOAD_ATTACHMENT}</a>';
                } else {
                    return '<a style="color:blue" href="javascript:fnd5730_sample_certify_sgs_uploadFun(' + sgs_report_id + ')">${l:ATM.UPLOAD_ATTACHMENT}</a>' + '<img id="fnd5730_vendor_survey_manufacturer_uploadBtu_img_2" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                }
            }
            
            function fnd5730_sample_certify_sgs_uploadFun(id) {
                var url = "${/request/@context_path}/attachment_file_upload.screen?table_name=KW_SAMPLE_SGS_REPORT_LINES&pkvalue=" + id;
                new Aurora.Window({
                    id: 'fnd5730_attachment_file_upload_link_window',
                    title: '${l:PROMPT.VENDOR_ATTACHMENT}',
                    url: url,
                    width: 610,
                    height: 500
                }).on('close', function() {
                    $('fnd5730_sample_certify_attachment_tab_ds').query();
                });
            }
            //上传供应商附件
            
            function fnd5730_sample_atm_upload_rendererFun(value, record, name) {
                debugger;
                var attachment_id = record.get('attachment_id');
                if (record.isNew) {
                    return '';
                }
                if (record.get('atm_count_vendor') == 0) {
                    return '<a style="color:blue" href="javascript:fnd5730_sample_certify_uploadFun(' + attachment_id + ')">${l:ATM.UPLOAD_ATTACHMENT}</a>';
                } else {
                    return '<a style="color:blue" href="javascript:fnd5730_sample_certify_uploadFun(' + attachment_id + ')">${l:ATM.UPLOAD_ATTACHMENT}</a>' + '<img id="fnd5730_vendor_survey_manufacturer_uploadBtu_img_2" height="12" src="${/request/@context_path}/images/touch/report.png" width="15"/>';
                }
            }
            
            function fnd5730_sample_certify_uploadFun(id) {
                var url = "${/request/@context_path}/attachment_file_upload.screen?table_name=KW_FND_SAMPLE_CERTIFY_ATTACHMENT&pkvalue=" + id;
                new Aurora.Window({
                    id: 'fnd5730_attachment_file_upload_link_window',
                    title: '${l:PROMPT.VENDOR_ATTACHMENT}',
                    url: url,
                    width: 610,
                    height: 500
                }).on('close', function() {
                    $('fnd5730_sample_certify_attachment_tab_ds').query();
                });
            }
            //无头内容时提示已提交
            
            function fnd5730_sample_certify_ds_load() {
                var records = $('fnd5730_sample_certify_headers_ds').getAll();
                if (records.length == 0) {
                    Aurora.showMessage('提示', '样品认证申请已提交!', function() {
                        window.close();
                    });
                }
            }
            
            function fnd5730_sample_certify_ds_update(value, record, name) {
                var chemical_flag = record.get('chemical_flag');
                if (name == "chemical_flag") {
                    if (chemical_flag == '-20179779770759243') {
                        record.getField('chemical_range_desc').setReadOnly(false);
            
                        record.getField('chemical_range_desc').setRequired(true);
                    } else if (chemical_flag == '-5745718419227655414') {
                        record.set('chemical_range_desc', '');
                        record.set('chemical_range', '');
                        record.getField('chemical_range_desc').setReadOnly(true);
                        record.getField('chemical_range_desc').setRequired(false);
                    }
                }
            
            }
            
            function fnd5730_sample_certify_headers_ds_submitsuccessFun() {
                Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SAVE_SUCCESS}', null);
                $('fnd5730_sample_certify_headers_ds').query();
            }
            
            function fnd5730_maskWindow() {
                if ($('fnd5730_sample_certify_detail_win')) {
                    Aurora.Masker.mask($('fnd5730_sample_certify_detail_win').wrap, '...');
                } else {
                    Aurora.Masker.mask(Ext.getBody(), '..');
                }
            }
            
            function fnd5730_unmaskWindow() {
                if ($('fnd5730_sample_certify_detail_win')) {
                    Aurora.Masker.unmask($('fnd5730_sample_certify_detail_win').wrap, '...');
                } else {
                    Aurora.Masker.unmask(Ext.getBody(), '..');
                }
            }
            
            function fnd5730_sample_certify_saveFun() {
                if (!$('fnd5730_sample_certify_headers_ds').validate()) {
                    return;
                }
                fnd5730_maskWindow();
                $('fnd5730_sample_certify_headers_ds').submit();
                fnd5730_unmaskWindow();
            }
            //校验是否有数据未保存
            
            function check_update() {
            
                if ($('fnd5730_sample_certify_headers_ds').isModified()) {
                    return 'Y';
                }
            
                if ($('fnd5730_sample_certify_sgs_tab_ds').isModified()) {
                    return 'Y';
                }
                if ($('fnd5730_sample_certify_structrue_tab_ds').isModified()) {
                    return 'Y';
                }
                if ($('fnd5730_sample_certify_attachment_tab_ds').isModified()) {
                    return 'Y';
                }
            
                return 'N';
            }
            
            function fnd5750_sgs_report_add() {
                var supplier_company_id = $('fnd5730_sample_certify_headers_ds').getCurrentRecord().get('supplier_company_id');
                new Aurora.Window({
                    id: 'qms2130_env_material_add_win',
                    title: '',
                    url: $('fnd5750_sgs_report_add_link').getUrl() + '?line_id=' + supplier_company_id + '&sgs_flag=1',
                    width: 1200,
                    height: 600
                });
            }
            
            function fnd5730_sample_certify_submitFun() {
            
                Aurora.showConfirm('${l:PROMPT}', '${l:DOCUMNET_SUBMIT_CONFIRM}', function() {
                    debugger;
                    var isModified = check_update();
                    if (isModified == 'Y') {
                        Aurora.showMessage('${l:PROMPT}', '${l:PUR5730.SAVE_DATA_FIRST}', null);
                        return '';
                    }
                    var ds = $('fnd5730_sample_certify_headers_ds');
                    var atm_records = $('fnd5730_sample_certify_attachment_tab_ds').getAll();
                    var record = $('fnd5730_sample_certify_headers_ds').getCurrentRecord();
                    var atm_count_mt = record.get('atm_count_mt');
                    if (atm_count_mt == 0) {
                        Aurora.showMessage('${l:PROMPT}', '证明资料1附件不能为空！', null);
                        return '';
                    }
                    if (atm_records.length <= 0) {
                        Aurora.showMessage('${l:PROMPT}', '请至少维护一条附件信息！', null);
                        return '';
                    }
            
                    if (!ds.validate()) {
                        return;
                    }
                    fnd5730_maskWindow();
                    Aurora.request({
                        url: $('fnd5730_sample_certify_submit_link').getUrl(),
                        para: {
                            'sample_certification_header_id': '${/parameter/@sample_certification_header_id}'
                        },
                        success: function(result) {
                            Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SUBMIT_SUCCESS}', null);
                            fnd5730_unmaskWindow();
                            fnd5730_sample_certify_returnFun();
                        },
                        failure: function() {
                            fnd5730_unmaskWindow();
                        },
                        error: function() {
                            fnd5730_unmaskWindow();
                        },
                        scope: this
                    });
                });
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="fnd5730_sample_certify_attach_type_ds">
                <a:datas dataSource="fnd5730_sample_certify_attach_type"/>
            </a:dataSet>
