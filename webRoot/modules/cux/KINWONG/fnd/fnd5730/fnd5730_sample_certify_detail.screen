        <a:screenBody>
            <a:screenTopToolbar>
                <a:toolbarButton id="fnd5730_sample_certify_saveBtn" click="fnd5730_sample_certify_saveFun" style="margin-left:20px;" text="HAP_SAVE" width="100"/>
                <a:toolbarButton id="fnd5730_sample_certify_submitBtn" click="fnd5730_sample_certify_submitFun" text="HAP_SUBMIT" width="100"/>
                <a:toolbarButton id="fnd5730_sample_certify_backBtn" click="fnd5730_sample_certify_returnFun" text="HAP_BACK" width="100"/>
            </a:screenTopToolbar>
            <a:vBox labelWidth="120" padding="0">
                <a:hBox labelWidth="120" style="margin-top:5px;">
                    <a:textField name="application_number" bindTarget="fnd5730_sample_certify_headers_ds" prompt="申请编号" readOnly="true"/>
                    <a:textField name="status_desc" bindTarget="fnd5730_sample_certify_headers_ds" prompt="状态" readOnly="true"/>
                    <a:textField name="application_by_desc" bindTarget="fnd5730_sample_certify_headers_ds" prompt="申请人" readOnly="true"/>
                </a:hBox>
                <a:hBox labelWidth="120" style="margin-top:5px;">
