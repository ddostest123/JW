                    <a:field name="qualified_units_desc" readOnly="true"/>
                    <a:field name="chemical_flag"/>
                    <a:field name="chemical_flag_desc" displayField="flex_desc" options="fnd5730_chemical_flag_ds" required="true" returnField="chemical_flag" valueField="flex_value"/>
                    <a:field name="chemical_range"/>
                    <a:field name="chemical_range_desc" displayField="flex_desc" options="fnd5730_chemical_range_ds" readOnly="true" returnField="chemical_range" valueField="flex_value"/>
                    <a:field name="exception_record"/>
                    <a:field name="exception_record_desc" displayField="flex_desc" options="fnd5730_exception_record_ds" required="true" returnField="exception_record" valueField="flex_value"/>
                </a:fields>
                <a:events>
                    <a:event name="load" handler="fnd5730_sample_certify_ds_load"/>
                    <a:event name="update" handler="fnd5730_sample_certify_ds_update"/>
                    <a:event name="submitsuccess" handler="fnd5730_sample_certify_headers_ds_submitsuccessFun"/>
                </a:events>
            </a:dataSet>
            <!--  <a:dataSet id="fnd5730_sample_certify_sgs_tab_ds" autoQuery="true" bindName="sgs" bindTarget="fnd5730_sample_certify_headers_ds" model="cux.KINWONG.fnd.fnd7210.fnd7210_sample_certify_sgs_tab" queryUrl="${/request/@context_path}/autocrud/cux.KINWONG.fnd.fnd7210.fnd7210_sample_certify_sgs_tab/query?sample_certification_header_id=${/parameter/@sample_certification_header_id}" selectable="true">
                <a:fields>
                    <a:field name="sample_certification_header_id" defaultValue="${/parameter/@sample_certification_header_id}"/>
                    <a:field name="hf_materials_flag" checkedValue="Y" defaultValue="N" uncheckedValue="N"/>
                </a:fields>
                <a:events>
                    <a:event name="submitSuccess" handler="fnd5730_sample_certify_sgs_tab_ds_submitsuccessFun"/>
                </a:events>
            </a:dataSet> -->
            <a:dataSet id="fnd5730_sample_certify_sgs_tab_ds" autoCount="true" autoQuery="true" bindName="sgs" bindTarget="fnd5730_sample_certify_headers_ds" fetchAll="true" model="cux.KINWONG.fnd.fnd7210.fnd7210_sample_certify_sgs_tab" queryUrl="${/request/@context_path}/autocrud/cux.KINWONG.fnd.fnd7210.fnd7210_sample_certify_sgs_tab/query?sample_certification_header_id=${/parameter/@sample_certification_header_id}" selectable="true">
