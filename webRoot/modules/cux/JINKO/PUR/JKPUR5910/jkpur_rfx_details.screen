                                <a:column name="upload_date" align="center" prompt="FND_ATM_LINES.UPLOAD_DATE" width="100"/>
                                <a:column name="atm_flag" align="center" renderer="pur5130_rfxDetailAttachment_renderFun" width="20"/>
