                                        <a:comboBox name="rfx_mode_desc" bindTarget="pur71101_rfx_headers_ds" prompt="竞标模式" readOnly="true"/>
                                        <a:comboBox name="price_category_desc" bindTarget="pur71101_rfx_headers_ds" prompt="标准/样件" readOnly="true"/>
                                        <a:numberField name="extend_date" allowNegative="false" bindTarget="pur71101_rfx_headers_ds" decimalPrecision="2" prompt="延长截标时间（m）" readOnly="true" width="170"/>
                                    </a:hBox>
                                </a:vBox>
                            </a:accordion>
                        </a:accordions>
                    </a:accordionPanel>
                </a:hBox>
            </a:vBox>
            <a:tabPanel height="400" marginWidth="130">
                <a:tabs>
                    <a:tab prompt="PUR5120.ITEM_DETIAL" width="120">
                        <a:grid id="pur51302_rfx_ln_items_grid" bindTarget="pur51303_rfx_ln_items_ds" height="350" marginWidth="140" navBar="true">
