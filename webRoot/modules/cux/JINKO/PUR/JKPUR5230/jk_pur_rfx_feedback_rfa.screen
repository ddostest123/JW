            function sacpur52105RfxFeedbackRfqSave() {
                var item_ds = $('jkpur5230_rfx_feedback_rfa_line_ds');
                if (!item_ds.validate()) {
                    return;
                }
                var item_rec = item_ds.getAll();
                for (i = 0;i < item_rec.length;i++) {
                    item_rec[i].dirty = true;
                }
                var data = {};
                data['items'] = item_ds.getJsonData();
                Aurora.Masker.mask($('jkpur5230_rfx_feedback_rfa_window').wrap, '${l:LOADING}');
                Aurora.request({
                    url: $('jkpur5230_rfx_feedback_save_link').getUrl(),
                    para: data,
                    success: function(result) {
                        sacpur52101RfxFeedbackRfaCountsAndToalPrice();
                        $('jkpur5230_rfx_feedback_rfa_line_ds').query();
                        Aurora.Masker.unmask($('jkpur5230_rfx_feedback_rfa_window').wrap);
                    },
                    failure: function() {
                        Aurora.Masker.unmask($('jkpur5230_rfx_feedback_rfa_window').wrap);
                        return;
                    },
                    error: function() {
                        Aurora.Masker.unmask($('jkpur5230_rfx_feedback_rfa_window').wrap);
                        return;
                    }
                });
            }
            
            function sacpur52105RfxFeedbackRfqSubmit() {
                var header_ds = $('jkpur5230_rfx_feedback_rfa_header_ds');
                var item_ds = $('jkpur5230_rfx_feedback_rfa_line_ds');
                if (!item_ds.validate()) {
                    return;
                }
                var header_record = header_ds.getCurrentRecord();
                var feedback_header_id = header_record.get('feedback_header_id');
                var status = header_record.get('status');
            
                var item_rec = item_ds.getAll();
                for (i = 0;i < item_rec.length;i++) {
                    item_rec[i].dirty = true;
                }
            
                var data = {};
                data.feedback_header_id = feedback_header_id;
                data.status = status;
                data['items'] = item_ds.getJsonData();
                Aurora.Masker.mask($('jkpur5230_rfx_feedback_rfa_window').wrap, '${l:LOADING}');
                jkpur5230_stop_auto_refresh();
                Aurora.request({
                    url: $('jkpur5230_rfx_feedback_submit_link').getUrl(),
                    para: data,
                    success: function(result) {
                        Aurora.Masker.unmask($('jkpur5230_rfx_feedback_rfa_window').wrap);
                        Aurora.showMessage('${l:PROMPT}', '${l:SACPUR5210.SUBMITTED_SUCCESSFULLY}');
                        jkpur5230_window_reload();
                        jkpur5230_start_auto_refresh();
                    },
                    failure: function() {
                        Aurora.Masker.unmask($('jkpur5230_rfx_feedback_rfa_window').wrap);
                        jkpur5230_start_auto_refresh();
                        return;
                    },
                    error: function() {
                        Aurora.Masker.unmask($('jkpur5230_rfx_feedback_rfa_window').wrap);
                        jkpur5230_start_auto_refresh();
                        return;
                    }
                });
            }
            
