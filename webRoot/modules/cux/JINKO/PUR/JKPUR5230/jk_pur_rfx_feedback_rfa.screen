            function jkpur5230_rfx_feedback_rfa_line_render(value, record, name) {
                var open_rule = record.get('open_rule'); //公开规则
                var auction_rule = record.get('auction_rule'); //竞价规则
                var ranking_rule = record.get('ranking_rule'); //排名规则
                var auction_ranking = record.get('auction_ranking'); //竞价排名
                if (name == 'current_rank') {
                    if (auction_ranking == 'OPEN_COUNT_OPEN_RANK' || auction_ranking == 'HIDE_COUNT_OPEN_RANK') {
                        return value;
                    } else if (auction_ranking == 'OPEN_COUNT_HIDE_RANK' || auction_ranking == 'HIDE_COUNT_HIDE_RANK') {
                        return jkpur5230_hidden_display;
                    }
                }
            
                if (name == 'feedback_by_count') {
                    if (auction_ranking == 'OPEN_COUNT_OPEN_RANK' || auction_ranking == 'OPEN_COUNT_HIDE_RANK') {
                        return value;
                    } else if (auction_ranking == 'HIDE_COUNT_OPEN_RANK' || auction_ranking == 'HIDE_COUNT_HIDE_RANK') {
                        return jkpur5230_hidden_display;
                    }
                }
            }
            
            function jkpur5230_rfx_feedback_rfa_record_render(value, record, name) {
                var open_rule = record.get('open_rule'); //公开规则
                var auction_rule = record.get('auction_rule'); //竞价规则
                var ranking_rule = record.get('ranking_rule'); //排名规则
                var auction_ranking = record.get('auction_ranking'); //竞价排名
                var vendor_desc = record.get('vendor_desc'); //供应商
                var current_vendor = '${/model/header_record_rp/record/@current_company}'; //供应商
                if (open_rule == 'HIDDEN_IDENTITY_HIDDEN_QUOTN' && vendor_desc != current_vendor) { //隐藏身份隐藏报价
                    if (name == 'vendor_desc' || name == 'feedback_retail_price') {
                        return jkpur5230_hidden_display;
                    }
                } else if (open_rule == 'HIDDEN_IDENTITY_OPEN_QUOTN' && vendor_desc != current_vendor) { //隐藏身份公开报价
                    if (name == 'vendor_desc') {
                        return jkpur5230_hidden_display;
                    } else if (name == 'feedback_retail_price') {
                        return formatNumber2(value, 8);
                    }
                } else if (open_rule == 'OPEN_IDENTITY_HIDDEN_QUOTN' && vendor_desc != current_vendor) { //公开身份隐藏报价
                    if (name == 'vendor_desc') {
                        return value;
                    } else if (name == 'feedback_retail_price') {
                        return jkpur5230_hidden_display;
                    }
            
                } else if (open_rule == 'OPEN_IDENTITY_OPEN_QUOTN' && vendor_desc != current_vendor) { //公开身份公开报价
                    if (name == 'vendor_desc') {
                        return value;
                    } else if (name == 'feedback_retail_price') {
                        return formatNumber2(value, 8);
                    }
                } else {
                    return value;
                }
            
                if (name == 'rk') {
                    if (ranking_rule == 'HIDE_COUNT_OPEN_RANK' || ranking_rule == 'OPEN_COUNT_OPEN_RANK' || vendor_desc == current_vendor) {
                        return value;
                    } else {
                        return jkpur5230_hidden_display;
                    }
                }
            }
            
            
