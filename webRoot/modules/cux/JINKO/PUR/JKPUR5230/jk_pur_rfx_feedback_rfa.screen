                var header_record = $('jkpur5230_rfx_feedback_rfa_header_ds').getAt(0);
                var init_feedback_end_time = header_record.get('init_feedback_end_time');
                var records = ds.getAll();
                var left_time;
                if (records.length == 0 || first_load == 'Y') {
                    var feedback_end_time = header_record.get('feedback_end_time');
                    left_time = header_record.get('left_time');
                    
                    //设置倒计时文字
                    set_countdown_text(feedback_end_time, init_feedback_end_time);
                    
                    //初始化倒计时
                    jkpur5230_init_countdown(left_time);
                } else {
                    //可能其他供应商提交后「截止时间」延长，所以每次查询都更新头上「截止时间」字段
                    var end_date = records[0].get('end_date');
                    left_time = records[0].get('left_time');
                    header_record.set('feedback_end_time', end_date);
                    
                    //设置倒计时文字
                    set_countdown_text(end_date, init_feedback_end_time);
                    
                    //更新倒计时
                    jkpur5230_reload_countdown(left_time);
                }
                //如果时间截止，就停止自动更新
                if (left_time <= 0) {
                    jkpur5230_stop_auto_refresh();
                }
                g_feedback_end_time = header_record.get('feedback_end_time');
            }
