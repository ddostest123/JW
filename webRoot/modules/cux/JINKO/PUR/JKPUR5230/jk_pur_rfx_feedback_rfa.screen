                    <a:field name="abandoned_flag" checkedValue="Y" defaultValue="N" uncheckedValue="N"/>
                    <!-- <a:field name="feedback_expiry_date_from" required="true" requiredMessage="${l:PUR5210.FEEDBACK_EXPIRY_DATE}" validator="jkpur5230_dateValidator"/> -->
                    <!-- <a:field name="feedback_expiry_date_to" required="true" requiredMessage="${l:PUR5210.FEEDBACK_EXPIRY_DATE}" validator="jkpur5230_dateValidator"/> -->
                    <a:field name="promised_date" validator="jkpur5230_dateValidator"/>
                    <a:field name="tax_included_flag" checkedValue="Y" defaultValue="N" uncheckedValue="N"/>
                    <a:field name="samples_requested" checkedValue="Y" defaultValue="N" uncheckedValue="N"/>
                    <a:field name="trans_cost_flag" checkedValue="Y" readOnly="true" uncheckedValue="N"/>
                    <a:field name="tax_type_rate" lovHeight="480" lovService="public.fnd_tax_type_code_lov" lovWidth="500" requiredMessage="${l:PUR5130.PLEASE_CHOOSE_TAX_CODE}" title="PUR_REQ_LINES.TAX_TYPE_RATE">
                        <a:mapping>
                            <a:map from="tax_type_id" to="tax_type_id"/>
                            <a:map from="tax_type_code" to="tax_type_code"/>
                            <a:map from="description" to="tax_type_desc"/>
                            <a:map from="tax_type_rate" to="tax_type_rate"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="current_fb_retail_price" required="true" requiredMessage="请先输入报价！"/>
                </a:fields>
                <!-- <a:events>
                    <a:event name="update" handler="jkpur5230_rfx_feedback_rfa_line_ds_update"/>
                    <a:event name="load" handler="jkpur5230_rfx_feedback_rfa_line_ds_load"/>
                </a:events> -->
            </a:dataSet>
            <a:dataSet id="jkpur5230_rfx_feedback_rfa_record_ds" bindName="feedback_rfa_records" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" fetchAll="true" queryUrl="${/request/@context_path}/modules/cux/JINKO/PUR/JKPUR5230/jk_pur_rfx_feedback_rfa_record_cache.svc">
                <a:fields>
                    <a:field name="is_online_desc" displayField="code_value_name" options="jkpur5230_online_status_ds" returnField="is_online" valueField="code_value"/>
                </a:fields>
                <a:events>
                    <a:event name="load" handler="jkpur5230_rfx_feedback_record_ds_load"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="jkpur5230_rfx_feedback_vendor_ds" bindName="feedback_rfa_chart_vendor" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" fetchAll="true" queryUrl="${/request/@context_path}/modules/cux/JINKO/PUR/JKPUR5230/jk_pur_rfx_feedback_rfa_vendor_cache.svc">
                <a:events>
                    <a:event name="load" handler="jkpur5230_rfx_feedback_vendor_ds_load"/>
                </a:events>
            </a:dataSet>
            <!-- <a:dataSet id="jkpur5230_rfx_feedback_rfa_rank_ds" bindName="feedback_rfa_ranks" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" fetchAll="true" model="cux.JINKO.PUR.JKPUR5230.jk_pur_rfx_feedback_rfa_rank_query"/> -->
            <a:dataSet id="jkpur5230_rfx_feedback_rfa_chart_forward_ds" fetchAll="true" queryUrl="${/request/@context_path}/modules/cux/JINKO/PUR/JKPUR5230/jk_pur_rfx_record_chart_query.svc">
                <a:fields>
                    <a:field name="vendor_desc" prompt="供应商名称"/>
                    <a:field name="feedback_time" prompt="竞标时间"/>
                    <a:field name="feedback_retail_price" prompt="竞标价格"/>
                </a:fields>
                <a:events>
                    <a:event name="load" handler="jkpur5230_rfx_feedback_chart_ds_load"/>
                </a:events>
            </a:dataSet>
        </a:dataSets>
        <a:screenBody>
            <a:screenTopToolbar>
                <!-- <a:toolbarButton click="sacpur52105RfxFeedbackRfqSave" text="HAP_SAVE" width="100"/> -->
                <!-- <a:toolbarButton click="sacpur52105RfxFeedbackRfqSubmit" text="HAP_SUBMIT" width="100"/> -->
                <a:toolbarButton click="jkpur5230_auto_refresh" text="刷新" width="100"/>
                <a:toolbarButton click="sacpur52105RfxFeedbackRfqReturn" text="HAP_BACK" width="100"/>
            </a:screenTopToolbar>
            <a:hBox>
                <a:vBox labelWidth="90" padding="0">
                    <a:hBox labelWidth="90">
                        <a:dateTimePicker name="feedback_start_time" bindTarget="jkpur5230_rfx_feedback_rfa_header_ds" prompt="JK_PUR_RFX_HEADERS.FEEDBACK_START_TIME" readOnly="true"/>
                        <a:dateTimePicker name="feedback_end_time" bindTarget="jkpur5230_rfx_feedback_rfa_header_ds" prompt="JK_PUR_RFX_HEADERS.FEEDBACK_END_TIME" readOnly="true"/>
                        <a:textField name="item_desc" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" prompt="物料描述" readOnly="true" width="400"/>
                    </a:hBox>
                    <a:hBox labelWidth="90">
                        <a:checkBox name="tax_included_flag" bindTarget="jkpur5230_rfx_feedback_rfa_header_ds" prompt="PUR_RFX_HEADERS.TAX_INCLUDED_FLAG" readOnly="true" width="152"/>
                        <a:textField name="tax_type_rate" bindTarget="jkpur5230_rfx_feedback_rfa_header_ds" prompt="PUR_RFX_HEADERS.TAX_RATE" readOnly="true"/>
                        <!-- <a:textField name="limit_price" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" prompt="限价" readOnly="true"/> -->
                        <a:textField name="feedback_range" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" prompt="报价幅度" readOnly="true"/>
                    </a:hBox>
                    <a:hBox labelWidth="90">
                        <!-- <a:textField name="buffer_range" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" prompt="报价缓冲幅度" readOnly="true"/> -->
                        <a:textField name="uom_desc" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" prompt="价格单位" readOnly="true"/>
                        <a:numberField name="current_fb_retail_price" bindTarget="jkpur5230_rfx_feedback_rfa_line_ds" prompt="JK_PUR_RFX_FEEDBACK_LINES.CURRENT_FB_RETAIL_PRICE" width="152">
                            <a:events>
                                <a:event name="enterdown" handler="sacpur52105RfxFeedbackRfqSubmit"/>
                            </a:events>
                        </a:numberField>
                        <a:toolbarButton click="sacpur52105RfxFeedbackRfqSubmit" text="HAP_SUBMIT" width="50"/>
                    </a:hBox>
                </a:vBox>
                <div class="circle">
                    <span id="jkpur5230_rfx_feedback_countdown_text" style="display:block;margin-left:10px; margin-top:20px;"><![CDATA[${l:SACPUR5210.HOW_LONG_IS_QUOTATION_DEADLINE}]]></span>
                    <div id="countdown18" class="ClassyCountdownDemo"/>
                </div>
            </a:hBox>
            <a:hBox>
                <a:chart id="jkpur5230_rfx_feedback_rfa_chart" backgroundColor="" bindTarget="jkpur5230_rfx_feedback_rfa_chart_forward_ds" borderWidth="0" height="330" plotBackgroundColor="" plotBorderWidth="0" style="margin-right:10px" type="line">
                    <a:placeHolder id="jkpur5230_chart_placeHolder"/>
                    <a:legend align="right" backgroundColor="rgba(255,255,255,0.9)" borderWidth="1" floating="true" labelFormatter="jkpur5230_label_format1" layout="vertical" verticalAlign="top"/>
                    <a:xAxis>
                        <a:xAxi name="feedback_time" gridLineWidth="0" tickLength="0">
                            <!-- <a:plotBands>
                                        <a:plotBand color="rgba(68,170,213,.2)" from="4.5" to="6.5"/>
                                    		</a:plotBands> -->
                            <a:labels rotation="-45" style="font-weight:normal;font-size:12px;font-family:Microsoft YaHei"/>
                        </a:xAxi>
                    </a:xAxis>
                    <a:yAxis>
                        <a:yAxi name="vendor_desc" bindTarget="jkpur5230_rfx_feedback_vendor_ds" gridLineWidth="1" minorGridLineWidth="0" tickLength="0">
                            <a:title style="font-size:12px;font-family:Microsoft YaHei" text="竞标价格"/>
                        </a:yAxi>
                    </a:yAxis>
                    <a:tooltip formatter="jkpur5230_toolFormat1" shared="true"/>
                    <a:seriesList name="feedback_retail_price">
                        <a:seriesItem>
                            <a:fillColor linearGradient="0,0,0,300" stops="[0,Aurora.Charts.getOptions().colors[0]],[1,Aurora.Charts.Color(Aurora.Charts.getOptions().colors[0]).setOpacity(0).get(&apos;rgba&apos;)]"/>
                        </a:seriesItem>
                    </a:seriesList>
                    <a:plotOptions>
                        <a:series connectNulls="true"/>
                    </a:plotOptions>
                </a:chart>
                <a:vBox>
                    <a:hBox>
                        <a:grid id="jkpur5230_rfx_feedback_rfa_record_grid" bindTarget="jkpur5230_rfx_feedback_rfa_record_ds" height="330" navBar="true">
                            <!-- <a:toolBar>
                                            <a:button click="jkpur5230_rfx_feedback_rfa_record_grid_refresh" icon="${/request/@context_path}/images/undo.png" text="SACPUR5210.REFRESH_QUOTATION"/>
                                        </a:toolBar> -->
                            <a:columns>
                                <a:column name="vendor_desc" align="left" prompt="JK_PUR_RFX_FEEDBACK_RECORDS.FEEDBACK_BY" renderer="jkpur5230_rfx_feedback_rfa_record_render" width="200"/>
                                <a:column name="feedback_retail_price" align="right" prompt="JK_PUR_RFX_FEEDBACK_RECORDS.FEEDBACK_RETAIL_PRICE" renderer="jkpur5230_rfx_feedback_rfa_record_render" width="80"/>
                                <a:column name="rk" align="center" prompt="JK_PUR_RFX_FEEDBACK_RECORDS.RANK" renderer="jkpur5230_rfx_feedback_rfa_record_render" width="80"/>
                                <a:column name="is_online_desc" align="center" prompt="JK_PUR_RFX_FEEDBACK_RECORDS.ONLINE_STATUS" width="70"/>
                                <a:column name="feedback_time" align="center" prompt="JK_PUR_RFX_FEEDBACK_RECORDS.FEEDBACK_TIME" width="150"/>
                            </a:columns>
                        </a:grid>
                    </a:hBox>
                </a:vBox>
            </a:hBox>
        </a:screenBody>
        <script><![CDATA[
        		jkpur5230_start_auto_refresh();
        ]]></script>
    </a:view>
    <a:view-config>
        <c:create-config targetId="jkpur5230_chart_placeHolder">
            <p:switch test="/model/header_record_rp/record/@auction_direction">
                <p:case value="FORWARD">
                    <c:process-config>
                        <a:title text="正向竞标走势图"/>
                    </c:process-config>
                </p:case>
                <p:case value="REVERSE">
                    <c:process-config>
                        <a:title text="反向竞标走势图"/>
                    </c:process-config>
                </p:case>
            </p:switch>
        </c:create-config>
    </a:view-config>
</a:screen>
