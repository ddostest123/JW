            //附件上传1 Block start
            function qms8010PcnApplyCreateAtmUpload() {
                var pcn_apply_header_id = $('qms8010_pcn_apply_create_header_ds').getCurrentRecord().get('pcn_apply_header_id');
                if (Aurora.isEmpty(pcn_apply_header_id)) {
                    Aurora.showMessage('${l:PROMPT}', '请先保存数据,再上传附件！');
                    return false;
                }else{
                    var url = "${/request/@context_path}/uploadFile.screen?table_name=QMS_PCN_APPLY_HEADERS&header_id=" + pcn_apply_header_id;
                    new Aurora.Window({
                        url: url,
                        title: '${l:ATM.UPLOAD_ATTACHMENT}',
                        id: 'qms8010_entrustment_edit_atm_window',
                        width: 650,
                        height: 400
                    }).on('beforeclose', function() {
                        $('qms8010_pcn_apply_create_atm_files_ds').query();
                    });
                }
            }
            
            function qms8010PcnApplyCreateAtmRender(value, rec, name) {
                if (name == 'file_download_link') {
                    var file_download_link = rec.get('file_download_link');
                    return file_download_link;
                }
            }//附件上传1 Block end
            
            //附件上传2 Block start
            function qms8010_pcn_apply_create_attachments_ds_beforeCreate(ds, record, index){
                var pcn_apply_header_id = $('qms8010_pcn_apply_create_header_ds').getCurrentRecord().get('pcn_apply_header_id');
                if (Aurora.isEmpty(pcn_apply_header_id)) {
                    Aurora.showMessage('${l:PROMPT}', '请先保存数据,再新附件行！');
                    return false;
                }
            }
            
            function qms8010_pcn_apply_create_attachments_ds_add(ds, record, index){
                var pcn_apply_header_id = $('qms8010_pcn_apply_create_header_ds').getCurrentRecord().get('pcn_apply_header_id');
                record.set('source_pk_value', pcn_apply_header_id);
            }
            
            function qms8010_upload_render(value, record, name) {
                var atm_line_id = record.get('atm_line_id');
                var atm_counts = record.get('atm_counts');
                var header_record =  $('qms8010_pcn_apply_create_header_ds').getCurrentRecord();
                if (name == "attachment") {
                    if (!record.isNew && atm_line_id && (header_record.get('pcn_apply_status_display')=='新建' ||　header_record.get('pcn_apply_status_display')==undefined ||　header_record.get('pcn_apply_status_display')=='已驳回')) {
                        return '<a href="javascript:qms8010_upload_file(' + atm_line_id + ')">${l:PROMPT.UPLOAD_DOWNLOAD}</a>';
                    }
                    else
                    {
                         return '<a href="javascript:qms8010_upload_file_readonly(' + atm_line_id + ')">附件查看</a>';
                    }
                }
            
                if (name == "atm_flag") {
                    if (atm_counts > 0) {
                        return '<div style="margin-top:3px;margin-left:auto;margin-right:auto;vertical-align:middle;height:20px;line-height:20px;"><img src="${/request/@context_path}/images/paperclip.png"/></div>';
                    } else {
                        return '';
                    }
                }
            }
            
            function qms8010_upload_file(id) {
                var url = "${/request/@context_path}/uploadFile.screen?table_name=QMS_PCN_APPLY_HEADERS&header_id=" + id;
                new Aurora.Window({
                    url: url,
                    title: '${l:ATM.UPLOAD_ATTACHMENT}',
                    id: 'qms8010_entrustment_edit_atm_window',
                    width: 850,
                    height: 500
                }).on('beforeclose', function() {
                    $('qms8010_pcn_apply_create_atm_files_ds').query();
                });
            }//附件上传2 Block end
            
           function qms8010_upload_file_readonly(id) {
                var url = "${/request/@context_path}/uploadFileView.screen?table_name=QMS_PCN_APPLY_HEADERS&header_id=" + id;
                new Aurora.Window({
                    url: url,
                    title: '附件查看',
                    id: 'qms8010_entrustment_edit_atm_window',
                    width: 850,
                    height: 500
                }).on('beforeclose', function() {
                    $('qms8010_pcn_apply_create_atm_files_ds').query();
                });
            }//附件上传2 Block end
            
            function qms8010_init()
            {
                var record = $('qms8010_pcn_apply_create_header_ds').getCurrentRecord();
                if(record.get('pcn_apply_status_display')!='新建' && record.get('pcn_apply_status_display')!=undefined && record.get('pcn_apply_status_display')!='已驳回')
                {
                    $('save').disable();
                    $('submit').disable();
                    record.getMeta().getField('date_of_filling').setReadOnly(true);
                    record.getMeta().getField('change_type_display').setReadOnly(true);
                    record.getMeta().getField('item_desc').setReadOnly(true);
                    record.getMeta().getField('sqe_contact_name').setReadOnly(true);
                    record.getMeta().getField('estimated_change_date').setReadOnly(true);
                    record.getMeta().getField('date_of_new_sample').setReadOnly(true);
                    record.getMeta().getField('last_date').setReadOnly(true);
                    record.getMeta().getField('final_shipment_date').setReadOnly(true);
                    record.getMeta().getField('change_content').setReadOnly(true);
                    record.getMeta().getField('modified_item_after_change').setReadOnly(true);
                    record.getMeta().getField('change_before').setReadOnly(true);
                    record.getMeta().getField('change_after').setReadOnly(true);
                    record.getMeta().getField('change_report').setReadOnly(true); 
                    $('add').disable();
                    $('delete').disable();
                }
            }
        ]]></script>
