                    },
                    failure: function() {
                        Aurora.Masker.unmask($('qms8020_pcn_reply_edit_window').wrap);
                        return;
                    },
                    error: function() {
                        Aurora.Masker.unmask($('qms8020_pcn_reply_edit_window').wrap);
                        return;
                    }
                });
            }
        ]]></script>
