                    <a:field name="hc_inventory_approach" readOnly="true" required="false" requiredMessage="请输入汇川库存处理方式"/>
                    <a:field name="vendor_feedback"/>
                </a:fields>
                <a:events>
                    <a:event name="load" handler="qms8020_pcn_reply_edit_line_ds_load"/>
                </a:events>
            </a:dataSet>
            <!-- <a:dataSet id="qms8020_pcn_reply_edit_atm_files_ds" autoCount="true" autoQuery="true" fetchAll="true" model="public.fnd_attachment_file" queryUrl="${/request/@context_path}/autocrud/public.fnd_attachment_file/query?table_name=QMS_PCN_APPLY_HEADERS&amp;pk_value=${/parameter/@pcn_apply_header_id}"/> -->
            <a:dataSet id="qms8020_pcn_reply_edit_atm_files_ds" autoQuery="true" bindTarget="qms8020_pcn_reply_edit_header_ds" model="public.fnd_atm_lines" queryUrl="${/request/@context_path}/autocrud/public.fnd_atm_lines/query?source_table_name=QMS_PCN_APPLY_HEADERS&amp;source_pk_value=${/parameter/@pcn_apply_header_id}">
                <a:fields>
                    <a:field name="atm_line_id"/>
                    <a:field name="line_number"/>
                    <a:field name="atm_desc"/>
                    <a:field name="source_table_name" defaultValue="QMS_PCN_APPLY_HEADERS"/>
                    <a:field name="source_pk_value" defaultValue="${/parameter/@pcn_apply_header_id}"/>
                </a:fields>
                <a:events>
                    <a:event name="beforecreate" handler="qms8020_pcn_reply_edit_attachments_ds_beforeCreate"/>
                    <a:event name="add" handler="qms8020_pcn_reply_edit_attachments_ds_add"/>
                </a:events>
            </a:dataSet>
        </a:dataSets>
        <a:screenBody>
            <a:fieldSet title="PCN确认">
                <a:screenTopToolbar>
                    <a:toolbarButton click="qms8020_pcnReplyEditOperationRecordFun" text="操作记录" width="100"/>
                    <!-- <a:toolbarButton click="qms8020_pcnReplyEditCloseWindowFun" text="PROMPT.RETURN" width="100"/> -->
                </a:screenTopToolbar>
                <a:fieldSet title="基本信息">
                    <a:vBox>
                        <a:hBox labelWidth="110">
                            <a:textField name="vendor_name" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="供应商名称"/>
                            <a:datePicker name="date_of_filling" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="填写日期" renderer="Aurora.formatDateTime"/>
                            <a:textField name="pcn_apply_status_display" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="PCN状态"/>
                            <a:comboBox name="change_type_display" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="变更类型"/>
                        </a:hBox>
                        <a:hBox labelWidth="110">
                            <a:lov name="item_desc" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="供应物料"/>
                            <a:textField name="item_specs" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="物料型号"/>
                            <a:lov name="sqe_contact_name" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="采购员"/>
                            <a:textField name="agree_changes_flag" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="是否同意进行变更"/>
                        </a:hBox>
                        <a:hBox labelWidth="110">
                            <a:textField name="need_item_assessment" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="是否进行物料评估"/>
                            <!-- <a:textField name="evaluation_results" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="评估结果"/> -->
                        </a:hBox>
                    </a:vBox>
                </a:fieldSet>
                <a:fieldSet title="PCN变更事务">
                    <a:vBox>
                        <a:hBox labelWidth="110">
                            <a:datePicker name="estimated_change_date" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="预计变更日期" renderer="Aurora.formatDateTime"/>
                            <a:datePicker name="date_of_new_sample" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="新样品提供日期" renderer="Aurora.formatDateTime"/>
                        </a:hBox>
                        <a:hBox labelWidth="110">
                            <a:datePicker name="last_date" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="最后接单日期" renderer="Aurora.formatDateTime"/>
                            <a:datePicker name="final_shipment_date" bindTarget="qms8020_pcn_reply_edit_header_ds" prompt="最后发货日期" renderer="Aurora.formatDateTime"/>
                        </a:hBox>
                        <a:fieldSet title="变更原因">
                            <a:textArea name="change_content" bindTarget="qms8020_pcn_reply_edit_header_ds" height="230" marginWidth="200"/>
                        </a:fieldSet>
                        <a:hBox labelWidth="150">
                            <a:textField name="modified_item_after_change" bindTarget="qms8020_pcn_reply_edit_header_ds" marginWidth="400" prompt="变更后受影响的物料清单"/>
                        </a:hBox>
                    </a:vBox>
                </a:fieldSet>
                <a:fieldSet>
                    <a:tabPanel id="qms8010_pcn_apply_create_tab" height="580" marginWidth="70">
                        <a:tabs>
                            <a:tab prompt="PCN变更内容前后对比" width="180">
                                <a:hBox marginWidth="100">
                                    <a:fieldSet title="变更前">
                                        <a:textArea name="change_before" bindTarget="qms8020_pcn_reply_edit_header_ds" height="230" marginWidth="800"/>
                                    </a:fieldSet>
                                    <a:fieldSet title="变更后">
                                        <a:textArea name="change_after" bindTarget="qms8020_pcn_reply_edit_header_ds" height="230" marginWidth="800"/>
                                    </a:fieldSet>
                                </a:hBox>
                                <a:fieldSet title="变更实验报告">
                                    <a:textArea name="change_report" bindTarget="qms8020_pcn_reply_edit_header_ds" height="230" marginWidth="100"/>
                                </a:fieldSet>
                            </a:tab>
                            <a:tab prompt="附件信息" width="100">
                                <a:grid id="qms8020_pcn_apply_create_atm_files_grid" bindTarget="qms8020_pcn_reply_edit_atm_files_ds" height="520" marginWidth="80" navBar="true">
                                    <a:columns>
                                        <a:column name="atm_desc" prompt="FND_ATM_LINES.ATM_DESC"/>
                                        <a:column name="upload_user_desc" prompt="上传人"/>
                                        <a:column name="upload_date" prompt="上传时间"/>
                                        <a:column name="atm_flag" align="center" renderer="qms8020_upload_render" width="20"/>
                                        <a:column name="attachment" align="center" prompt="PROMPT.ATTACHMENT_DOWNLOAD" renderer="qms8020_upload_render" width="100"/>
                                    </a:columns>
                                    <!--                 <a:editors>
                                        <a:textField id="qms8010_atch_desc_tf" maxLength="500"/>
                                    </a:editors> -->
                                </a:grid>
                            </a:tab>
                        </a:tabs>
                    </a:tabPanel>
                </a:fieldSet>
            </a:fieldSet>
        </a:screenBody>
        <script><![CDATA[
        	init(); 
        ]]></script>
    </a:view>
</a:screen>
