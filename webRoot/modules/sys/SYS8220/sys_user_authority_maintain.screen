            function initRowList_08(para) {
            
                //获取树形结果数据
                var records = $(grid_ds_08).getAll();
            
                //对数据进行遍历
                for (var i = 0;i < records.length;i++) {
            
                    //判断是否包含当前查询字符，包含则将其位置放入匹配结果数组
                    if (!Aurora.isEmpty(records[i].get(indexof_08_1))) {
