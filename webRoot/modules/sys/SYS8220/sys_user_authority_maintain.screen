                            show_index_06 = 0;
                        }
                    }
                }
            }
            
            //查询dataset更新事件
            
            function sys8220_fndUserAuthorityPurchaseMaterialQueryDsUpdate(ds, record, name, value, oldvalue) {
                $(query_ds_06).getCurrentRecord().dirty = false;
                if (name == query_name_06) {
                    //查询条件改变，重置全局定位变量
                    initGlobalPara_06();
                }
            }
            /**************************************************************
             * 定位查询开始结束，采购物料
             **************************************************************/
            
            
            
