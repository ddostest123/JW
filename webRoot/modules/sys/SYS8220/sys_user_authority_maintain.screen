                    if (!Aurora.isEmpty(records[i].get(indexof_08_2))) {
                        if (records[i].get(indexof_08_2).indexOf(para) != -1 || records[i].get(indexof_08_2).indexOf(para.toUpperCase()) != -1) {
                            row_list_08.push($(grid_ds_08).indexOf(records[i]));
                        }
