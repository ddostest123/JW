                document.getElementById('wfl1001_cloudAdmin_qV1').innerHTML = data.pur_amount;
                document.getElementById('wfl1001_cloudAdmin_qV2').innerHTML = data.asn_amount;
                document.getElementById('wfl1001_cloudAdmin_qV3').innerHTML = data.acp_amount;
                wfl1001_cloudAdmin_quickViewLine_set(pur_speed, 'wfl1001_cloudAdmin_qVline1', '${l:PROMPT.ORDER}');
                wfl1001_cloudAdmin_quickViewLine_set(asn_speed, 'wfl1001_cloudAdmin_qVline2', '${l:PROMPT.ASN}');
