            function wfl1001_set_wordLength(str, length) {
                var substr;
                var word_sum = 0;
                var len = 0;
                //判断中文字符所占的空间大小
                for (var i = 0;i < str.length;i++) {
                    //中文字符
                    if (str.charCodeAt(i) > 127 || str.charCodeAt(i) == 94) {
                        len += 2;
                        word_sum++;
                        //英文字符
                    } else {
                        len++;
                        word_sum++;
                    }
                    if (len == length) {
                        return str.substr(0, word_sum) + '...';
                    }
                }
                return str;
            }
            
            function wfl1001_cloudAdmin_BgUlDataLoad() {
                var ds = $('wfl1001_cloudAdmin_bgQUery_ds').getAll();
                var parent = document.getElementById('wfl1001_cloudAdmin_bgUl_id');
                var children = parent.children;
                for (var i = 0;i < children.length;i++) {
                    children[i].children[0].innerHTML = (ds.length > i) ? wfl1001_set_wordLength(ds[i].get('business_group_name'),20) : '';
                }
                for (i = 0;i < 8;i++) {
                    wfl1001_cloudAdmin_bgLogoInsert(ds[i].get('business_group_code'), ds[i].get('business_group_name'));
                }
            }
