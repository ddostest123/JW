            function wfl1001_cloud_admin_bgUl_click(index) {
                var bg_ds = $('wfl1001_cloudAdmin_bgQUery_ds').getAll();
                var bg_code = bg_ds[index].get('business_group_code');
                var parent = document.getElementById('wfl1001_cloudAdmin_bgUl_id');
                for (var i = 0;i < parent.children.length;i++) {
                    parent.children[i].children[1].children[0].style.display = 'none';
                }
                parent.children[index].children[1].children[0].style.display = 'block';
                $('wfl1001_cloudAdmin_quickView_ds').setQueryUrl('${/request/@context_path}/autocrud/wfl.WFL1001.wfl1001_cloud_admin_bgUlQuery/query?bussiness_group=' + bg_code);
                $('wfl1001_cloudAdmin_quickView_ds').query();
            }
            
            function wfl1001_cloudAdmin_quickViewLine_set(speed, id, type) {
                var value = document.getElementById(id);
                var text = value.nextSibling;
                if (speed == '无') {
                    value.style.width = '0';
                    text.innerHTML = type + '：上个月数量为0';
                } else if (speed < 0) {
                    value.style.width = speed * -1 + '%';
                    text.innerHTML = type + '：' + speed * -1 + '% less than last month';
                } else {
                    value.style.width = speed * 100 + '%';
                    text.innerHTML = type + '：' + speed * 1 + '% more than last month';
                }
            }
            
