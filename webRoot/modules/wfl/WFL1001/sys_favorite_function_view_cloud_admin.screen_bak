                                    <script><![CDATA[
                                        var image_index = -1;
                                        var all_child = jQuery(".foot .body_content.middle_size");
                                        var all_image = jQuery(".show_list").children('img');
                                        var setinter;
                                        var setinter_inter;
                                        init();
                                        
                                        function init() {
                                            //初始化图片
                                            all_child.eq(0).css({
                                                'background-color': '#fff',
                                                'color': '#969696'
                                            });
                                            all_image.eq(0).css('left', '0');
                                            image_index = 0;
                                            setinter_time();
                                            //绑定左右按钮
                                            jQuery(".show_list .angle_right").click(function() {
                                                clearInter();
                                                show_right();
                                                setInter();
                                            });
                                            jQuery(".show_list .angle_left").click(function() {
                                                clearInter();
                                                show_left();
                                                setInter();
                                            });
                                            //绑定地端的轮播效果
                                            all_child.each(function() {
                                                var _this = jQuery(this);
                                                _this.click(function() {
                                                    clearInter();
                                                    var index = all_child.index(jQuery(this));
                                                    if (index >= image_index) {
                                                        show_image_right(index);
                                                    } else if (index < image_index) {
                                                        show_image_left(index);
                                                    }
                                                    setInter();
                                                });
                                            });
                                            setInter();
                                        }
                                        
                                        function setInter() {
                                            setinter = setInterval(function() {
                                                show_right();
                                                setinter_time();
                                            }, 5000);
                                        }
                                        
                                        function clearInter() {
                                            clearInterval(setinter);
                                        }
                                        
                                        function show_left() {
                                            var index = (image_index == 0) ? 5 : image_index - 1;
                                            show_image_left(index);
                                        }
                                        
                                        function show_right() {
                                            var index = (image_index == 5) ? 0 : image_index + 1;
                                            show_image_right(index);
                                        }
                                        
                                        function show_image_left(index) {
                                            var remove_foot = all_child.eq(image_index);
                                            var remove_image = all_image.eq(image_index);
                                            var _this_image = all_image.eq(index);
                                            var _this_foot = all_child.eq(index);
                                            //原来的图片右移
                                            remove_image.animate({
                                                'left': '100%'
                                            }, {
                                                queue: false,
                                                duration: 500
                                            });
                                            remove_foot.css({
                                                'background-color': '#0b927b',
                                                'color': '#fff'
                                            });
                                            all_child.eq(image_index).children('div').css('width', '0');
                                            //要显示的图片右移
                                            _this_image.css('left', '-100%');
                                            _this_image.animate({
                                                'left': '0'
                                            }, {
                                                queue: false,
                                                duration: 500
                                            });
                                            _this_foot.css({
                                                'background-color': '#fff',
                                                'color': '#969696'
                                            });
                                            image_index = index;
                                            all_child.eq(image_index).children('div').css('width', '0');
                                            foot_hover();
                                            setinter_time();
                                        }
                                        
                                        function show_image_right(index) {
                                            var remove_foot = all_child.eq(image_index);
                                            var remove_image = all_image.eq(image_index);
                                            var _this_image = all_image.eq(index);
                                            var _this_foot = all_child.eq(index);
                                            //原来的图片左移
                                            remove_image.animate({
                                                'left': '-100%'
                                            }, {
                                                queue: false,
                                                duration: 500
                                            });
                                            remove_foot.css({
                                                'background-color': '#0b927b',
                                                'color': '#fff'
                                            });
                                            all_child.eq(image_index).children('div').css('width', '0');
                                            //要显示的图片左移
                                            _this_image.css('left', '100%');
                                            _this_image.animate({
                                                'left': '0'
                                            }, {
                                                queue: false,
                                                duration: 500
                                            });
                                            _this_foot.css({
                                                'background-color': '#fff',
                                                'color': '#969696'
                                            });
                                            image_index = index;
                                            all_child.eq(image_index).children('div').css('width', '0');
                                            foot_hover();
                                            setinter_time();
                                        }
                                        
                                        function foot_hover() {
                                            all_child.unbind('hover');
                                            all_child.not(all_child.eq(image_index)).each(function() {
                                                var _this = jQuery(this);
                                                _this.hover(function() {
                                                    jQuery(this).css({
                                                        'background-color': '#fff',
                                                        'color': '#969696'
                                                    });
                                                }, function() {
                                                    jQuery(this).css({
                                                        'background-color': '#0b927b',
                                                        'color': '#fff'
                                                    });
                                                });
                                            });
                                        }
                                        
                                        function setinter_time(){
                                            clearInterval(setinter_inter);
                                            all_child.eq(image_index).css('background-color', '#0b927b');
                                            all_child.eq(image_index).children('div').css('width', '2%');
                                            setinter_inter = setInterval(function() {
                                                var _this = all_child.eq(image_index).children('div');
                                                var width = _this.width();
                                                var percent = width / all_child.eq(image_index).innerWidth() * 100;
                                                _this.css('width', percent + 2 + '%');
                                            }, 100);
                                        }
                                    ]]></script>
