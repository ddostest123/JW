        <script><![CDATA[
                var wfl1001_enterFlag = true;
                jQuery('.customer_ele').find('span').each(function() {
                    var _this = jQuery(this);
                    var height = _this.height();
                    var length = _this.html().length;
                    height = length * 24;
                    var tota_height = 270;
                    var top = (tota_height - height) / 2;
                    _this.css('top', top);
                });
                wfl1001_customerInit();
            
                function wfl1001_customerInit() {
                    jQuery('.lb_customer_wrap').children().eq(0).find('div').css({
                        'top': 0,
                        'left': 0
                    });
                    jQuery('.lb_customer_wrap').children().eq(0).find('span').css({
                        'color': '#fff'
                    });
                    jQuery('.lb_customer_wrap').children().eq(0).find('img').css({
                        'left': jQuery('.lb_customer_wrap').children().eq(0).find('img').attr('data_hover') + 'px'
                    });
                    jQuery('#wfl1001_customer_foot1').children().eq(1).css({
                        'background-color': '#fff',
                        'color': '#969696'
                    });
                }
            
                function wfl1001_customerOutOfInit() {
                    jQuery('.lb_customer_wrap').children().eq(0).find('div').css({
                        'top': 0,
                        'left': -100 + '%'
                    });
                    jQuery('.lb_customer_wrap').children().eq(0).find('span').css({
                        'color': '#484b50'
                    });
                    jQuery('.lb_customer_wrap').children().eq(0).find('img').css({
                        'left': jQuery('.lb_customer_wrap').children().eq(0).find('img').attr('data_normal') + 'px'
                    });
                    jQuery('#wfl1001_customer_foot1').children().eq(1).css({
                        'background-color': '#0b927b',
                        'color': '#fff'
                    });
                }
                jQuery('.customer_ele').bind("mouseenter mouseleave", function(e) {
                    var w = jQuery(this).width();
                    var h = jQuery(this).height();
                    var x = (e.pageX - jQuery(this).offset().left - (w / 2)) * (w > h ? (h / w) : 1);
                    var y = (e.pageY - jQuery(this).offset().top - (h / 2)) * (h > w ? (w / h) : 1);
                    var direction = Math.round((((Math.atan2(y, x) * (180 / Math.PI)) + 180) / 90) + 3) % 4;
                    var img_left;
                    var index = jQuery(this).index();
                    var foot = document.getElementById('wfl1001_customer_foot1');
                    var children = foot.children[index + 1];
                    if (e.type == 'mouseenter') {
                        wfl1001_customerOutOfInit();
                        children.style.backgroundColor = '#fff';
                        children.style.color = '#969696';
                        // 0:up - 1:right - 2:down - 3:left
                        if (direction == 0) {
                            jQuery(this).find('div').css({
                                'top': '-100%',
                                'left': '0px'
                            });
                            jQuery(this).find('div').animate({
                                'top': 0
                            }, {
                                queue: false,
                                duration: 300
                            });
                        } else if (direction == 2) {
                            jQuery(this).find('div').css({
                                'top': '100%',
                                'left': '0px'
                            });
                            jQuery(this).find('div').animate({
                                'top': 0
                            }, {
                                queue: false,
                                duration: 300
                            });
                        } else if (direction == 1) {
                            jQuery(this).find('div').css({
                                'top': '0px',
                                'left': '100%'
                            });
                            jQuery(this).find('div').animate({
                                'left': 0
                            }, {
                                queue: false,
                                duration: 300
                            });
                        } else if (direction == 3) {
                            jQuery(this).find('div').css({
                                'top': '0px',
                                'left': '-100%'
                            });
                            jQuery(this).find('div').animate({
                                'left': 0
                            }, {
                                queue: false,
                                duration: 300
                            });
                        }
                        jQuery(this).find('span').css('color', '#fff');
                        img_left = jQuery(this).find('img').position().left;
                        jQuery(this).find('img').animate({
                            'left': jQuery(this).find('img').attr('data_hover') + 'px'
                        }, {
                            queue: false,
                            duration: 200
                        });
                    } else if (e.type == 'mouseleave') {
                        children.style.backgroundColor = '#0b927b';
                        children.style.color = '#fff';
                        if (direction == 0 && jQuery(this).index() != 0) {
                            jQuery(this).find('div').animate({
                                'top': -100 + '%'
                            }, {
                                queue: false,
                                duration: 300
                            });
                        } else if (direction == 2 && jQuery(this).index() != 0) {
                            jQuery(this).find('div').animate({
                                'top': 100 + '%'
                            }, {
                                queue: false,
                                duration: 300
                            });
                        } else if (direction == 1) {
                            jQuery(this).find('div').animate({
                                'left': 100 + '%'
                            }, {
                                queue: false,
                                duration: 300
                            });
                        } else if (direction == 3 && jQuery(this).index() != 0) {
                            jQuery(this).find('div').animate({
                                'left': -100 + '%'
                            }, {
                                queue: false,
                                duration: 300
                            });
                        }
                        if (direction == 1 || jQuery(this).index() != 0) {
                            jQuery(this).find('span').css('color', '#484b50');
                            jQuery(this).find('img').animate({
                                'left': jQuery(this).find('img').attr('data_normal') + 'px'
                            }, {
                                queue: false,
                                duration: 200
                            });
                        }
                        wfl1001_customerInit();
                    }
                });
        ]]></script>
