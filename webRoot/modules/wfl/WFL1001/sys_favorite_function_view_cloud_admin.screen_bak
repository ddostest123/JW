            function wfl1001_cloudAdmin_bgLogoInsert(bg_code, bg_name) {
                var parent = document.getElementById('wfl1001_cloudAdmin_bgLogo');
                var color = wfl1001_cloudAdmin_bgLogoColor[wfl1001_cloudAdmin_bgLogoIndex];
                wfl1001_cloudAdmin_bgLogoIndex = (wfl1001_cloudAdmin_bgLogoIndex == wfl1001_cloudAdmin_bgLogoColor.length - 1) ? 0 : wfl1001_cloudAdmin_bgLogoIndex + 1;
                var div = document.createElement("div");
                div.className = 'price';
                var html = '<div class="price_table ' + color + '">';
                html += '<div class="head"><div class="h_title">' + bg_code + '</div><div class="h_content"><span>Logo</span></div></div>';
                html += '<ul class="content"><li>' + bg_name + '</li></ul>';
                html += '<div class="foot"><div></div></div>';
                html += '</div>';
                div.innerHTML = html;
                parent.appendChild(div);
            }
            
