            }
            
            function wfl1001_cloudAdmin_cloudVSwitch(index) {
                var parent = document.getElementById('wfl1001_cloudAdmin_CloudVTa');
                var children = parent.children;
                var click_parent = document.getElementById('wfl1001_cloudAdmin_CloudVSelect');
                var click_children = click_parent.children;
                var n = index + 1;
                for (var i = 0;i < children.length;i++) {
                    children[i].style.display = 'none';
                    click_children[i].style.backgroundColor = '#ededed';
                    click_children[i].style.color = '#969696';
                }
                children[index].style.display = 'block';
                click_children[index].style.backgroundColor = '#0ca389';
                click_children[index].style.color = '#fff';
                jQuery('#wfl1001_cloudAdmin_CloudVSelect').children().hover(function() {
                    jQuery(this).css("background-color", "#0ca389");
                    jQuery(this).css("color", "#fff");
                }, function() {
                    jQuery(this).css("background-color", "#ededed");
                    jQuery(this).css("color", "#969696");
                });
                jQuery('#wfl1001_cloudAdmin_CloudVSelect div:nth-child(' + n + ')').unbind('mouseenter').unbind('mouseleave');
            }
            
