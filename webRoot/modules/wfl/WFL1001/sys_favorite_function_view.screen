            //tab页面的设计对象 以及 函数
            var tabObj = {
                beforeDisplayTgr: null,
                CurrentDisplayTgr: null,
                titleTgr: null
            };
            
            function initTab() {
                tabObj.beforeDisplayTgr = window.document.getElementById("tab-son5");
                tabObj.CurrentDisplayTgr = window.document.getElementById("tab-son5");
                tabObj.titleTgr = window.document.getElementById("tab-son1-title");
                
                new initTypeTab("tab_con1");
            }
            
			function initTypeTab(con){
				var _con = (typeof con == "string" ? document.getElementById(con) : con);
				_con.activeIndex = 0;
				var _tabTitle = _con.children[0];
				var _tabContent = _con.children[1];
				_init();
				
				function _init(){
					var children = _tabContent.children;
					var l = children.length;
					for (var i = 1; i < l; i ++){
						children[i].style.display = "none";
					}
				}
				_tabTitle.onclick = function (e){
					var event = e || window.event;
					var target = event.target || event.srcElement;
					while(target.className.indexOf('tab-title') == -1 && target.tagName.toUpperCase() != "BODY"){
						target = target.parentNode;
					}
					var index = target.getAttribute("index");
					var activeIndex = _con.activeIndex;
					
					if (index && index != activeIndex){
						_tabContent.children[activeIndex].style.display = "none";
						_tabContent.children[index].style.display = "block";
						
						_tabTitle.children[activeIndex].className = "tab-title-item";
						_tabTitle.children[index].className = "active tab-title-item";
						
						_con.activeIndex = index;
					}
				}
			}
			
            
            function changeTabSon(e) {
                var tgr = e.target || e.srcElement;
                if (tgr.tagName == 'LI') {
                    var id = tgr.getAttribute('contentId');
                    tabObj.tabCurrentDisplayTgr = window.document.getElementById(id);
                    if (tabObj.beforeDisplayTgr == tabObj.tabCurrentDisplayTgr) {
                        return;
                    }
                    tabObj.beforeDisplayTgr.style.display = 'none';
                    tabObj.tabCurrentDisplayTgr.style.display = 'block';
                    tabObj.tabCurrentDisplayTgr.style.marginTop = '18';
                    tabObj.beforeDisplayTgr = tabObj.tabCurrentDisplayTgr;
            
                    //改变标题的边框
                    tabObj.titleTgr.className = '';
                    tgr.className = 'active-pcd-tab-item';
                    tabObj.titleTgr = tgr;
                }
            }
            
            function colorPicker(s) {
                var r;
                switch (s) {
                case 'PRIMARY':
                    return '#3498db';
                case 'RED':
                    return '#f56954';
                case 'INFO':
                    return '#00afd1';
                case 'PURPLE':
                    return '#9b59b6';
                case 'DANGER':
                    return '#f6504d';
                case 'SUCCESS':
                    return '#27ae60';
                case 'WARNING':
                    return '#ffc61d';
                case 'GREEN':
                    return '#01b6ad';
                case 'PINK':
                    return '#ff3366';
                case 'INVERSE':
                    return '#2d2d2d';
                }
                return '#00ffff';
            }
            
            function userEventInsert(o) {
