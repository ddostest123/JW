                            'start_month': o.start.getMonth(),
                            'start_day': o.start.getDate(),
                            'start_hour': o.start.getHours(),
                            'start_minute': o.start.getMinutes()
                        },
                        success: function(result) {
                        },
                        scope: this
                    });
                }
            }
            
            function userEventDelete(id) {
