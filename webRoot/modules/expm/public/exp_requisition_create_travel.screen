                if (!$('exp_requisition_line_ds').validate()) {
                    return;
                }
                if (!$('exp_requisition_travel_ds').validate()) {
                    return;
                }
                if (!$('exp_report_head_dimension').validate()) {
                    return;
                }
                if (!$('exp_report_head_object').validate()) {
                    return;
                }
                if ($('exp_requisition_travel_ds').getAll().length == 0) {
                    Aurora.showInfoMessage('${l:PROMPT}', '${l:PLEASE_INSERT_LINE_DATA}', null, 250, 100);
                    return;
                }
                if (saveFlag == false) {
                    $('exp_requisition_head_ds').submit();
                    saveFlag = true;
                    $('submitBtn').enable();
                    if (typeof(nextStep) == 'function') {
                        nextStep();
                    }
                } else {
                    $('exp_requisition_head_ds').setSubmitUrl('${/request/@context_path}/modules/expm/public/exp_requisition_travel_update_save.svc');
                    $('exp_requisition_head_ds').submit();
                    saveFlag = true;
                    $('submitBtn').enable();
                    if (typeof(nextStep) == 'function') {
                        nextStep();
                    }
                }
            }
            
