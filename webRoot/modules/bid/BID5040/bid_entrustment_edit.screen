        <script><![CDATA[
            var bid5040_tax_type_id = null;
            var bid5040_currency_code_change_flag = false;
            var bid5040_currency_name_change_flag = false;
            //自动带出评分要素
            
            function bid5040() {
                var entrustment_header_id = $('bid5040_edit_entrustment_header_ds').getAt(0).get('entrustment_header_id');
            }
            
            
            function bid5040EntrustmentEditOnLoad(ds) {
                var record = ds.getCurrentRecord();
                var company_id = record.get('company_id');
                var using_company_id = record.get('using_company_id');
                var entrustment_type_id = record.get('entrustment_type_id');
                var functional_currency_code = record.get('functional_currency_code');
                var currency_code = record.get('currency_code');
                var suggested_bidding_method = record.get('suggested_bidding_method');
                var offline_agent_tendering_flag = record.get('offline_agent_tendering_flag');
                bid5040_tax_type_id = record.get('tax_type_id');
            
                if (!Aurora.isEmpty(entrustment_type_id)) {
                    record.getField('tendering_tmpl_desc').setLovPara('entrustment_type_id', entrustment_type_id);
                }
                // if (functional_currency_code != currency_code) {
                // record.getMeta().getField('exchange_rate').setReadOnly(false);
                // record.getMeta().getField('exchange_rate').setRequired(true);
                // } else {
                // record.getMeta().getField('exchange_rate').setReadOnly(true);
                // record.getMeta().getField('exchange_rate').setRequired(false);
                // }
                if (suggested_bidding_method == 'OPEN') {
                    $('bid5040_edit_entrustment_tab').setDisabled(2);
                } else {
                    $('bid5040_edit_entrustment_tab').setEnabled(2);
                }
            
                if (offline_agent_tendering_flag == 'Y') {
                    $('bid5040EntrustmentEditSubmitBtn').disable();
                }
            
                //判断税
                // var tax_included_flag = record.get('tax_included_flag');
                // if (tax_included_flag == 'Y') {
                // record.getField('tax_type_rate').setReadOnly(false);
                // record.getField('tax_type_rate').setRequired(true);
                // }
            
                if (suggested_bidding_method == 'AGENT') {
                    record.getField('agent_company_name').setReadOnly(false);
                    record.getField('agent_company_name').setRequired(true);
                    record.getField('agent_company_tel').setReadOnly(false);
                    record.getField('agent_company_address').setReadOnly(false);
                } else {
                    record.getField('agent_company_name').setRequired(false);
                    record.getField('agent_company_name').setReadOnly(true);
                    record.getField('agent_company_tel').setReadOnly(true);
                    record.getField('agent_company_address').setReadOnly(true);
                }
            
                var bidding_file_free_flag = record.get('bidding_file_free_flag');
                if (bidding_file_free_flag == 'Y') {
                    record.getMeta().getField('bid_file_amount').setReadOnly(true);
                    record.getMeta().getField('bid_file_amount').setRequired(false);
                } else {
                    record.getMeta().getField('bid_file_amount').setReadOnly(false);
                    record.getMeta().getField('bid_file_amount').setRequired(true);
                }
            
            
                var margin_required_flag = record.get('margin_required_flag');
                if (margin_required_flag == 'Y') {
                    record.getMeta().getField('deposit_amount').setReadOnly(false);
                    record.getMeta().getField('deposit_amount').setRequired(true);
                } else {
                    record.getMeta().getField('deposit_amount').setReadOnly(true);
                    record.getMeta().getField('deposit_amount').setRequired(false);
                }
            
                var prequal_flag = record.get('prequal_flag');
                if (prequal_flag == 'Y') {
                    document.getElementById('bid5040_prequal_div').style.display = "block";
                    document.getElementById('bid5040_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.CANCEL_PREQUAL}';
                } else {
                    document.getElementById('bid5040_prequal_div').style.display = "none";
                    document.getElementById('bid5040_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.ADD_PREQUAL}';
                }
            
                if (using_company_id) {
                    record.getMeta().getField('inv_organization_desc').setReadOnly(false);
                    record.getField('inv_organization_desc').setLovPara('company_id', using_company_id);
                }
            
                var score_mode = record.get('score_mode');
                if (score_mode == 'TECH_BIZ') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'TECHNOLOGY');
                    record.set('team_2', 'BUSINESS');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    document.getElementById('bid5040_expertScore_div').style.display = "block";
                    document.getElementById('bid5040_expertScore_no_div').style.display = "none";
            
                } else if (score_mode == 'BIZ_TECH') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid5040_expertScore_div').style.display = "block";
                    document.getElementById('bid5040_expertScore_no_div').style.display = "none";
            
                } else if (score_mode == 'SAME_TIME') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '1');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid5040_expertScore_div').style.display = "block";
                    document.getElementById('bid5040_expertScore_no_div').style.display = "none";
            
                } else if (score_mode == 'NONDISTINCTION') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'NONDISTINCTION');
                    record.set('team_1_desc', '');
                    record.set('team_2_desc', '');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '');
                    document.getElementById('bid5040_expertScore_div').style.display = "none";
                    document.getElementById('bid5040_expertScore_no_div').style.display = "block";
            
                } else {
                    record.getField('score_mode_desc').setRequired(false);
                }
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    //专家与评分要素查询
                    $('bid5040_ln_expert_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5040_ln_expert_1_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5040_ln_expert_1_ds').query();
                    $('bid5040_ln_expert_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid5040_ln_expert_2_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid5040_ln_expert_2_ds').query();
                    $('bid5040_scoring_elements_1_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5040_scoring_elements_1_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5040_scoring_elements_1_assign_ds').query();
                    $('bid5040_scoring_elements_2_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid5040_scoring_elements_2_assign_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid5040_scoring_elements_2_assign_ds').query();
                } else if (score_mode == 'NONDISTINCTION') {
                    //专家与评分要素查询
                    $('bid5040_ln_expert_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5040_ln_expert_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5040_ln_expert_ds').query();
                    $('bid5040_scoring_elements_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5040_scoring_elements_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5040_scoring_elements_assign_ds').query();
                }
            
                var expert_score_flag = record.get('expert_score_flag');
                if (expert_score_flag == 'Y') {
                    document.getElementById('bid5040_expertScore').innerText = '${l:BID_ENTRUSTMENT_HEADERS.NO_EXPERT_SCORE}';
                } else {
                    document.getElementById('bid5040_expertScore').innerText = '${l:BID_ENTRUSTMENT_HEADERS.EXPERT_SCORE}';
            
                }
                if (record.get('payment_terms')) {
                    document.getElementById("contain").innerHTML = record.get('payment_terms');
                }
            
                if (record.get('is_project_flag') == 'Y') {
                    $('bid5040_edit_entrustment_tab').setDisabled(0);
                } else {
                    $('bid5040_edit_entrustment_tab').setDisabled(1);
                }
            }
            
            function bid5040EntrustmentEditOnUpdate(ds, record, name, value, oldvalue) {
            
                if (name == "using_company_id") {
                    bid5040_currency_code_change_flag = true;
                    bid5040_currency_name_change_flag = true;
                    record.set('exchange_rate', 1);
            
                    record.set('inv_organization_id', '');
                    record.set('inv_organization_desc', '');
                    if (value) {
                        record.getMeta().getField('inv_organization_desc').setReadOnly(false);
                        record.getField('inv_organization_desc').setLovPara('company_id', value);
                    } else {
                        record.getMeta().getField('inv_organization_desc').setReadOnly(true);
                        record.getField('inv_organization_desc').setLovPara('company_id', -1);
                    }
            
                    //Clear Item Line - Org
                    var records = $('bid5040_edit_entrustment_items_ds').getAll();
                    for (var i = 0;i < records.length;i++) {
                        records[i].set('inv_organization_id', '');
                        records[i].set('inv_organization_desc', '');
                    }
                } else if (name == 'payment_terms') {
                    if (value) {
                        document.getElementById("contain").innerHTML = value;
                    }
                } else if (name == "entrustment_type_id") {
                    record.set('tendering_tmpl_id', '');
                    record.set('tendering_tmpl_desc', '');
                    record.getField('tendering_tmpl_desc').setLovPara('entrustment_type_id', value);
                } else if (name == "functional_currency_code") {
                    if (bid5040_currency_code_change_flag == true) {
                        record.set('currency_code', value);
            
                        bid5040_currency_code_change_flag = false;
                    }
                } else if (name == "functional_currency_name") {
                    if (bid5040_currency_name_change_flag == true) {
                        record.set('currency_name', value);
            
                        bid5040_currency_name_change_flag = false;
                    }
                } else if (name == "currency_code") {
                    var functional_currency_code = record.get('functional_currency_code');
                    if (functional_currency_code != value) {
                        //record.getMeta().getField('exchange_rate').setReadOnly(false);
                        //record.getMeta().getField('exchange_rate').setRequired(true);
                        record.set('exchange_rate', '');
                    } else {
                        //record.getMeta().getField('exchange_rate').setReadOnly(true);
                        //record.getMeta().getField('exchange_rate').setRequired(false);
                        record.set('exchange_rate', 1);
                    }
                } else if (name == 'suggested_bidding_method') {
                    if (value == 'OPEN') {
                        $('bid5040_edit_entrustment_tab').setDisabled(2);
                    } else {
                        $('bid5040_edit_entrustment_tab').setEnabled(2);
                    }
            
                    record.set('agent_company_id', '');
                    record.set('agent_company_name', '');
                    record.set('agent_company_tel');
                    record.set('agent_company_address');
            
                    if (value == 'AGENT') {
                        record.getField('agent_company_name').setReadOnly(false);
                        record.getField('agent_company_name').setRequired(true);
                        record.getField('agent_company_tel').setReadOnly(false);
                        record.getField('agent_company_address').setReadOnly(false);
                    } else {
                        record.getField('agent_company_name').setRequired(false);
                        record.getField('agent_company_name').setReadOnly(true);
                        record.getField('agent_company_tel').setReadOnly(true);
                        record.getField('agent_company_address').setReadOnly(true);
                    }
                } else if (name == 'tax_included_flag') {
                    record.set('tax_type_id', '');
                    record.set('tax_type_code', '');
                    record.set('tax_type_rate', '');
                    // if (value == 'Y') {
                    // record.getField('tax_type_rate').setReadOnly(false);
                    // record.getField('tax_type_rate').setRequired(true);
                    // } else {
                    // record.getField('tax_type_rate').setRequired(false);
                    // record.getField('tax_type_rate').setReadOnly(true);
                    // }
                } else if (name == 'bidding_file_free_flag') {
                    record.set('bid_file_amount', '');
                    if (value == 'Y') {
                        record.getMeta().getField('bid_file_amount').setReadOnly(true);
                        record.getMeta().getField('bid_file_amount').setRequired(false);
                    } else {
                        record.getMeta().getField('bid_file_amount').setReadOnly(false);
                        record.getMeta().getField('bid_file_amount').setRequired(true);
                    }
                } else if (name == 'margin_required_flag') {
                    record.set('deposit_amount', '');
                    if (value == 'Y') {
                        record.getMeta().getField('deposit_amount').setReadOnly(false);
                        record.getMeta().getField('deposit_amount').setRequired(true);
                    } else {
                        record.getMeta().getField('deposit_amount').setReadOnly(true);
                        record.getMeta().getField('deposit_amount').setRequired(false);
                    }
                } else if (name == 'score_mode') {
                    var entrustment_header_id = $('bid5040_edit_entrustment_header_ds').getAt(0).get('entrustment_header_id');
                    var sequence_num = $('bid5040_edit_entrustment_header_ds').getAt(0).get('sequence_num_1');
                    var team = $('bid5040_edit_entrustment_header_ds').getAt(0).get('team_1');
                    var entrustment_head_id = $('bid5040_edit_entrustment_header_ds').getCurrentRecord().get('entrustment_type_id');
                    $('bid5040_scoring_elements_ds').setQueryUrl("${/request/@context_path}/autocrud/bid.BID5040.bid5040_scoring_elements/query?entrustment_head_id=" + entrustment_head_id);
                    $('bid5040_scoring_elements_ds').query();
                    var elements_ds = $('bid5040_scoring_elements_ds').getAll();
                    var scoring_elements1_ds = $('bid5040_scoring_elements_1_assign_ds');
                    var scoring_elements2_ds = $('bid5040_scoring_elements_2_assign_ds');
                    var scoring_elements3_ds = $('bid5040_scoring_elements_assign_ds');
                    var elements1_ds = scoring_elements1_ds.getAll();
                    var elements2_ds = scoring_elements2_ds.getAll();
                    var elements3_ds = scoring_elements3_ds.getAll();
                    if (elements1_ds.length == 0) {
                        for (var t = 0;t < elements_ds.length;t++) {
                            scoring_elements1_ds.create({
                                'entrustment_header_id': entrustment_header_id,
                                'team': team,
                                'sequence_num': sequence_num,
                                'scoring_elements_code': elements_ds[t].get('scoring_elements_code'),
                                'scoring_rules': elements_ds[t].get('scoring_detail'),
                                'scoring_elements_id': elements_ds[t].get('scoring_elements_id'),
                                'scoring_elements_desc': elements_ds[t].get('scoring_elements_desc'),
                                'highest_scores': elements_ds[t].get('default_highest_score')
            
                            });
                        }
                    }
                    if (elements2_ds.length == 0) {
                        for (var m = 0;m < elements_ds.length;m++) {
                            scoring_elements2_ds.create({
                                'entrustment_header_id': entrustment_header_id,
                                'team': team,
                                'sequence_num': sequence_num,
                                'scoring_elements_code': elements_ds[m].get('scoring_elements_code'),
                                'scoring_rules': elements_ds[m].get('scoring_detail'),
                                'scoring_elements_id': elements_ds[m].get('scoring_elements_id'),
                                'scoring_elements_desc': elements_ds[m].get('scoring_elements_desc'),
                                'highest_scores': elements_ds[m].get('default_highest_score')
            
                            });
                        }
                    }
                    if (elements3_ds.length == 0) {
                        for (var n = 0;n < elements_ds.length;n++) {
                            scoring_elements3_ds.create({
                                'entrustment_header_id': entrustment_header_id,
                                'team': team,
                                'sequence_num': sequence_num,
                                'scoring_elements_code': elements_ds[n].get('scoring_elements_code'),
                                'scoring_rules': elements_ds[n].get('scoring_detail'),
                                'scoring_elements_id': elements_ds[n].get('scoring_elements_id'),
                                'scoring_elements_desc': elements_ds[n].get('scoring_elements_desc'),
                                'highest_scores': elements_ds[n].get('default_highest_score')
            
                            });
                        }
                    }
            
                    if (value == 'TECH_BIZ') {
                        
                        record.set('team_1', 'TECHNOLOGY');
                        record.set('team_2', 'BUSINESS');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '2');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        document.getElementById('bid5040_expertScore_div').style.display = "block";
                        document.getElementById('bid5040_expertScore_no_div').style.display = "none";
            
                    } else if (value == 'BIZ_TECH') {
                        record.set('team_1', 'BUSINESS');
                        record.set('team_2', 'TECHNOLOGY');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '2');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        document.getElementById('bid5040_expertScore_div').style.display = "block";
                        document.getElementById('bid5040_expertScore_no_div').style.display = "none";
                    } else if (value == 'SAME_TIME') {
                        record.set('team_1', 'BUSINESS');
                        record.set('team_2', 'TECHNOLOGY');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '1');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        document.getElementById('bid5040_expertScore_div').style.display = "block";
                        document.getElementById('bid5040_expertScore_no_div').style.display = "none";
                    } else if (value == 'NONDISTINCTION') {
                        record.set('team_1', 'NONDISTINCTION');
                        record.set('team_2', '');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '');
                        record.set('team_1_desc', '');
                        record.set('team_2_desc', '');
                        document.getElementById('bid5040_expertScore_div').style.display = "none";
                        document.getElementById('bid5040_expertScore_no_div').style.display = "block";
                    }
            
                    if (value == 'TECH_BIZ' || value == 'BIZ_TECH' || value == 'SAME_TIME') {
                        //专家与评分要素查询
                        $('bid5040_ln_expert_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5040_ln_expert_1_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid5040_ln_expert_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                        $('bid5040_ln_expert_2_ds').setQueryParameter('team', record.get('team_2'));
                        $('bid5040_scoring_elements_1_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5040_scoring_elements_1_assign_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid5040_scoring_elements_2_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                        $('bid5040_scoring_elements_2_assign_ds').setQueryParameter('team', record.get('team_2'));
                    } else if (value == 'NONDISTINCTION') {
                        //专家与评分要素查询
                        $('bid5040_ln_expert_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5040_ln_expert_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid5040_scoring_elements_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5040_scoring_elements_assign_ds').setQueryParameter('team', record.get('team_1'));
                    }
            
                    bid5040_ExpertScoreTeamChange(record.get('team_1'), record.get('sequence_num_1'), record.get('team_2'), record.get('sequence_num_2'));
                }else if (name == 'bid_closing_date'){
                    if(value){
                        record.set('bid_opening_start_date',value);
                    }
                }
            }
            
            
            function bid5040EntrustmentEditItemsOnAdd(ds, rec, index) {
                var header_ds = $('bid5040_edit_entrustment_header_ds');
                var header_rec = header_ds.getCurrentRecord();
                var using_company_id = header_rec.get('using_company_id');
            
                if (Aurora.isEmpty(using_company_id)) {
                    Aurora.showMessage('${l:PROMPT}', '${l:BID_ENTRUSTMENT_MESSAGE.TENDER_CO_MATERIAL}');
                    return false;
                }
            
                rec.set('need_by_date', header_rec.get('complete_date'));
