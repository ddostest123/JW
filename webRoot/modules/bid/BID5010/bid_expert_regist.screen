            function bid5010ExpertRegistValidator(record, name, value) {
                if (name == 'birthday') {
                    var current_date = Aurora.formatDate(new Date());
                    value = Aurora.formatDate(value);
                    if (!Aurora.isEmpty(value) && !record.getField(value).isReadOnly()) {
                        if (value > current_date) {
                            return '${l:PROMPT.BIRTH_DATE_FORMATION}';
                        }
                    }
                    return true;
                } else if (name == 'email') {
                    var email_regexp = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,4})+$/;
                    if (!Aurora.isEmpty(value)) {
                        if (!email_regexp.test(value)) {
                            return '${l:PROMPT.EMAIL_FORMATION}';
                        }
                    }
                    return true;
                }
            }
            
            function bid5010ExpertAppCareersValidator(record, name, value) {
                if (name == 'start_date') {
                    value = Aurora.formatDate(value);
                    var end_date = Aurora.formatDate(record.get('end_date'));
                    var current_date = Aurora.formatDate(new Date());
                    if (!Aurora.isEmpty(value)) {
                        if (value > current_date) {
                            return '${l:PROMPT.TIME_FORMATION}';
                        }
                    }
                    if (!Aurora.isEmpty(value) && !Aurora.isEmpty(end_date)) {
                        if (value > end_date) {
                            return '${l:PROMPT.TIME_FORMATION}';
                        }
                    }
                    return true;
                } else if (name == 'end_date') {
                    value = Aurora.formatDate(value);
                    var start_date = Aurora.formatDate(record.get('start_date'));
                    if (!Aurora.isEmpty(value) && !Aurora.isEmpty(start_date)) {
                        if (start_date > value) {
                            return '${l:PROMPT.TIME_FORMATION}';
                        }
                    }
                    return true;
                }
            }
            
            function bid5010ExpertAppEducationsValidator(record, name, value) {
                if (name == 'start_date') {
                    value = Aurora.formatDate(value);
                    var end_date = Aurora.formatDate(record.get('end_date'));
                    var current_date = Aurora.formatDate(new Date());
                    if (!Aurora.isEmpty(value)) {
                        if (value > current_date) {
                            return '${l:PROMPT.TIME_FORMATION}';
                        }
                    }
                    if (!Aurora.isEmpty(value) && !Aurora.isEmpty(end_date)) {
                        if (value > end_date) {
                            return '${l:PROMPT.TIME_FORMATION}';
                        }
                    }
                    return true;
                } else if (name == 'end_date') {
                    value = Aurora.formatDate(value);
                    var start_date = Aurora.formatDate(record.get('start_date'));
                    if (!Aurora.isEmpty(value) && !Aurora.isEmpty(start_date)) {
                        if (start_date > value) {
                            return '${l:PROMPT.TIME_FORMATION}';
                        } else {
                            return true;
                        }
                    }
                }
            }
            
            function bid5010Save() {
                var ds = $('bid5010_expert_regist_ds');
                if (!ds.validate()) {
                    return;
                }
                Aurora.Masker.mask($('bid5020_expert_apply_create_window').wrap, '${l:LOADING}');
                ds.submit();
            }
            
            function bid5010Submit() {
                var ds = $('bid5010_expert_regist_ds');
                var rec = ds.getCurrentRecord();
                var expert_app_header_id = rec.get('expert_app_header_id');
                if (!Aurora.isEmpty(expert_app_header_id)) {
                    Aurora.Masker.mask($('bid5020_expert_apply_create_window').wrap, '${l:LOADING}');
            
                    Aurora.request({
                        url: $('bid5010_submit_expert_apply_link').getUrl(),
                        para: {
                            'expert_app_header_id': expert_app_header_id,
                            'status': rec.get('status')
                        },
                        success: function(res) {
                            Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
                            Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SUBMITED}', function() {
                                $('bid5010_expert_regist_ds').reset();
                                $('bid5010_field_add_ds').reset();
                                $('bid5010_expert_app_accounts_ds').reset();
                                $('bid5010_expert_app_fields_ds').reset();
                                $('bid5010_expert_app_achievements_ds').reset();
                                $('bid5010_expert_app_careers_ds').reset();
                                $('bid5010_expert_app_educations_ds').reset();
                                $('bid5010_expert_app_atm_files_ds').reset();
                                $('bid5010_expert_regist_ds').query();
            
                                Ext.get('bid_5010_tabpanel').setDisplayed(false);
                            });
                        },
                        failure: function(res) {
                            Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
                            return;
                        },
                        error: function(res) {
                            Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
                            return;
                        },
                        scope: this
                    });
            
                } else {
                    Aurora.showMessage('${l:PROMPT}', '${l:SAVE_FIRST}');
                }
            }
            
            function bid5010Cancel() {
                var ds = $('bid5010_expert_regist_ds');
                var rec = ds.getCurrentRecord();
                var expert_app_header_id = rec.get('expert_app_header_id');
                if (!Aurora.isEmpty(expert_app_header_id)) {
                    Aurora.Masker.mask($('bid5020_expert_apply_create_window').wrap, '${l:LOADING}');
            
                    Aurora.request({
                        url: $('bid5010_cancel_expert_apply_link').getUrl(),
                        para: {
                            'expert_app_header_id': expert_app_header_id,
                            'status': rec.get('status')
                        },
                        success: function(res) {
                            Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
                            Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.CANCELED}');
                        },
                        failure: function(res) {
                            Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
                            return;
                        },
                        error: function(res) {
                            Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
                            return;
                        },
                        scope: this
                    });
            
                } else {
                    Aurora.showMessage('${l:PROMPT}', '${l:SAVE_FIRST}');
                }
            }
            
