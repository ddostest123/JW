            function bid5010ExpertAppAttchmentsRender(value, record, name) {
                if (name == 'preview') {
                    if (record.isNew) {
                        return '<a href="javascript:bid5010ExpertAppAttchmentsUpload()">${l:PROMPT.UPLOAD_ATTACHMENT}</a>';
                    } else {
                        return '<a href="javascript:bid5010ExpertAppAttchmentsUpload()">${l:PROMPT.PREVIEW}</a>';
                    }
                } else if (name == 'file_name') {
                    if (Aurora.isEmpty(value)) {
                        return '${l:PROMPT.UPLOAD_WAITING}';
                    } else {
                        return value;
                    }
                }
            }
            
