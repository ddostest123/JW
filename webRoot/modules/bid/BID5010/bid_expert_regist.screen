        <script><![CDATA[
            function bid5010ExpertRegistOnUpdate(ds, record, name, value, old_value) {
                if (name == 'expert_type') {
                    bid5010ExpertRegistOnUpdateClear();
            
                    if (value == 'EXTERNAL') {
                        record.getField('expert_user_name').setReadOnly(true);
                        record.getField('expert_user_name').setRequired(false);
            
                        record.getField('full_name').setReadOnly(false);
                        record.getField('sex_desc').setReadOnly(false);
                        record.getField('birthday').setReadOnly(false);
                        record.getField('id_type_desc').setReadOnly(false);
                        record.getField('id_code').setReadOnly(false);
                        record.getField('mobile_phone').setReadOnly(false);
                        record.getField('telephone').setReadOnly(false);
                        record.getField('email').setReadOnly(false);
            
            
                        record.getField('full_name').setRequired(true);
                        record.getField('sex_desc').setRequired(true);
                        record.getField('birthday').setRequired(true);
                        record.getField('id_type').setRequired(true);
                        record.getField('id_type_desc').setRequired(true);
                        record.getField('id_code').setRequired(true);
                        record.getField('mobile_phone').setRequired(true);
                        record.getField('telephone').setRequired(true);
                        record.getField('email').setRequired(true);
                    } else if (value == 'INTERNAL') {
                        record.getField('expert_user_name').setReadOnly(false);
                        record.getField('expert_user_name').setRequired(true);
            
                        record.getField('hiredate').setReadOnly(true);
                        record.getField('full_name').setReadOnly(true);
                        record.getField('sex_desc').setReadOnly(false);
                        record.getField('birthday').setReadOnly(false);
                        record.getField('id_type_desc').setReadOnly(false);
                        record.getField('id_code').setReadOnly(false);
                        record.getField('mobile_phone').setReadOnly(false);
                        record.getField('telephone').setReadOnly(false);
                        record.getField('email').setReadOnly(false);
            
                        record.getField('id_type').setRequired(true);
                        record.getField('id_type_desc').setRequired(true);
                        record.getField('id_code').setRequired(true);
                        record.getField('mobile_phone').setRequired(true);
                        record.getField('email').setRequired(true);
            
            
                    } else if (Aurora.isEmpty(value)) {
                        record.getField('expert_user_name').setReadOnly(true);
                        record.getField('hiredate').setReadOnly(true);
                        record.getField('full_name').setReadOnly(true);
                        record.getField('sex_desc').setReadOnly(true);
                        record.getField('birthday').setReadOnly(true);
                        record.getField('id_type_desc').setReadOnly(true);
                        record.getField('id_code').setReadOnly(true);
                        record.getField('mobile_phone').setReadOnly(true);
                        record.getField('telephone').setReadOnly(true);
                        record.getField('email').setReadOnly(true);
                    }
                }
            
                function bid5010ExpertRegistOnUpdateClear() {
                    record.set('expert_user_name', '');
                    record.set('expert_user_id', '');
                    record.set('hiredate', null);
                    record.set('full_name', null);
                    record.set('birthday', null);
                    record.set('id_type', null);
                    record.set('id_type_desc', null);
                    record.set('id_code', null);
                    record.set('mobile_phone', null);
                    record.set('telephone', null);
                    record.set('email', null);
            
                    record.getField('expert_user_name').setRequired(false);
                    record.getField('hiredate').setRequired(false);
                    record.getField('full_name').setRequired(false);
                    record.getField('birthday').setRequired(false);
                    record.getField('id_type').setRequired(false);
                    record.getField('id_type_desc').setRequired(false);
                    record.getField('id_code').setRequired(false);
                    record.getField('mobile_phone').setRequired(false);
                    record.getField('telephone').setRequired(false);
                    record.getField('email').setRequired(false);
                }
            }
            
            function bid5010ExpertRegistOnSubmitsuccess(ds) {
                Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
                var expert_app_header_id = ds.getAt(0).get('expert_app_header_id');
                if (!ds.isNew && !Aurora.isEmpty(expert_app_header_id)) {
