            function bid5010ExpertAppAccountsOnAdd(ds, record, index) {
                var head_ds = $('bid5010_expert_regist_ds');
                var expert_app_header_id = head_ds.getAt(0).get('expert_app_header_id');
                record.set('expert_app_header_id', expert_app_header_id);
            }
            
            function bid5010ExpertAppAccountsOnUpdate(ds, record, name, value, old_value) {
                var records = ds.getAll();
                if (name == 'primary_flag') {
                    if (value == 'Y') {
                        for (var i = 0;i < records.length;i++) {
                            if (ds.indexOf(record) != ds.indexOf(records[i])) {
                                ds.indexOf(record);
                                records[i].set('primary_flag', 'N');
                            }
                        }
                    }
                } else if (name == 'province_name') {
                    record.set('city_name', null);
                    record.set('city_code', null);
                }
            }
            
