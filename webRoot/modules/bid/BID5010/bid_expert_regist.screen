            function bid5010ExpertAppAccountsEditor(record, name) {
                if (name == 'city_name') {
                    var province_name = record.get('province_name');
                    if (!Aurora.isEmpty(province_name)) {
                        var city_name_field = record.getField('city_name');
                        var province_id = record.get('province_id');
                        city_name_field.setLovService('public.fnd_region_city_lov');
                        city_name_field.setLovPara('province_id', province_id);
                        return 'bid5010_expert_app_accounts_lov';
                    }
                }
            }
            
            function bid5010ExpertAppAtmUpload() {
                var expert_app_header_id = $('bid5010_expert_regist_ds').getAt(0).get('expert_app_header_id');
                if (expert_app_header_id) {
                    var url = "${/request/@context_path}/uploadFile.screen?table_name=BID_EXPERT_APP_HEADERS&header_id=" + expert_app_header_id;
                    new Aurora.Window({
                        url: url,
                        title: '${l:ATM.UPLOAD_ATTACHMENT}',
                        id: 'bid5010_expert_app_atm_window',
                        width: 610,
                        height: 500
                    }).on('beforeclose', function() {
                        $('bid5010_expert_app_atm_files_ds').query();
                    });
                }
            }
            
