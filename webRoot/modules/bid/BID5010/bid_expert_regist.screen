                }
            
                $('bid5010_expert_app_accounts_ds').setQueryParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_accounts_ds').setSubmitParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_fields_ds').setQueryParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_fields_ds').setSubmitParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_achievements_ds').setQueryParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_achievements_ds').setSubmitParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_careers_ds').setQueryParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_careers_ds').setSubmitParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_educations_ds').setQueryParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_educations_ds').setSubmitParameter('expert_app_header_id', expert_app_header_id);
                $('bid5010_expert_app_atm_files_ds').setQueryParameter('table_name', 'BID_EXPERT_APP_HEADERS');
                $('bid5010_expert_app_atm_files_ds').setQueryParameter('pk_value', expert_app_header_id);
            }
            
            function bid5010ExpertRegistOnSubmitfailed() {
                Aurora.Masker.unmask($('bid5020_expert_apply_create_window').wrap);
