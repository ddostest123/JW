            function bid6050EntrustmentEditSave() {
                var header_ds = $('bid6050_edit_entrustment_header_ds');
                var item_ds = $('bid6050_edit_entrustment_items_ds');
                var bidder_ds = $('bid6050_edit_entrustment_bidders_ds');
                var prequal_ds = $('bid6050_edit_entrustment_prequal');
                var expert_ds = $('bid6050_ln_expert_ds');
                var expert_1_ds = $('bid6050_ln_expert_1_ds');
                var expert_2_ds = $('bid6050_ln_expert_2_ds');
                var element_ds = $('bid6050_scoring_elements_assign_ds');
                var element_1_ds = $('bid6050_scoring_elements_1_assign_ds');
                var element_2_ds = $('bid6050_scoring_elements_2_assign_ds');
                var lnExpertAll_ds = $('bid6050_entrustmentLnExpertAll_ds');
                var expertScore_ds = $('bid6050_expertScore_ds');
            
                if (!header_ds.validate()) {
                    return;
                }
                if (!item_ds.validate()) {
                    return;
                }
                if (!bidder_ds.validate()) {
                    return;
                }
                if (!expertScore_ds.validate()) {
                    return;
                }
            
            
                var prequal_flag = header_ds.getCurrentRecord().get('prequal_flag');
                if (prequal_flag == 'Y' && !prequal_ds.validate()) {
                    return;
                }
            
                var data = header_ds.getCurrentRecord().data;
                data['items'] = item_ds.getJsonData();
                data['bidders'] = bidder_ds.getJsonData();
                if (prequal_flag == 'Y') {
                    data['prequal'] = prequal_ds.getJsonData();
                }
            
            
                var score_mode = header_ds.getCurrentRecord().get('score_mode');
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    if (!expert_1_ds.validate() || !expert_2_ds.validate() || !element_1_ds.validate() || !element_2_ds.validate()) {
                        return;
                    }
            
                    data['experts_1'] = expert_1_ds.getJsonData();
                    data['experts_2'] = expert_2_ds.getJsonData();
                    data['elements_1'] = element_1_ds.getJsonData();
                    data['elements_2'] = element_2_ds.getJsonData();
                } else if (score_mode == 'NONDISTINCTION') {
                    if (!expert_ds.validate() || !element_ds.validate()) {
                        return;
                    }
            
                    data['experts_1'] = expert_ds.getJsonData();
                    data['elements_1'] = element_ds.getJsonData();
                }
            
                //评分转置
                var scores = [];
                // var allExperts = lnExpertAll_ds.getJsonData();
                // for (a in allExperts) {
                // alert(1 + '---' + a + '---' + allExperts[a]);
                // }
                var allExpertRecords = lnExpertAll_ds.getAll();
                for (var i = 0;i < allExpertRecords.length;i++) {
                    var score_records = allExpertRecords[i].get('scores');
                    if (score_records) {
                        if ('data' in score_records) {
                            var score_datas = score_records['data'];
                            for (a in score_datas) {
                                if (!isNaN(a)) {
                                    var score = {};
                                    if (score_datas[a]) {
                                        if ('data' in score_datas[a]) {
                                            var s_data = score_datas[a]['data'];
                                            score['score_header_id'] = s_data['score_header_id'];
                                            score['expert_suggestion'] = s_data['expert_suggestion'];
                                            score['bid_score_sum'] = s_data['bid_score_sum'];
                                            score['bid_header_id'] = s_data['bid_header_id'];
                                            score['line_expert_id'] = s_data['line_expert_id'];
                                            score['expert_user_id'] = s_data['expert_user_id'];
                                            score['comments'] = s_data['comments'];
            
                                            var score_lines = [];
            
                                            var element_records = $('bid6050_scoring_elements_ds').getAll();
                                            for (var j = 0;j < element_records.length;j++) {
                                                var score_line = {};
                                                var scoring_elements_id = element_records[j].get('scoring_elements_id');
                                                var point_score_line_id = 'score_line_id_' + scoring_elements_id;
                                                var score_line_id = s_data[point_score_line_id];
                                                if (score_line_id) {
                                                    score_line['score_line_id'] = score_line_id;
                                                    score_line['elements_score'] = s_data[scoring_elements_id];
                                                    score_line['scoring_elements_id'] = scoring_elements_id;
                                                    score_line['_status'] = 'update';
                                                }
                                                score_lines.push(score_line);
                                            }
                                            score['score_lines'] = score_lines;
                                            score['_status'] = 'update';
                                            scores.push(score);
            
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            
            
                data['scores'] = scores;
            
                Aurora.Masker.mask($('bid6050_entrustment_edit_window').wrap, '${l:LOADING}');
            
                Aurora.request({
                    url: $('bid6050_edit_entrustment_save_link').getUrl(),
                    para: data,
                    success: function() {
                        Aurora.Masker.unmask($('bid6050_entrustment_edit_window').wrap);
                        Aurora.showMessage('${l:PROMPT}', '${l:BID_ENTRUSTMENT_HEADERS.SAVED_MSG}!', function() {
                           //$( 'bid6050_entrustment_edit_window').load($('bid6050_entrustment_edit_link').getUrl() + '?entrustment_header_id=${/parameter/@entrustment_header_id}');
                            $('bid6050_entrustment_edit_window').close();
                            new Aurora.Window({
			                    id: 'bid6050_entrustment_edit_window',
			                    title: '${l:BID_ENTRUSTMENT_HEADERS.ENTRUSTMENT_APPLICATION_MAINTAIN}',
			                    url: $('bid6050_entrustment_edit_link').getUrl() + '?entrustment_header_id=${/parameter/@entrustment_header_id}',
			                    fullScreen:true
			                });
                        });
                    },
                    failure: function() {
                        Aurora.Masker.unmask($('bid6050_entrustment_edit_window').wrap);
                        return;
                    },
                    error: function() {
                        Aurora.Masker.unmask($('bid6050_entrustment_edit_window').wrap);
                        return;
                    }
                });
            }
            
