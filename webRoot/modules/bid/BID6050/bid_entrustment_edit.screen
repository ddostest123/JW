            function bid6050_agentTenderingResult_renderFun(value, record, name) {
                if ($('bid6050_edit_entrustment_header_ds').getAll().length == 1) {
                    prequal_flag = $('bid6050_edit_entrustment_header_ds').getAt(0).get('prequal_flag');
                } else {
                    prequal_flag = record.get('prequal_flag');
                }
                var prequal_pass_flag = record.get('prequal_pass_flag');
                var bidding_flag = record.get('bidding_flag');
                if (prequal_flag == 'Y') {
                    if (prequal_pass_flag == 'Y') {
                        record.getField('bidding_flag').setReadOnly(false);
            
                        if (bidding_flag == 'Y') {
                            record.getField('bidding_price').setReadOnly(false);
                            record.getField('bidding_price').setRequired(true);
                            record.getField('success_bid_quantity').setReadOnly(false);
                            record.getField('success_bid_quantity').setRequired(true);
                        } else {
                            record.getField('bidding_price').setRequired(false);
                            record.getField('bidding_price').setReadOnly(true);
                            record.getField('success_bid_quantity').setRequired(false);
                            record.getField('success_bid_quantity').setReadOnly(true);
                        }
                    } else {
                        record.getField('bidding_flag').setReadOnly(true);
                        record.getField('bidding_price').setRequired(false);
                        record.getField('bidding_price').setReadOnly(true);
                        record.getField('success_bid_quantity').setRequired(false);
                        record.getField('success_bid_quantity').setReadOnly(true);
                    }
                } else {
                    record.getField('prequal_pass_flag').setReadOnly(true);
                    record.getField('bidding_flag').setReadOnly(false);
                    if (prequal_pass_flag == 'Y') {
                        record.set('prequal_pass_flag', 'N');
                    }
            
                    if (bidding_flag == 'Y') {
                        record.getField('bidding_price').setReadOnly(false);
                        record.getField('bidding_price').setRequired(true);
                        record.getField('success_bid_quantity').setReadOnly(false);
                        record.getField('success_bid_quantity').setRequired(true);
                    } else {
                        record.getField('bidding_price').setRequired(false);
                        record.getField('bidding_price').setReadOnly(true);
                        record.getField('success_bid_quantity').setRequired(false);
                        record.getField('success_bid_quantity').setReadOnly(true);
                    }
                }
            
                return value;
            }
