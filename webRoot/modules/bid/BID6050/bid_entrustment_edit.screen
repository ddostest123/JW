            function bid6050_editAgentTenderingResult_updateFun(ds, record, name, value, oldvalue) {
            
                var prequal_flag = $('bid6050_edit_entrustment_header_ds').getAt(0).get('prequal_flag');
                if (name == 'prequal_pass_flag') {
                    //bid6050_setExpertScoreDisable(); //设置评分信息不可编辑
                    if (prequal_flag == 'Y') {
                        if (value == 'Y') {
                            record.getField('bidding_flag').setReadOnly(false);
                        } else {
                            record.set('bidding_flag', '');
                            record.set('bidding_price', '');
                            record.set('success_bid_quantity', '');
                            record.getField('bidding_flag').setReadOnly(true);
                            record.getField('bidding_price').setRequired(false);
                            record.getField('bidding_price').setReadOnly(true);
                            record.getField('success_bid_quantity').setRequired(false);
                            record.getField('success_bid_quantity').setReadOnly(true);
                        }
                    }
                } else if (name == 'bidding_flag') {
                    //bid6050_setExpertScoreDisable(); //设置评分信息不可编辑
                    if (value == 'Y') {
                        record.getField('bidding_price').setReadOnly(false);
                        record.getField('bidding_price').setRequired(true);
                        record.getField('success_bid_quantity').setReadOnly(false);
                        record.getField('success_bid_quantity').setRequired(true);
                    } else {
                        record.set('bidding_price', '');
                        record.set('success_bid_quantity', '');
                        record.getField('bidding_price').setRequired(false);
                        record.getField('bidding_price').setReadOnly(true);
                        record.getField('success_bid_quantity').setRequired(false);
                        record.getField('success_bid_quantity').setReadOnly(true);
                    }
                } else if (name == 'bidding_price' || name == 'success_bid_quantity') {
                    var bidding_price = record.get('bidding_price');
                    var success_bid_quantity = record.get('success_bid_quantity');
                    if (!Ext.isEmpty(success_bid_quantity)) {
                        if (!Ext.isEmpty(bidding_price)) {
                            record.set('bid_amount', parseFloat(bidding_price * success_bid_quantity).toFixed(2));
                        } else {
                            record.set('bid_amount', '');
                        }
                        var quantity = $('bid6050_edit_entrustment_items_ds').getCurrentRecord().get('quantity');
                        var success_proportion = (parseFloat(success_bid_quantity / quantity) * 100).toFixed(2) + '%';
                        record.set('success_proportion', success_proportion);
                    } else {
                        record.set('bid_amount', '');
                        record.set('success_proportion', '');
                    }
                }
            }
            
            function bid6050_expertScoreDs_updateFun(ds, record, name, value, oldvalue) {
                if (name == 'comments' || name == 'bid_score_sum') {
            
                   } else {
                    var element_records = $('bid6050_scoring_elements_ds').getAll();
                    var bid_score_sum = 0;
                    for (var i = 0;i < element_records.length;i++) {
                        var cloumn_name = element_records[i].get('scoring_elements_id');
                        if (parseFloat(record.get(cloumn_name)).toString() != "NaN") {
                            bid_score_sum = bid_score_sum + parseFloat(record.get(cloumn_name));
                        }
                    }
                    record.set('bid_score_sum', bid_score_sum);
                }
            
            }
            
