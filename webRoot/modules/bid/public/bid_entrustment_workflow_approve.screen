<a:screen xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script">
    <a:init-procedure/>
    <a:view>
        <script><![CDATA[
        
            function bid_publicDetailEntrustmentHeaderOnLoad(ds) {
                var record = ds.getCurrentRecord();
                var authorized_flag = record.get('authorized_flag');
            
                if (authorized_flag == 'N') {
                    record.getMeta().getField('tender_agent_desc').setRequired(true);
                } else if (authorized_flag == 'Y') {
                    record.getMeta().getField('tender_agent_desc').setReadOnly(true);
                }
                
                var prequal_flag = record.get('prequal_flag');
                if (prequal_flag == 'Y') {
                    document.getElementById('bid_public_approve_prequal_div').style.display = "block";
                } else {
                    document.getElementById('bid_public_approve_prequal_div').style.display = "none";
                }
            
                var score_mode = record.get('score_mode');
                if (score_mode == 'TECH_BIZ') {
                    record.set('team_1', 'TECHNOLOGY');
                    record.set('team_2', 'BUSINESS');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    document.getElementById('bid_public_approve_expertScore_div').style.display = "block";
                    document.getElementById('bid_public_approve_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'BIZ_TECH') {
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid_public_approve_expertScore_div').style.display = "block";
                    document.getElementById('bid_public_approve_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'SAME_TIME') {
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '1');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid_public_approve_expertScore_div').style.display = "block";
                    document.getElementById('bid_public_approve_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'NONDISTINCTION') {
                    record.set('team_1', 'NONDISTINCTION');
                    record.set('team_1_desc', '');
                    record.set('team_2_desc', '');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '');
                    document.getElementById('bid_public_approve_expertScore_div').style.display = "none";
                    document.getElementById('bid_public_approve_expertScore_no_div').style.display = "block";
                } 
                
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    //专家与评分要素查询
                    $('bid_public_approve_detail_entrustment_expert_snaps_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_public_approve_detail_entrustment_expert_snaps_1_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_public_approve_detail_entrustment_expert_snaps_1_ds').query();
                    $('bid_public_approve_detail_entrustment_expert_snaps_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid_public_approve_detail_entrustment_expert_snaps_2_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid_public_approve_detail_entrustment_expert_snaps_2_ds').query();
                    $('bid_public_scoring_elements_assign_snaps_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_public_scoring_elements_assign_snaps_1_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_public_scoring_elements_assign_snaps_1_ds').query();
                    $('bid_public_scoring_elements_assign_snaps_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid_public_scoring_elements_assign_snaps_2_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid_public_scoring_elements_assign_snaps_2_ds').query();
                } else if (score_mode == 'NONDISTINCTION') {
                    //专家与评分要素查询
                    $('bid_public_approve_detail_entrustment_expert_snaps_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_public_approve_detail_entrustment_expert_snaps_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_public_approve_detail_entrustment_expert_snaps_ds').query();
                    $('bid_public_scoring_elements_assign_snaps_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_public_scoring_elements_assign_snaps_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_public_scoring_elements_assign_snaps_ds').query();
                }
            }
            
            function bid_publicEntrustmentDetailAtmRender(value, rec, name) {
                if (name == 'file_download_link') {
                    var file_download_link = rec.get('file_download_link');
                    return file_download_link;
                }
            }
            
            function bid5160EntrustmentItemEditRender(value, rec, name) {
                if (name == 'quantity') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value);
                    }
                    return value;
                } else if (name == 'base_price') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value);
                    }
                    return value;
                } else if (name == 'unit_price') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value);
                    }
                    return value;
                } else if (name == 'net_price') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value);
                    }
                    return value;
                } else if (name == 'net_amount') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value);
                    }
                    return value;
                } else if (name == 'tax_amount') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value);
                    }
                    return value;
                } else if (name == 'total_amount') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value);
                    }
                    return value;
                }
            }
            
        ]]></script>
