<a:screen xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script">
    <a:init-procedure/>
    <a:view>
        <a:link id="bid_public_bidding_docm_detail_link" url="${/request/@context_path}/modules/bid/BID7060/bid_bidding_docm_detail.screen"/>
        <a:link id="bid_public_bidding_score_link" url="${/request/@context_path}/modules/bid/BID6040/bid_bidding_score.screen"/>
        <a:link id="bid_public_bidding_docm_history_link" url="${/request/@context_path}/modules/bid/BID6040/bid_bidding_docm_history.screen"/>
        <script><![CDATA[
            function bid_publicEntrustmentLnItemsFootRender(data, name) {
                var ds = $('bid_public_entrustment_ln_items_snaps_ds');
                var records = ds.getAll();
                if (name == 'base_price') {
                    return '合计';
                } else if (name == 'total_amount') {
                    var base_price_sum = 0;
                    for (var i = 0;i < records.length;i++) {
                        base_price_sum += records[i].get(name);
                    }
                    return base_price_sum;
                } else if (name == 'success_bid_amount') {
                    var success_bid_amount_sum = 0;
                    for (var j = 0;j < records.length;j++) {
                        success_bid_amount_sum += (Aurora.isEmpty(records[j].get(name)) ? 0 : records[j].get(name));
                    }
                    return success_bid_amount_sum;
                }
            }
            
            function bid_publicBiddingDocmDetailRender(value, record, name) {
                var bid_header_id = record.get('bid_header_id');
                var entrustment_header_id = record.get('entrustment_header_id');
                var round = record.get('round');
                var version = record.get('version');
                var success_bid_quantity = record.get('success_bid_quantity');
                if (name == 'bid_header_number') {
                    return '<a href="javascript:bid_publicBiddingDocmDetail(' + entrustment_header_id + ',' + bid_header_id + ')">' + value + '</a>';
                } else if (name == 'bidder_desc') {
                    var bidder_id = record.get('bidder_id');
                    var bidder_company_id = record.get('bidder_company_id');
                    return '<a href="javascript:bid_publicBiddingDocmHistory(' + entrustment_header_id + ',' + round + ',' + version + ',' + bidder_id + ',' + bidder_company_id + ')">' + value + '</a>';
                } else if (name == 'min_price_weight') {
                    return value + '%';
                } else if (name == 'bidding_score') {
                    if (value) {
                        return '<a href="javascript:bid_publicBiddingScoreDetail(' + entrustment_header_id + ',' + round + ',' + version + ',' + bid_header_id + ');">' + value + '</a>';
                    } else {
                        return '${l:BID_ENTRUSTMENT_LN_EXPERTS_NO_SCORED}';
                    }
            
                } else if (name == 'success_bid_proportion') {
                    var item_ds = $('bid_public_entrustment_ln_items_snaps_ds');
                    var item_rec = item_ds.getCurrentRecord();
                    var quantity = item_rec.get('quantity');
            
                    if (Aurora.isEmpty(success_bid_quantity)) {
                        return '0%';
                    }
                    return (parseFloat(success_bid_quantity / quantity) * 100).toFixed(2) + '%';
                }
            }
            
            function bid_publicBiddingDocmDetail(id, bid_header_id) {
                new Aurora.Window({
                    id: 'bid7060_bidding_docm_detail_window',
                    title: '${l:BID_ENTRUSTMENT_HEADERS.BID_DETAIL}',
                    url: $('bid_public_bidding_docm_detail_link').getUrl() + '?bid_header_id=' + bid_header_id + '&entrustment_header_id=' + id,
                    fullScreen: true
                });
            }
            
            function bid_publicBiddingDocmHistory(entrustment_header_id, round, version, bidder_id, bidder_company_id) {
                new Aurora.Window({
                    id: 'bid6040_bidding_docm_history_window',
                    title: '${l:BID_ENTRUSTMENT_HEADERS.BID_HISTORY}',
                    url: $('bid_public_bidding_docm_history_link').getUrl() + '?entrustment_header_id=' + entrustment_header_id + '&round=' + round + '&version=' + version + '&bidder_id=' + bidder_id + '&bidder_company_id=' + bidder_company_id,
                    height: 420,
                    width: 600
                });
            }
            
            function bid_publicBiddingScoreDetail(entrustment_header_id, round, version, bid_header_id) {
                new Aurora.Window({
                    id: 'bid6040_bidding_score_window',
                    title: '专家评分',
                    url: $('bid_public_bidding_score_link').getUrl() + '?entrustment_header_id=' + entrustment_header_id + '&bid_header_id=' + bid_header_id + '&round=' + round + '&version=' + version,
                    width: '800',
                    height: '400'
                });
            }
            
            function bid_publicBiddingDocmDetail2Render(value, record, name) {
                var bid_header_id = record.get('bid_header_id');
                var entrustment_header_id = record.get('entrustment_header_id');
                var round = record.get('round');
                var version = record.get('version');
                var success_bid_quantity = record.get('success_bid_quantity');
                if (name == 'bid_header_number') {
                    return '<a href="javascript:bid_publicBiddingDocmDetail(' + entrustment_header_id + ',' + bid_header_id + ')">' + value + '</a>';
                } else if (name == 'bidder_desc') {
                    var bidder_id = record.get('bidder_id');
                    var bidder_company_id = record.get('bidder_company_id');
                    return '<a href="javascript:bid_publicBiddingDocmHistory(' + entrustment_header_id + ',' + round + ',' + version + ',' + bidder_id + ',' + bidder_company_id + ')">' + value + '</a>';
                } else if (name == 'min_price_weight') {
                    return value + '%';
                } else if (name == 'bidding_score') {
                    if (value) {
                        return '<a href="javascript:bid_publicBiddingScoreDetail(' + entrustment_header_id + ',' + round + ',' + version + ',' + bid_header_id + ');">' + value + '</a>';
                    } else {
                        return '${l:BID_ENTRUSTMENT_LN_EXPERTS_NO_SCORED}';
                    }
                } else if (name == 'success_bid_proportion') {
                    var item_ds = $('bid_public_entrustment_ln_items_snaps_ds');
                    var item_rec = item_ds.getCurrentRecord();
                    var quantity = item_rec.get('quantity');
            
                    if (Aurora.isEmpty(success_bid_quantity)) {
                        return '0%';
                    }
                    return (parseFloat(success_bid_quantity / quantity) * 100).toFixed(2) + '%';
                }
            }
            
            function bid_publicEntrustmentLnItems2FootRender(data, name) {
                var ds = $('bid_public_entrustment_ln_items2_ds');
                var records = ds.getAll();
                if (name == 'base_price') {
                    return '合计';
                } else if (name == 'total_amount') {
                    var base_price_sum = 0;
                    for (var i = 0;i < records.length;i++) {
                        base_price_sum += records[i].get(name);
                    }
                    return base_price_sum;
                } else if (name == 'success_bid_amount') {
                    var success_bid_amount_sum = 0;
                    for (var j = 0;j < records.length;j++) {
                        success_bid_amount_sum += records[j].get(name);
                    }
                    return success_bid_amount_sum;
                }
            }
            
            function bid_publicEntrustmentDetailInitFun() {
                var score_method = $('bid_public_entrustment_header_snaps_ds').getAt(0).get('score_method');
                if (score_method == 'COMPREHENSIVE') {
                    $('bid_public_bidding_docm_headers_ds_grid').hideColumn('min_price_weight');
                    $('bid_public_bidding_docm_headers2_ds_grid').hideColumn('min_price_weight');
                }
                var record = $('bid_public_entrustment_header_snaps_ds').getAt(0);
                var prequal_flag = record.get('prequal_flag');
                if (prequal_flag == 'Y') {
                    document.getElementById('bid6040_prequal_div').style.display = "block";
                } else {
                    document.getElementById('bid6040_prequal_div').style.display = "none";
                }
            
                var score_mode = record.get('score_mode');
                if (score_mode == 'TECH_BIZ') {
                    record.set('team_1', 'TECHNOLOGY');
                    record.set('team_2', 'BUSINESS');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    document.getElementById('bid_public_expertScore_div').style.display = "block";
                    document.getElementById('bid_public_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'BIZ_TECH') {
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid_public_expertScore_div').style.display = "block";
                    document.getElementById('bid_public_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'SAME_TIME') {
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '1');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid_public_expertScore_div').style.display = "block";
                    document.getElementById('bid_public_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'NONDISTINCTION') {
                    record.set('team_1', 'NONDISTINCTION');
                    record.set('team_1_desc', '');
                    record.set('team_2_desc', '');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '');
                    document.getElementById('bid_public_expertScore_div').style.display = "none";
                    document.getElementById('bid_public_expertScore_no_div').style.display = "block";
                }
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    //专家与评分要素查询
                    $('bid_public_ln_expert_snaps_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_public_ln_expert_snaps_1_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_public_ln_expert_snaps_1_ds').query();
                    $('bid_public_ln_expert_snaps_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid_public_ln_expert_snaps_2_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid_public_ln_expert_snaps_2_ds').query();
                    $('bid_public_scoring_elements_1_assign_snaps_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_public_scoring_elements_1_assign_snaps_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_public_scoring_elements_1_assign_snaps_ds').query();
                    $('bid_public_scoring_elements_2_assign_snaps_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid_public_scoring_elements_2_assign_snaps_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid_public_scoring_elements_2_assign_snaps_ds').query();
                } else if (score_mode == 'NONDISTINCTION') {
                    //专家与评分要素查询
                    $('bid_pulic_ln_expert_snaps_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_pulic_ln_expert_snaps_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_pulic_ln_expert_snaps_ds').query();
                    $('bid_public_scoring_elements_assign_snaps_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid_public_scoring_elements_assign_snaps_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid_public_scoring_elements_assign_snaps_ds').query();
            
                }
            }
        ]]></script>
        <a:dataSets>
            <!-- 招标书头快照 -->
            <a:dataSet id="bid_public_entrustment_header_snaps_ds" loadData="true" model="bid.public.bid_entrustment_header_snaps" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_entrustment_header_snaps/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}">
                <a:fields>
                    <a:field name="tax_included_flag" readOnly="true"/>
                    <a:field name="margin_required_flag" readOnly="true"/>
                    <a:field name="bidder_qualified_flag" readOnly="true"/>
                    <a:field name="tax_included_flag" readOnly="true"/>
                    <a:field name="margin_required_flag" readOnly="true"/>
                    <a:field name="bidder_qualified_flag" readOnly="true"/>
                    <a:field name="bidding_type" readOnly="true"/>
                    <a:field name="bidding_type_desc" readOnly="true"/>
                    <a:field name="allowed_part_bidding_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="bidding_file_free_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="closed_bidding_flag" checkedValue="Y" defaultValue="N" readOnly="true" uncheckedValue="N"/>
                    <a:field name="bid_file_amount" required="true"/>
                    <a:field name="deposit_amount" readOnly="true"/>
                    <a:field name="score_mode_desc" readOnly="true"/>
                    <a:field name="team_1" readOnly="true"/>
                    <a:field name="team_1_desc" readOnly="true"/>
                    <a:field name="team_2" readOnly="true"/>
                    <a:field name="team_2_desc" readOnly="true"/>
                </a:fields>
            </a:dataSet>
            <!-- 招标书行快照 -->
            <a:dataSet id="bid_public_entrustment_ln_items_snaps_ds" autoCount="true" autoPageSize="true" autoQuery="true" model="bid.public.bid_entrustment_ln_items_snaps" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_entrustment_ln_items_snaps/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}">
                <a:fields>
                    <a:field name="tax_included_flag" checkedValue="Y" uncheckedValue="N"/>
                </a:fields>
            </a:dataSet>
            <!-- 投标书头 -->
            <a:dataSet id="bid_public_bidding_docm_headers_ds" autoCount="true" autoPageSize="true" autoQuery="true" bindName="bidding_docm_headers" bindTarget="bid_public_entrustment_ln_items_snaps_ds" model="bid.public.bid_bidding_docm_headers" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_bidding_docm_headers/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}"/>
            <!-- 投标书头2 -->
            <a:dataSet id="bid_public_bidding_docm_headers2_ds" autoCount="true" autoPageSize="true" autoQuery="true" model="bid.public.bid_bidding_docm_headers2" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_bidding_docm_headers2/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}"/>
            <!-- 投标书行 -->
            <a:dataSet id="bid_public_entrustment_ln_items2_ds" autoCount="true" autoPageSize="true" autoQuery="true" bindName="entrustment_ln_items" bindTarget="bid_public_bidding_docm_headers2_ds" model="bid.public.bid_bidding_lines_query" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_bidding_lines_query/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}">
                <a:fields>
                    <a:field name="tax_included_flag" checkedValue="Y" uncheckedValue="N"/>
                </a:fields>
            </a:dataSet>
            <!-- 资格预审 -->
            <a:dataSet id="bid_public_edit_entrustment_prequal_snaps_ds" autoQuery="true" model="bid.public.bid_entrustment_prequal_snaps" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_entrustment_prequal_snaps/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}">
                <a:fields>
                    <a:field name="entrustment_header_id" defaultValue="${/parameter/@entrustment_header_id}"/>
                    <a:field name="closing_date" readOnly="true"/>
                    <a:field name="review_method_desc" readOnly="true"/>
