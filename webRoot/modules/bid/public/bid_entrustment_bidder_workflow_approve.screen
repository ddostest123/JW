                </a:fields>
            </a:dataSet>
            <!-- 招标专家组2快照 -->
            <a:dataSet id="bid_public_ln_expert_snaps_2_ds" autoCount="true" autoPageSize="true" model="bid.public.bid_entrustment_ln_experts_snaps" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_entrustment_ln_experts_snaps/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}&amp;sequence_num=2">
                <a:fields>
