            <!-- 招标专家组1快照 -->
            <a:dataSet id="bid_public_ln_expert_snaps_1_ds" autoCount="true" autoPageSize="true" model="bid.public.bid_entrustment_ln_experts_snaps" queryUrl="${/request/@context_path}/autocrud/bid.public.bid_entrustment_ln_experts_snaps/query?entrustment_header_snap_id=${/parameter/@entrustment_header_snap_id}&amp;sequence_num=1">
                <a:fields>
                    <a:field name="entrustment_header_id" defaultValue="${/parameter/@entrustment_header_id}"/>
                    <a:field name="team"/>
                    <a:field name="sequence_num" defaultValue="1"/>
