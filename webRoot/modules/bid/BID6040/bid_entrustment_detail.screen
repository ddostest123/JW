                        success: function(res) {
                            Aurora.Masker.unmask($('bid6040_entrustment_detail_window').wrap);
                            Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.CONFIRMED}', function() {
                                $('bid6040_entrustment_detail_window').close();
                            });
            
                        },
                        failure: function() {
                            Aurora.Masker.unmask($('bid6040_entrustment_detail_window').wrap);
                            return;
                        },
                        error: function() {
                            Aurora.Masker.unmask($('bid6040_entrustment_detail_window').wrap);
                            return;
                        },
                        socpe: this
                    });
            
                }, function() {});
            }
            
