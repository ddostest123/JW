            function tamouseover(a, e) {
                var head_ds = $('bid6040_entrustment_header_ds');
                var record = head_ds.getAt(0);
                if (record.get('payment_terms')) {
                    document.getElementById("contain").style.display = "inline";
                }
            }
            
            function tAfocus(a) {
                document.getElementById("contain").style.display = "none";
            }
            
            function bid_bidding_docm_print() {
              var entrustment_header_id = '${/parameter/@entrustment_header_id}';
                var item_type =  $('bid6040_entrustment_header_ds').getAt(0).get('item_type');
             //  alert(item_type);
               if(item_type =='ITEM'){
               window.open($('bid_entrustment_print_link').getUrl() + '?entrustment_header_id=' + entrustment_header_id, '_blank');
               }
               else {
               window.open($('bid_entrustment_print_link_project').getUrl() + '?entrustment_header_id=' + entrustment_header_id, '_blank');
               }
            
            }
