        <script><![CDATA[
            function bid6040DetailEntrustmentHeaderOnLoad(ds) {
                var record = ds.getCurrentRecord();
            
                if (record.get('payment_terms')) {
                    document.getElementById("contain").innerHTML = record.get('payment_terms');
                }
            }
            
            function bid6040SubmitOa() {
                Aurora.showConfirm('${l:PROMPT}', '是否确认提交OA审批？', function() {
                    Aurora.Masker.mask($('bid6040_entrustment_detail_window').wrap, '${l:LOADING}');
            
                    var header_ds = $('bid6040_entrustment_header_ds');
                    var header_rec = header_ds.getCurrentRecord();
            
                    var status = header_rec.get('status');
                    if (status == 'OA_SUBMITTED') {
                        Aurora.showMessage('${l:PROMPT}', '状态为已提交OA审批的单据不能再次提交！', function() {});
                        return;
                    }
                    Aurora.request({
                        url: $('bid6040_submit_to_oa_link').getUrl(),
                        para: {
                            'entrustment_header_id': header_rec.get('entrustment_header_id'),
                            'round': header_rec.get('round'),
                            'eq_application_number':header_rec.get('eq_application_number'),
                            'version': header_rec.get('version'),
                            'status': header_rec.get('status')
                        },
                        success: function(res) {
                            Aurora.Masker.unmask($('bid6040_entrustment_detail_window').wrap);
                            Aurora.showMessage('${l:PROMPT}', '提交OA审批成功！', function() {
                                $('bid6040_entrustment_detail_window').close();
                            });
            
                        },
                        failure: function() {
                            Aurora.Masker.unmask($('bid6040_entrustment_detail_window').wrap);
                            return;
                        },
                        error: function() {
                            Aurora.Masker.unmask($('bid6040_entrustment_detail_window').wrap);
                            return;
                        },
                        socpe: this
                    });
            
                }, function() {});
            }
            
            function bid6040Confirm() {
                Aurora.showConfirm('${l:PROMPT}', '${l:CONFIRM}？', function() {
                    Aurora.Masker.mask($('bid6040_entrustment_detail_window').wrap, '${l:LOADING}');
            
                    var header_ds = $('bid6040_entrustment_header_ds');
                    var header_rec = header_ds.getCurrentRecord();
            
                    Aurora.request({
                        url: $('bid6040_comfirn_finish_award_of_bid_link').getUrl(),
                        para: {
