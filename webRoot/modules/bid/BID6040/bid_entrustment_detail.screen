            function bid6040BiddingScoreDetail(entrustment_header_id, bid_header_id) {
                new Aurora.Window({
                    id: 'bid6040_bidding_score_window',
                    title: '${l:BID_ENTRUSTMENT_HEADERS.EXPERT_SCORE}',
