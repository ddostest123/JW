                    for (var i = 0;i < records.length;i++) {
                        base_price_sum += records[i].get(name);
                    }
                    return base_price_sum;
                } else if (name == 'success_bid_amount') {
                    var success_bid_amount_sum = 0;
                    for (var j = 0;j < records.length;j++) {
                        success_bid_amount_sum += records[j].get(name);
                    }
                    return success_bid_amount_sum;
                }
            }
            
            function bid6040_entrustmentDetailInitFun() {
                var score_method = $('bid6040_entrustment_header_ds').getAt(0).get('score_method');
                if (score_method == 'COMPREHENSIVE') {
                    $('bid6040_bidding_docm_headers_grid').hideColumn('min_price_weight');
                    $('bid6040_bidding_docm_headers2_grid').hideColumn('min_price_weight');
                }
                var record = $('bid6040_entrustment_header_ds').getAt(0);
                var prequal_flag = record.get('prequal_flag');
                if (prequal_flag == 'Y') {
                    document.getElementById('bid6040_prequal_div').style.display = "block";
                    //document.getElementById('bid6040_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.ADD_PREQUAL}';
                } else {
                    document.getElementById('bid6040_prequal_div').style.display = "none";
                    //document.getElementById('bid6040_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.ADD_PREQUAL}';
                }
            
                var score_mode = record.get('score_mode');
                if (score_mode == 'TECH_BIZ') {
                    record.set('team_1', 'TECHNOLOGY');
                    record.set('team_2', 'BUSINESS');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    document.getElementById('bid6040_expertScore_div').style.display = "block";
                    document.getElementById('bid6040_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'BIZ_TECH') {
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid6040_expertScore_div').style.display = "block";
                    document.getElementById('bid6040_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'SAME_TIME') {
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '1');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid6040_expertScore_div').style.display = "block";
                    document.getElementById('bid6040_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'NONDISTINCTION') {
                    record.set('team_1', 'NONDISTINCTION');
                    record.set('team_1_desc', '');
                    record.set('team_2_desc', '');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '');
                    document.getElementById('bid6040_expertScore_div').style.display = "none";
                    document.getElementById('bid6040_expertScore_no_div').style.display = "block";
                }
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    //专家与评分要素查询
                    $('bid6040_ln_expert_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6040_ln_expert_1_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6040_ln_expert_1_ds').query();
                    $('bid6040_ln_expert_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid6040_ln_expert_2_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid6040_ln_expert_2_ds').query();
                    $('bid6040_scoring_elements_1_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6040_scoring_elements_1_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6040_scoring_elements_1_assign_ds').query();
                    $('bid6040_scoring_elements_2_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid6040_scoring_elements_2_assign_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid6040_scoring_elements_2_assign_ds').query();
                } else if (score_mode == 'NONDISTINCTION') {
                    //专家与评分要素查询
                    $('bid6040_ln_expert_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6040_ln_expert_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6040_ln_expert_ds').query();
                    $('bid6040_scoring_elements_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6040_scoring_elements_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6040_scoring_elements_assign_ds').query();
            
                }
            }
            
