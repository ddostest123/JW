                bid5150_ExpertScoreTeamChangeLoop($('bid5150_ln_expert_2_ds'), team_2, sequence_num_2);
                bid5150_ExpertScoreTeamChangeLoop($('bid5150_ln_expert_ds'), team_1, sequence_num_1);
            
                bid5150_ExpertScoreTeamChangeLoop($('bid5150_scoring_elements_1_assign_ds'), team_1, sequence_num_1);
                bid5150_ExpertScoreTeamChangeLoop($('bid5150_scoring_elements_2_assign_ds'), team_2, sequence_num_2);
                bid5150_ExpertScoreTeamChangeLoop($('bid5150_scoring_elements_assign_ds'), team_1, sequence_num_1);
            }
