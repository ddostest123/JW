<a:screen xmlns:c="aurora.application.action" xmlns:p="uncertain.proc" xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script" trace="true">
    <a:init-procedure/>
    <a:view>
        <a:link id="bid5150_entrustment_save_link" url="${/request/@context_path}/modules/bid/BID5150/bid_entrustment_edit_save.svc"/>
        <a:link id="bid5150_entrustment_expert_random_choose_link" url="${/request/@context_path}/modules/bid/BID5150/bid_entrustment_expert_random_choose.screen"/>
        <a:link id="bid5150_entrustment_turn_over_link" url="${/request/@context_path}/modules/bid/BID5150/bid_entrustment_turn_over.screen"/>
        <a:link id="bid5150_issue_entrustment_link" model="bid.BID5150.bid_issue" modelaction="execute"/>
        <a:link id="bid5040_attachment_upload_link" url="${/request/@context_path}/modules/bid/BID5040/bid_entrustment_attachment_upload.screen"/>
        <script><![CDATA[
            var bid5150_prequalFun_flag = 'N'; //N为不可取消
            var bid5150_prequalAttachement_flag = 'N'; //N为不可上传附件
            
            function bid5150EntrustmentEditCompareDate(start, end) {
                if (start > end) {
                    return false;
                }
                return true;
            }
            
            //比较两个日期的大小
            
            function bid5150EntrustmentEditCompareDate_2(start, end) {
            
                if (typeof(start) == 'string') {
                    start = new Date(start.replace(/-/g, '/'));
                }
            
                if (typeof(end) == 'string') {
                    end = new Date(end.replace(/-/g, '/'));
                }
            
                if (start > end) {
                    return false;
                }
                return true;
            }
            
            function bid5150EntrustmentEditValidator(record, name, value) {
                var start_date = record.get('bid_opening_start_date');
                var bid_closing_date = record.get('bid_closing_date');
                var prequal_flag = record.get('prequal_flag');
                if (name == 'bid_opening_start_date') {
            
                    if (!bid5150EntrustmentEditCompareDate_2(bid_closing_date, start_date)) {
                        return '${l:BID_OPEN_EARLIER_THAN_BID_END}';
                    }
            
                    return true;
                } else if (name == 'bid_closing_date') {
                    var sysdate = new Date();
                    if (!bid5150EntrustmentEditCompareDate_2(sysdate, bid_closing_date)) {
                        return '${l:BID_END_EARLIER_THAN_NOW}';
                    }
            
                    if (!bid5150EntrustmentEditCompareDate_2(bid_closing_date, start_date)) {
                        return '${l:BID_OPEN_EARLIER_THAN_BID_END}';
                    }
                    if ($('bid5150_edit_entrustment_prequal').getAll().length > 0) {
                        var prequal_closing_date = $('bid5150_edit_entrustment_prequal').getAt(0).get('closing_date');
                        if (!bid5150EntrustmentEditCompareDate_2(prequal_closing_date, bid_closing_date) && prequal_flag == 'Y') {
                            return '${l:BID_CLOSING_EARLIER_THAN_PREQUAL_CLOSING}';
                        }
            
                    }
            
            
                    return true;
                }
            }
            
            function bid5150EntrustmentPrequalEditValidator(record, name, value) {
                var closing_date = record.get('closing_date');
                var bid_closing_date = $('bid5150_edit_entustment_header_ds').getAt(0).get('bid_closing_date');
                if (name == 'closing_date') {
                    if (!bid5150EntrustmentEditCompareDate_2(closing_date, bid_closing_date)) {
                        return '${l:BID_CLOSING_EARLIER_THAN_PREQUAL_CLOSING}';
                    }
            
                    var sysdate = new Date();
                    if (!bid5150EntrustmentEditCompareDate_2(sysdate, closing_date)) {
                        return '${l:PREQUAL_CLOSING_EARLIER_THAN_SYSDATE}';
                    }
            
                    return true;
                }
            }
            
            
            function bid5150_onLnExpert_loadFun(ds) {
                for (var i = 0;i < ds.getAll().length;i++) {
                    var expert_rec = ds.getAll()[i];
                    // var multistage_flag = expert_rec.get('multistage_flag');
                    // //if (multistage_flag == 'N') {
                    expert_rec.getField('expert_user_name').setReadOnly(false);
                    expert_rec.getField('leader_flag').setReadOnly(false);
                    // //}
                }
            }
            
            
            function bid5150_setExpertEdit() {
                //设置专家可维护
                $('bid5150_ln_expert_1_grid_add_btu').enable();
                $('bid5150_ln_expert_1_grid_delete_btu').enable();
                $('bid5150_ln_expert_1_grid_random_btu').enable();
                $('bid5150_ln_expert_2_grid_add_btu').enable();
                $('bid5150_ln_expert_2_grid_delete_btu').enable();
                $('bid5150_ln_expert_2_grid_random_btu').enable();
                $('bid5150_ln_expert_grid_add_btu').enable();
                $('bid5150_ln_expert_grid_delete_btu').enable();
                $('bid5150_ln_expert_grid_random_btu').enable();
            }
            
            
            function bid5150_onEditEntrustmentPrequal_loadFun(ds) {
                for (var i = 0;i < ds.getAll().length;i++) {
                    var prequal_rec = ds.getAll()[i];
                    var multistage_flag = prequal_rec.get('multistage_flag');
                    var prequal_status = prequal_rec.get('status');
                    if (multistage_flag == 'N') {
                        if (prequal_status == 'RECHECK') {
                            //资格预审重新作业
                            //只维护预审信息中的申请截止日期
                            //prequal_rec.getField('closing_date').setReadOnly(false);
                            prequal_rec.getField('review_method_desc').setReadOnly(true);
                            prequal_rec.getField('qualified_limit').setReadOnly(true);
                            prequal_rec.getField('prequal_location').setReadOnly(true);
                            prequal_rec.getField('file_free_flag').setReadOnly(true);
                            prequal_rec.getField('proqual_file_expense').setReadOnly(true);
                            prequal_rec.getField('comments').setReadOnly(true);
                        } else if (prequal_status == 'SUBMITTED' || prequal_status == 'CANCELLED' || prequal_status == 'NEW') {
                            //第一次进入招标作业可以新增/取消资格预审
                            var review_method = prequal_rec.get('review_method');
                            if (review_method == 'QUALIFICATION') {
                                prequal_rec.getField('qualified_limit').setReadOnly(true);
                                prequal_rec.getField('qualified_limit').setRequired(false);
                            } else if (review_method == 'QUANTITY') {
                                prequal_rec.getField('qualified_limit').setReadOnly(false);
                                prequal_rec.getField('qualified_limit').setRequired(true);
                            }
            
                            var file_free_flag = prequal_rec.get('file_free_flag');
                            if (file_free_flag == 'Y') {
                                prequal_rec.getField('proqual_file_expense').setReadOnly(true);
                                prequal_rec.getField('proqual_file_expense').setRequired(false);
                            } else {
                                prequal_rec.getField('proqual_file_expense').setReadOnly(false);
                                prequal_rec.getField('proqual_file_expense').setRequired(true);
                            }
                        }
                    } else {
                        var review_method = prequal_rec.get('review_method');
                        if (review_method == 'QUALIFICATION') {
                            prequal_rec.getField('qualified_limit').setReadOnly(true);
                            prequal_rec.getField('qualified_limit').setRequired(false);
                        } else if (review_method == 'QUANTITY') {
                            prequal_rec.getField('qualified_limit').setReadOnly(false);
                            prequal_rec.getField('qualified_limit').setRequired(true);
                        }
            
                        var file_free_flag = prequal_rec.get('file_free_flag');
                        if (file_free_flag == 'Y') {
                            prequal_rec.getField('proqual_file_expense').setReadOnly(true);
                            prequal_rec.getField('proqual_file_expense').setRequired(false);
                        } else {
                            prequal_rec.getField('proqual_file_expense').setReadOnly(false);
                            prequal_rec.getField('proqual_file_expense').setRequired(true);
                        }
                    }
                }
            }
            
            function bid5150EntrustmentEditOnLoad(ds) {
                var record = ds.getCurrentRecord(0);
            
                var entrustment_type_id = record.get('entrustment_type_id');
                record.getField('invitation_tmpl_desc').setLovPara('entrustment_type_id', entrustment_type_id);
                record.getField('bidding_tmpl_desc').setLovPara('entrustment_type_id', entrustment_type_id);
                record.getField('evaluation_tmpl_desc').setLovPara('entrustment_type_id', entrustment_type_id);
            
                var company_id = record.get('company_id');
                record.getField('tendering_agent_desc').setLovPara('company_id', company_id);
            
                var using_company_id = record.get('using_company_id');
                record.getField('using_company_agent_desc').setLovPara('company_id', using_company_id);
            
                var margin_required_flag = record.get('margin_required_flag');
                if (margin_required_flag == 'Y') {
                    record.getMeta().getField('deposit_amount').setRequired(true);
                } else if (margin_required_flag == 'N') {
                    record.getMeta().getField('deposit_amount').setReadOnly(true);
                }
            
                var suggested_bidding_method = record.get('suggested_bidding_method');
                var bidding_method = record.get('bidding_method');
            
                if (bidding_method == 'OPEN') {
                    $('bid5150_edit_entrustment_tab').setDisabled(0);
                } else {
                    $('bid5150_edit_entrustment_tab').setEnabled(0);
                }
            
            
                var prequal_flag = record.get('prequal_flag');
                if (prequal_flag == 'Y') {
                    document.getElementById('bid5150_prequal_div').style.display = "block";
                    document.getElementById('bid5150_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.CANCEL_PREQUAL}';
                } else {
                    document.getElementById('bid5150_prequal_div').style.display = "none";
                    document.getElementById('bid5150_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.ADD_PREQUAL}';
                }
            
                if (using_company_id) {
                    record.getMeta().getField('inv_organization_desc').setReadOnly(false);
                    record.getField('inv_organization_desc').setLovPara('company_id', using_company_id);
                }
            
                var score_mode = record.get('score_mode');
                if (score_mode == 'TECH_BIZ') {
                    record.getField('score_mode_desc').setRequired(true);
                    document.getElementById('bid5150_expertScore_div').style.display = "block";
                    document.getElementById('bid5150_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'BIZ_TECH') {
                    record.getField('score_mode_desc').setRequired(true);
                    document.getElementById('bid5150_expertScore_div').style.display = "block";
                    document.getElementById('bid5150_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'SAME_TIME') {
                    record.getField('score_mode_desc').setRequired(true);
                    document.getElementById('bid5150_expertScore_div').style.display = "block";
                    document.getElementById('bid5150_expertScore_no_div').style.display = "none";
            
                } else if (score_mode == 'NONDISTINCTION') {
                    record.getField('score_mode_desc').setRequired(true);
                    document.getElementById('bid5150_expertScore_div').style.display = "none";
                    document.getElementById('bid5150_expertScore_no_div').style.display = "block";
            
                } else {
                    record.getField('score_mode_desc').setRequired(false);
                }
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    //专家与评分要素查询
                    $('bid5150_ln_expert_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5150_ln_expert_1_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5150_ln_expert_1_ds').query();
                    $('bid5150_ln_expert_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid5150_ln_expert_2_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid5150_ln_expert_2_ds').query();
                    $('bid5150_scoring_elements_1_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5150_scoring_elements_1_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5150_scoring_elements_1_assign_ds').query();
                    $('bid5150_scoring_elements_2_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid5150_scoring_elements_2_assign_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid5150_scoring_elements_2_assign_ds').query();
                } else if (score_mode == 'NONDISTINCTION') {
                    //专家与评分要素查询
                    $('bid5150_ln_expert_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5150_ln_expert_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5150_ln_expert_ds').query();
                    $('bid5150_scoring_elements_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid5150_scoring_elements_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid5150_scoring_elements_assign_ds').query();
                }
            
                var expert_score_flag = record.get('expert_score_flag');
                if (expert_score_flag == 'Y') {
                    document.getElementById('bid5150_expertScore').innerText = '${l:BID_ENTRUSTMENT_HEADERS.NO_EXPERT_SCORE}';
                } else {
                    document.getElementById('bid5150_expertScore').innerText = '${l:BID_ENTRUSTMENT_HEADERS.EXPERT_SCORE}';
            
                }
            
                var multistage_flag = record.get('multistage_flag');
                var prequal_status = record.get('prequal_status');
                if (multistage_flag == 'Y') {
                    //多轮报价回到招标作业时，预审信息不可改、专家信息不可改、评分要素可改
                    record.getField('score_mode_desc').setReadOnly(false);
                    record.getField('closed_bidding_flag').setReadOnly(false);
                    bid5150_setExpertEdit();
                    
                    if (prequal_status == 'RECHECK') {
                        bid5150_prequalFun_flag = 'N';
                        bid5150_prequalAttachement_flag = 'Y';
                        //资格预审重新作业
                    } else if (prequal_status == 'SUBMITTED' || prequal_status == 'CANCELLED') {
                        //第一次进入招标作业可以新增/取消资格预审，也可以维护专家和评分要素
                        bid5150_prequalFun_flag = 'Y';
                        bid5150_prequalAttachement_flag = 'Y';
                    } else {
                        bid5150_prequalFun_flag = 'Y';
                        bid5150_prequalAttachement_flag = 'Y';
                    }
                } else {
                    if (prequal_status == 'RECHECK') {
                        bid5150_prequalFun_flag = 'N';
                        bid5150_prequalAttachement_flag = 'Y';
                        //资格预审重新作业
                    } else if (prequal_status == 'SUBMITTED' || prequal_status == 'CANCELLED') {
                        //第一次进入招标作业可以新增/取消资格预审，也可以维护专家和评分要素
                        bid5150_prequalFun_flag = 'Y';
                        bid5150_prequalAttachement_flag = 'Y';
                    } else {
                        bid5150_prequalFun_flag = 'Y';
                        bid5150_prequalAttachement_flag = 'Y';
                    }
                    bid5150_setExpertEdit();
                    record.getField('score_mode_desc').setReadOnly(false);
                    record.getField('closed_bidding_flag').setReadOnly(false);
                }
            }
            
            function bid5150EntrustmentEditOnUpdate(ds, record, name, value, oldvalue) {
                if (name == 'bidding_method') {
                    if (value == 'OPEN') {
                        $('bid5150_edit_entrustment_tab').setDisabled(0);
                        //$('bid5150_edit_bidder_line_ds').removeAll();
                    } else {
                        $('bid5150_edit_entrustment_tab').setEnabled(0);
                    }
                } else if (name == 'score_mode') {
                    if (value == 'TECH_BIZ') {
                        record.set('team_1', 'TECHNOLOGY');
                        record.set('team_2', 'BUSINESS');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '2');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        document.getElementById('bid5150_expertScore_div').style.display = "block";
                        document.getElementById('bid5150_expertScore_no_div').style.display = "none";
                    } else if (value == 'BIZ_TECH') {
                        record.set('team_1', 'BUSINESS');
                        record.set('team_2', 'TECHNOLOGY');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '2');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        document.getElementById('bid5150_expertScore_div').style.display = "block";
                        document.getElementById('bid5150_expertScore_no_div').style.display = "none";
                    } else if (value == 'SAME_TIME') {
                        record.set('team_1', 'BUSINESS');
                        record.set('team_2', 'TECHNOLOGY');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '1');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        document.getElementById('bid5150_expertScore_div').style.display = "block";
                        document.getElementById('bid5150_expertScore_no_div').style.display = "none";
                    } else if (value == 'NONDISTINCTION') {
                        record.set('team_1', 'NONDISTINCTION');
                        record.set('team_1_desc', '');
                        record.set('team_2', '');
                        record.set('team_2_desc', '');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '');
                        document.getElementById('bid5150_expertScore_div').style.display = "none";
                        document.getElementById('bid5150_expertScore_no_div').style.display = "block";
                    }
            
                    if (value == 'TECH_BIZ' || value == 'BIZ_TECH' || value == 'SAME_TIME') {
                        //专家与评分要素查询
                        $('bid5150_ln_expert_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5150_ln_expert_1_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid5150_ln_expert_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                        $('bid5150_ln_expert_2_ds').setQueryParameter('team', record.get('team_2'));
                        $('bid5150_scoring_elements_1_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5150_scoring_elements_1_assign_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid5150_scoring_elements_2_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                        $('bid5150_scoring_elements_2_assign_ds').setQueryParameter('team', record.get('team_2'));
                    } else if (value == 'NONDISTINCTION') {
                        //专家与评分要素查询
                        $('bid5150_ln_expert_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5150_ln_expert_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid5150_scoring_elements_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid5150_scoring_elements_assign_ds').setQueryParameter('team', record.get('team_1'));
                    }
            
                    bid5150_ExpertScoreTeamChange(record.get('team_1'), record.get('sequence_num_1'), record.get('team_2'), record.get('sequence_num_2'));
                } else if (name == 'bid_opening_start_date'){
                    if(value){
                        record.set('bid_closing_date',value);
                    }
                }
            }
            
            function bid5150EntrustmentEditExpertOnUpdate(ds, record, name, value, oldValue) {
                if (name == 'leader_flag') {
                    var records = ds.getAll();
                    if (value == 'Y') {
                        for (var i = 0;i < records.length;i++) {
                            if (ds.indexOf(record) != ds.indexOf(records[i])) {
                                records[i].set('leader_flag', 'N');
                            }
                        }
                    }
                }
            }
            
            function bid5150EntrustmentEditSave() {
                var header_ds = $('bid5150_edit_entustment_header_ds');
                var bidder_ds = $('bid5150_edit_bidder_line_ds');
                var prequal_ds = $('bid5150_edit_entrustment_prequal');
                var expert_ds = $('bid5150_ln_expert_ds');
                var expert_1_ds = $('bid5150_ln_expert_1_ds');
                var expert_2_ds = $('bid5150_ln_expert_2_ds');
                var element_ds = $('bid5150_scoring_elements_assign_ds');
                var element_1_ds = $('bid5150_scoring_elements_1_assign_ds');
                var element_2_ds = $('bid5150_scoring_elements_2_assign_ds');
            
            
                if (!header_ds.validate()) {
                    return;
                }
                if (!bidder_ds.validate()) {
                    return;
                }
            
                var prequal_flag = header_ds.getCurrentRecord().get('prequal_flag');
                if (prequal_flag == 'Y' && !prequal_ds.validate()) {
                    return;
                }
            
                var data = header_ds.getCurrentRecord().data;
                data['bidders'] = bidder_ds.getJsonData();
            
                if (prequal_flag == 'Y') {
                    data['prequal'] = prequal_ds.getJsonData();
                }
            
            
                var score_mode = header_ds.getCurrentRecord().get('score_mode');
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    if (!expert_1_ds.validate() || !expert_2_ds.validate() || !element_1_ds.validate() || !element_2_ds.validate()) {
                        return;
                    }
            
                    data['experts_1'] = expert_1_ds.getJsonData();
                    data['experts_2'] = expert_2_ds.getJsonData();
                    data['elements_1'] = element_1_ds.getJsonData();
                    data['elements_2'] = element_2_ds.getJsonData();
                } else if (score_mode == 'NONDISTINCTION') {
                    if (!expert_ds.validate() || !element_ds.validate()) {
                        return;
                    }
            
                    data['experts_1'] = expert_ds.getJsonData();
                    data['elements_1'] = element_ds.getJsonData();
                }
            
                Aurora.Masker.mask($('bid5150_operation_window').wrap, '${l:LOADING}');
            
                Aurora.request({
                    url: $('bid5150_entrustment_save_link').getUrl(),
                    para: data,
                    success: function() {
                        Aurora.Masker.unmask($('bid5150_operation_window').wrap);
                        Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.SAVED}', function() {
                            $('bid5150_operation_window').close();
                            //$('bid5150_operation_window').load($('bid5150_entrustment_edit_link').getUrl() + '?entrustment_header_id=${/parameter/@entrustment_header_id}');
			                new Aurora.Window({
			                    id: 'bid5150_operation_window',
			                    title: '${l:BID_ENTRUSTMENT_HEADERS.ENTRUSTMENT_OPERATION}',
			                    url: $('bid5150_entrustment_edit_link').getUrl() + '?entrustment_header_id=${/parameter/@entrustment_header_id}',
			                    fullScreen: true
			                }).on('beforeclose', function() {
			                    try{
			                    	$('bid6080_entrustment_result_ds').query();
			                    }catch(e){
			                        null;
			                    }
			                });
                        });
                    },
                    failure: function() {
