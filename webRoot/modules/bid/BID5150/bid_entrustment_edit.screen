            //Check Ds Record Dirty/New
            
            
            function bid5150EntrustmentEditBulletinPreview() {
                var header_ds = $('bid5150_edit_entustment_header_ds');
                var bidder_ds = $('bid5150_edit_bidder_line_ds');
                var prequal_ds = $('bid5150_edit_entrustment_prequal');
                var expert_ds = $('bid5150_ln_expert_ds');
                var expert_1_ds = $('bid5150_ln_expert_1_ds');
                var expert_2_ds = $('bid5150_ln_expert_2_ds');
                var element_ds = $('bid5150_scoring_elements_assign_ds');
                var element_1_ds = $('bid5150_scoring_elements_1_assign_ds');
                var element_2_ds = $('bid5150_scoring_elements_2_assign_ds');
            
                if (!header_ds.validate()) {
                    return;
                }
            
                if (!bidder_ds.validate()) {
                    return;
                }
            
                //Check Ds Record Dirty/New
                if (header_ds.isModified() || bidder_ds.isModified()) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.PLEASE_SAVE}', function() {});
                    return;
                }
            
                var prequal_flag = header_ds.getCurrentRecord().get('prequal_flag');
                if (prequal_flag == 'Y') {
                    if (!prequal_ds.validate()) {
                        return;
                    }
                    if (prequal_ds.isModified()) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.PLEASE_SAVE}', function() {});
                        return;
                    }
                }
            
            
                var score_mode = header_ds.getCurrentRecord().get('score_mode');
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    if (!expert_1_ds.validate() || !expert_2_ds.validate() || !element_1_ds.validate() || !element_2_ds.validate()) {
                        return;
                    }
                    
                    if (expert_1_ds.isModified() || expert_2_ds.isModified() || element_1_ds.isModified() || element_2_ds.isModified()) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.PLEASE_SAVE}', function() {});
                        return;
                    }
            
                } else if (score_mode == 'NONDISTINCTION') {
                    if (!expert_ds.validate() || !element_ds.validate()) {
                        return;
                    }
                    if (expert_ds.isModified() || element_ds.isModified()) {
                        Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.PLEASE_SAVE}', function() {});
                        return;
                    }
                }
            
                var rec = header_ds.getCurrentRecord();
                var entrustment_header_id = rec.get('entrustment_header_id');
                var status = rec.get('status');
                var round = rec.get('round');
                var version = rec.get('version');
            
                Aurora.showConfirm('${l:PROMPT}', '${l:PROMPT.CONFIRM_RELEASE}？', function() {
                    Aurora.Masker.mask($('bid5150_operation_window').wrap, '${l:LOADING}');
            
                    Aurora.request({
                        url: $('bid5150_issue_entrustment_link').getUrl(),
                        para: {
                            'entrustment_header_id': entrustment_header_id,
                            'status': status,
                            'round': round,
                            'version': version,
                            'ignore_flag': 'N'
                        },
                        success: function(res) {
                            Aurora.Masker.unmask($('bid5150_operation_window').wrap);
                            if (!Ext.isEmpty(res.result.message)) {
                                Aurora.showConfirm('${l:PROMPT}', res.result.message, function() {
                                    bid5150EntrustmentIssueIgnore(entrustment_header_id, status, round, version)
                                });
                            } else {
                                Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.RELEASED}', function() {
                                    $('bid5150_operation_window').close();
                                });
                            }
                        },
                        failure: function() {
                            Aurora.Masker.unmask($('bid5150_operation_window').wrap);
                            return;
                        },
                        error: function() {
                            Aurora.Masker.unmask($('bid5150_operation_window').wrap);
                            return;
                        }
                    });
                });
            }
            
