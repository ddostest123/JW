            function bid6060_ExpertScoreTeamChangeLoop(ds, team, sequence) {
                var records = ds.getAll();
                for (var i = 0;i < records.length;i++) {
                    records[i].set('team', team);
                    records[i].set('sequence_num', sequence);
                }
            }
            
            function bid6060_ExpertScoreTeamChange(team_1, sequence_num_1, team_2, sequence_num_2) {
                bid6060_ExpertScoreTeamChangeLoop($('bid6060_ln_expert_1_ds'), team_1, sequence_num_1);
                bid6060_ExpertScoreTeamChangeLoop($('bid6060_ln_expert_2_ds'), team_2, sequence_num_2);
                bid6060_ExpertScoreTeamChangeLoop($('bid6060_ln_expert_ds'), team_1, sequence_num_1);
            
                bid6060_ExpertScoreTeamChangeLoop($('bid6060_scoring_elements_1_assign_ds'), team_1, sequence_num_1);
                bid6060_ExpertScoreTeamChangeLoop($('bid6060_scoring_elements_2_assign_ds'), team_2, sequence_num_2);
                bid6060_ExpertScoreTeamChangeLoop($('bid6060_scoring_elements_assign_ds'), team_1, sequence_num_1);
            }
            
            
            function bid6060EntrustmentEditCompareDate(start, end) {
                if (start > end) {
                    return false;
                }
                return true;
            }
            
            //比较两个日期的大小
            
            function bid6060EntrustmentEditCompareDate_2(start, end) {
            
                if (typeof(start) == 'string') {
                    start = new Date(start.replace(/-/g, '/'));
                }
            
                if (typeof(end) == 'string') {
                    end = new Date(end.replace(/-/g, '/'));
                }
            
                if (start > end) {
                    return false;
                }
                return true;
            }
            
            function bid6060EntrustmentEditValidator(record, name, value) {
                var start_date = record.get('bid_opening_start_date');
                var bid_closing_date = record.get('bid_closing_date');
                var prequal_flag = record.get('prequal_flag');
                if (name == 'bid_opening_start_date' ) {
            
                    if (!bid6060EntrustmentEditCompareDate_2(bid_closing_date, start_date)) {
                        return '${l:BID_OPEN_EARLIER_THAN_BID_END}';
                    }
            
                    return true;
                } else if (name == 'bid_closing_date') {
                    var sysdate = new Date();
                    if (!bid6060EntrustmentEditCompareDate_2(sysdate, bid_closing_date)) {
                        return '${l:BID_END_EARLIER_THAN_NOW}';
                    }
            
                    if (!bid6060EntrustmentEditCompareDate_2(bid_closing_date, start_date)) {
                        return '${l:BID_OPEN_EARLIER_THAN_BID_END}';
                    }
                    if ($('bid6060_edit_entrustment_prequal').getAll().length > 0) {
                        var prequal_closing_date = $('bid6060_edit_entrustment_prequal').getAt(0).get('closing_date');
                        if (!bid6060EntrustmentEditCompareDate_2(prequal_closing_date, bid_closing_date) && prequal_flag == 'Y') {
                            return '${l:BID_CLOSING_EARLIER_THAN_PREQUAL_CLOSING}';
                        }
            
                    }
            
            
                    return true;
                }
            }
        ]]></script>
