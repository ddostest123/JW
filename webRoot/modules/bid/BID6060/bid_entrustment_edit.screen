<a:screen xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script">
    <a:view>
        <a:link id="bid6060_edit_entrustment_save_link" url="${/request/@context_path}/modules/bid/BID6060/bid_entrustment_save.svc"/>
        <a:link id="bid6060_edit_entrustment_prequal_save_link" model="bid.BID6060.bid_entrustment_prequal" modelaction="update"/>
        <a:link id="bid6060_eidt_random_choose_link" url="${/request/@context_path}/modules/bid/BID5040/bid_entrustment_expert_random_choose.screen"/>
        <a:link id="bid5040_attachment_upload_link" url="${/request/@context_path}/modules/bid/BID5040/bid_entrustment_attachment_upload.screen"/>
        <script><![CDATA[
            var bid6060_tax_type_id = null;
            var bid6060_currency_code_change_flag = false;
            var bid6060_currency_name_change_flag = false;
            
            function bid6060EntrustmentEditOnLoad(ds) {
                var record = ds.getCurrentRecord();
                var company_id = record.get('company_id');
                var using_company_id = record.get('using_company_id');
                var entrustment_type_id = record.get('entrustment_type_id');
                var functional_currency_code = record.get('functional_currency_code');
                var currency_code = record.get('currency_code');
                var suggested_bidding_method = record.get('suggested_bidding_method');
                bid6060_tax_type_id = record.get('tax_type_id');
                var status = record.get('status');
            
                if (suggested_bidding_method == 'OPEN') {
                    $('bid6060_edit_entrustment_tab').setDisabled(1);
                } else {
                    $('bid6060_edit_entrustment_tab').setEnabled(1);
                }
            
                if (status == 'SUBMITTED') { //submit status
                    if (suggested_bidding_method == 'AGENT') {
                        record.getField('agent_company_name').setReadOnly(false);
                        record.getField('agent_company_name').setRequired(true);
                        record.getField('agent_company_tel').setReadOnly(false);
                        record.getField('agent_company_address').setReadOnly(false);
                    } else {
                        record.getField('agent_company_name').setRequired(false);
                        record.getField('agent_company_name').setReadOnly(true);
                        record.getField('agent_company_tel').setReadOnly(true);
                        record.getField('agent_company_address').setReadOnly(true);
                    }
            
                    var bidding_file_free_flag = record.get('bidding_file_free_flag');
                    if (bidding_file_free_flag == 'Y') {
                        record.getMeta().getField('bid_file_amount').setReadOnly(true);
                        record.getMeta().getField('bid_file_amount').setRequired(false);
                    } else {
                        record.getMeta().getField('bid_file_amount').setReadOnly(false);
                        record.getMeta().getField('bid_file_amount').setRequired(true);
                    }
            
                    var margin_required_flag = record.get('margin_required_flag');
                    if (margin_required_flag == 'Y') {
                        record.getMeta().getField('deposit_amount').setReadOnly(false);
                        record.getMeta().getField('deposit_amount').setRequired(true);
                    } else {
                        record.getMeta().getField('deposit_amount').setReadOnly(true);
                        record.getMeta().getField('deposit_amount').setRequired(false);
                    }
            
                    if (using_company_id) {
                        record.getMeta().getField('inv_organization_desc').setReadOnly(false);
                        record.getField('inv_organization_desc').setLovPara('company_id', using_company_id);
                    }
            
                    //Time Info
                    record.getMeta().getField('bid_closing_date').setReadOnly(true);
                    record.getMeta().getField('bid_closing_date').setRequired(false);
                    record.getMeta().getField('bid_opening_start_date').setReadOnly(true);
                    record.getMeta().getField('bid_opening_start_date').setRequired(false);
                    record.getMeta().getField('opening_place_desc').setReadOnly(true);
                    record.getMeta().getField('opening_place_desc').setRequired(false);
                } else if (status == 'ISSUED') { //issued status
                    record.getMeta().getField('entrustment_type_desc').setReadOnly(true);
                    record.getMeta().getField('entrustment_type_desc').setRequired(false);
            
                    record.getMeta().getField('suggested_bidding_method_desc').setReadOnly(true);
                    record.getMeta().getField('suggested_bidding_method_desc').setRequired(false);
            
                    record.getMeta().getField('agent_company_name').setReadOnly(true);
                    record.getMeta().getField('agent_company_name').setRequired(false);
            
                    record.getMeta().getField('agent_company_tel').setReadOnly(true);
                    record.getMeta().getField('agent_company_tel').setRequired(false);
            
                    record.getMeta().getField('agent_company_address').setReadOnly(true);
                    record.getMeta().getField('agent_company_address').setRequired(false);
            
                    record.getMeta().getField('bidding_type_desc').setReadOnly(true);
                    record.getMeta().getField('bidding_type_desc').setRequired(false);
            
                    record.getMeta().getField('score_method_desc').setReadOnly(true);
                    record.getMeta().getField('score_method_desc').setRequired(false);
            
                    record.getMeta().getField('allowed_part_bidding_flag').setReadOnly(true);
                    record.getMeta().getField('allowed_part_bidding_flag').setRequired(false);
            
                    record.getMeta().getField('project_location').setReadOnly(true);
                    record.getMeta().getField('project_location').setRequired(false);
            
                    record.getMeta().getField('title').setReadOnly(true);
                    record.getMeta().getField('title').setRequired(false);
            
                    record.getMeta().getField('using_company_desc').setReadOnly(true);
                    record.getMeta().getField('using_company_desc').setRequired(false);
            
                    record.getMeta().getField('pur_organization_desc').setReadOnly(true);
                    record.getMeta().getField('pur_organization_desc').setRequired(false);
            
                    record.getMeta().getField('inv_organization_desc').setReadOnly(true);
                    record.getMeta().getField('inv_organization_desc').setRequired(false);
            
                    record.getMeta().getField('tender_agent_desc').setReadOnly(true);
                    record.getMeta().getField('tender_agent_desc').setRequired(false);
            
                    record.getMeta().getField('tax_included_flag').setReadOnly(true);
                    record.getMeta().getField('tax_included_flag').setRequired(false);
            
                    record.getMeta().getField('currency_name').setReadOnly(true);
                    record.getMeta().getField('currency_name').setRequired(false);
            
                    record.getMeta().getField('bidding_file_free_flag').setReadOnly(true);
                    record.getMeta().getField('bidding_file_free_flag').setRequired(false);
            
                    record.getMeta().getField('bid_file_amount').setReadOnly(true);
                    record.getMeta().getField('bid_file_amount').setRequired(false);
            
                    record.getMeta().getField('necessary_lead_time').setReadOnly(true);
                    record.getMeta().getField('necessary_lead_time').setRequired(false);
            
                    record.getMeta().getField('margin_required_flag').setReadOnly(true);
                    record.getMeta().getField('margin_required_flag').setRequired(false);
            
                    record.getMeta().getField('deposit_amount').setReadOnly(true);
                    record.getMeta().getField('deposit_amount').setRequired(false);
            
                    record.getMeta().getField('complete_date').setReadOnly(true);
                    record.getMeta().getField('complete_date').setRequired(false);
            
                    record.getMeta().getField('winning_bidder_maxcount').setReadOnly(true);
                    record.getMeta().getField('winning_bidder_maxcount').setRequired(false);
            
                    record.getMeta().getField('functional_amount').setReadOnly(true);
                    record.getMeta().getField('functional_amount').setRequired(false);
            
                    record.getMeta().getField('entered_amount').setReadOnly(true);
                    record.getMeta().getField('entered_amount').setRequired(false);
            
                    record.getMeta().getField('comments').setReadOnly(true);
                    record.getMeta().getField('comments').setRequired(false);
                }
            
            
                var prequal_flag = record.get('prequal_flag');
                if (prequal_flag == 'Y') {
                    document.getElementById('bid6060_prequal_div').style.display = "block";
                    document.getElementById('bid6060_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.CANCEL_PREQUAL}';
                } else {
                    document.getElementById('bid6060_prequal_div').style.display = "none";
                    document.getElementById('bid6060_prequal').innerText = '${l:BID_ENTRUSTMENT_HEADERS.ADD_PREQUAL}';
                }
            
                var score_mode = record.get('score_mode');
                if (score_mode == 'TECH_BIZ') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'TECHNOLOGY');
                    record.set('team_2', 'BUSINESS');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    document.getElementById('bid6060_expertScore_div').style.display = "block";
                    document.getElementById('bid6060_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'BIZ_TECH') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '2');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid6060_expertScore_div').style.display = "block";
                    document.getElementById('bid6060_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'SAME_TIME') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'BUSINESS');
                    record.set('team_2', 'TECHNOLOGY');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '1');
                    record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                    record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                    document.getElementById('bid6060_expertScore_div').style.display = "block";
                    document.getElementById('bid6060_expertScore_no_div').style.display = "none";
                } else if (score_mode == 'NONDISTINCTION') {
                    record.getField('score_mode_desc').setRequired(true);
                    record.set('team_1', 'NONDISTINCTION');
                    record.set('team_1_desc', '');
                    record.set('team_2_desc', '');
                    record.set('sequence_num_1', '1');
                    record.set('sequence_num_2', '');
                    document.getElementById('bid6060_expertScore_div').style.display = "none";
                    document.getElementById('bid6060_expertScore_no_div').style.display = "block";
                } else {
                    record.getField('score_mode_desc').setRequired(false);
                }
            
                if (score_mode == 'TECH_BIZ' || score_mode == 'BIZ_TECH' || score_mode == 'SAME_TIME') {
                    //专家与评分要素查询
                    $('bid6060_ln_expert_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6060_ln_expert_1_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6060_ln_expert_1_ds').query();
                    $('bid6060_ln_expert_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid6060_ln_expert_2_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid6060_ln_expert_2_ds').query();
                    $('bid6060_scoring_elements_1_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6060_scoring_elements_1_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6060_scoring_elements_1_assign_ds').query();
                    $('bid6060_scoring_elements_2_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                    $('bid6060_scoring_elements_2_assign_ds').setQueryParameter('team', record.get('team_2'));
                    $('bid6060_scoring_elements_2_assign_ds').query();
                } else if (score_mode == 'NONDISTINCTION') {
                    //专家与评分要素查询
                    $('bid6060_ln_expert_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6060_ln_expert_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6060_ln_expert_ds').query();
                    $('bid6060_scoring_elements_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                    $('bid6060_scoring_elements_assign_ds').setQueryParameter('team', record.get('team_1'));
                    $('bid6060_scoring_elements_assign_ds').query();
                }
            
                var expert_score_flag = record.get('expert_score_flag');
                if (expert_score_flag == 'Y') {
                    document.getElementById('bid6060_expertScore').innerText = '${l:BID_ENTRUSTMENT_HEADERS.NO_EXPERT_SCORE}';
                } else {
                    document.getElementById('bid6060_expertScore').innerText = '${l:BID_ENTRUSTMENT_HEADERS.EXPERT_SCORE}';
                }
            }
            
            function bid6060EntrustmentEditOnUpdate(ds, record, name, value, oldvalue) {
                if (name == "using_company_id") {
                    bid6060_currency_code_change_flag = true;
                    bid6060_currency_name_change_flag = true;
                    record.set('exchange_rate', 1);
            
                    record.set('inv_organization_id', '');
                    record.set('inv_organization_desc', '');
                    if (value) {
                        record.getMeta().getField('inv_organization_desc').setReadOnly(false);
                        record.getField('inv_organization_desc').setLovPara('company_id', value);
                    } else {
                        record.getMeta().getField('inv_organization_desc').setReadOnly(true);
                        record.getField('inv_organization_desc').setLovPara('company_id', -1);
                    }
            
                    //Clear Item Line - Org
                    var records = $('bid6060_edit_entrustment_items_ds').getAll();
                    for (var i = 0;i < records.length;i++) {
                        records[i].set('inv_organization_id', '');
                        records[i].set('inv_organization_desc', '');
                    }
                } else if (name == "entrustment_type_id") {
                    record.set('tendering_tmpl_id', '');
                    record.set('tendering_tmpl_desc', '');
                    record.getField('tendering_tmpl_desc').setLovPara('entrustment_type_id', value);
                } else if (name == "functional_currency_code") {
                    if (bid6060_currency_code_change_flag == true) {
                        record.set('currency_code', value);
            
                        bid6060_currency_code_change_flag = false;
                    }
                } else if (name == "functional_currency_name") {
                    if (bid6060_currency_name_change_flag == true) {
                        record.set('currency_name', value);
            
                        bid6060_currency_name_change_flag = false;
                    }
                } else if (name == "currency_code") {
                    var functional_currency_code = record.get('functional_currency_code');
                    if (functional_currency_code != value) {
                        //record.getMeta().getField('exchange_rate').setReadOnly(false);
                        //record.getMeta().getField('exchange_rate').setRequired(true);
                        record.set('exchange_rate', '');
                    } else {
                        //record.getMeta().getField('exchange_rate').setReadOnly(true);
                        //record.getMeta().getField('exchange_rate').setRequired(false);
                        record.set('exchange_rate', 1);
                    }
                } else if (name == 'suggested_bidding_method') {
                    if (value == 'OPEN') {
                        $('bid6060_edit_entrustment_tab').setDisabled(1);
                    } else {
                        $('bid6060_edit_entrustment_tab').setEnabled(1);
                    }
            
                    record.set('agent_company_id', '');
                    record.set('agent_company_name', '');
                    record.set('agent_company_tel');
                    record.set('agent_company_address');
            
                    if (value == 'AGENT') {
                        record.getField('agent_company_name').setReadOnly(false);
                        record.getField('agent_company_name').setRequired(true);
                        record.getField('agent_company_tel').setReadOnly(false);
                        record.getField('agent_company_address').setReadOnly(false);
                    } else {
                        record.getField('agent_company_name').setRequired(false);
                        record.getField('agent_company_name').setReadOnly(true);
                        record.getField('agent_company_tel').setReadOnly(true);
                        record.getField('agent_company_address').setReadOnly(true);
                    }
                } else if (name == 'tax_included_flag') {
                    record.set('tax_type_id', '');
                    record.set('tax_type_code', '');
                    record.set('tax_type_rate', '');
                    // if (value == 'Y') {
                    // record.getField('tax_type_rate').setReadOnly(false);
                    // record.getField('tax_type_rate').setRequired(true);
                    // } else {
                    // record.getField('tax_type_rate').setRequired(false);
                    // record.getField('tax_type_rate').setReadOnly(true);
                    // }
                } else if (name == 'bidding_file_free_flag') {
                    record.set('bid_file_amount', '');
                    if (value == 'Y') {
                        record.getMeta().getField('bid_file_amount').setReadOnly(true);
                        record.getMeta().getField('bid_file_amount').setRequired(false);
                    } else {
                        record.getMeta().getField('bid_file_amount').setReadOnly(false);
                        record.getMeta().getField('bid_file_amount').setRequired(true);
                    }
                } else if (name == 'margin_required_flag') {
                    record.set('deposit_amount', '');
                    if (value == 'Y') {
                        record.getMeta().getField('deposit_amount').setReadOnly(false);
                        record.getMeta().getField('deposit_amount').setRequired(true);
                    } else {
                        record.getMeta().getField('deposit_amount').setReadOnly(true);
                        record.getMeta().getField('deposit_amount').setRequired(false);
                    }
                } else if (name == 'score_mode') {
                    if (value == 'TECH_BIZ') {
                        record.set('team_1', 'TECHNOLOGY');
                        record.set('team_2', 'BUSINESS');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '2');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        document.getElementById('bid6060_expertScore_div').style.display = "block";
                        document.getElementById('bid6060_expertScore_no_div').style.display = "none";
                    } else if (value == 'BIZ_TECH') {
                        record.set('team_1', 'BUSINESS');
                        record.set('team_2', 'TECHNOLOGY');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '2');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        document.getElementById('bid6060_expertScore_div').style.display = "block";
                        document.getElementById('bid6060_expertScore_no_div').style.display = "none";
                    } else if (value == 'SAME_TIME') {
                        record.set('team_1', 'BUSINESS');
                        record.set('team_2', 'TECHNOLOGY');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '1');
                        record.set('team_1_desc', '${l:BID_ENTRUSTMENT_BUSINESS_TEAM}');
                        record.set('team_2_desc', '${l:BID_ENTRUSTMENT_TECHNOLOGY_TEAM}');
                        document.getElementById('bid6060_expertScore_div').style.display = "block";
                        document.getElementById('bid6060_expertScore_no_div').style.display = "none";
                    } else if (value == 'NONDISTINCTION') {
                        record.set('team_1', 'NONDISTINCTION');
                        record.set('team_2', '');
                        record.set('sequence_num_1', '1');
                        record.set('sequence_num_2', '');
                        record.set('team_1_desc', '');
                        record.set('team_2_desc', '');
                        document.getElementById('bid6060_expertScore_div').style.display = "none";
                        document.getElementById('bid6060_expertScore_no_div').style.display = "block";
                    }
            
                    if (value == 'TECH_BIZ' || value == 'BIZ_TECH' || value == 'SAME_TIME') {
                        //专家与评分要素查询
                        $('bid6060_ln_expert_1_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid6060_ln_expert_1_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid6060_ln_expert_2_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                        $('bid6060_ln_expert_2_ds').setQueryParameter('team', record.get('team_2'));
                        $('bid6060_scoring_elements_1_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid6060_scoring_elements_1_assign_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid6060_scoring_elements_2_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_2'));
                        $('bid6060_scoring_elements_2_assign_ds').setQueryParameter('team', record.get('team_2'));
                    } else if (value == 'NONDISTINCTION') {
                        //专家与评分要素查询
                        $('bid6060_ln_expert_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid6060_ln_expert_ds').setQueryParameter('team', record.get('team_1'));
                        $('bid6060_scoring_elements_assign_ds').setQueryParameter('sequence_num', record.get('sequence_num_1'));
                        $('bid6060_scoring_elements_assign_ds').setQueryParameter('team', record.get('team_1'));
                    }
            
                    bid6060_ExpertScoreTeamChange(record.get('team_1'), record.get('sequence_num_1'), record.get('team_2'), record.get('sequence_num_2'));
                }
            }
            
            
            function bid6060EntrustmentEditItemsOnAdd(ds, rec, index) {
                var header_ds = $('bid6060_edit_entrustment_header_ds');
                var header_rec = header_ds.getCurrentRecord();
                var using_company_id = header_rec.get('using_company_id');
            
                if (Aurora.isEmpty(using_company_id)) {
                    Aurora.showMessage('${l:PROMPT}', '${l:BID_ENTRUSTMENT_MESSAGE.TENDER_CO_MATERIAL}');
                    return false;
                }
            
                rec.set('need_by_date', header_rec.get('complete_date'));
                rec.set('tax_included_flag', header_rec.get('tax_included_flag'));
                rec.set('tax_type_id', header_rec.get('tax_type_id'));
                rec.set('tax_type_code', header_rec.get('tax_type_code'));
                rec.set('tax_type_rate', header_rec.get('tax_type_rate'));
                rec.set('inv_organization_id', header_rec.get('inv_organization_id'));
                rec.set('inv_organization_desc', header_rec.get('inv_organization_desc'));
            }
            
            //Line Load
            
            
            function bid6060EntrustmentEditItemsOnLoad(ds) {
                var records = ds.getAll();
                var header_status;
                for (var i = 0;i < records.length;i++) {
                    var record = records[i];
                    //判断税
                    var tax_included_flag = record.get('tax_included_flag');
                    if (tax_included_flag == 'Y') {
                        record.getField('tax_type_rate').setReadOnly(false);
                        record.getField('tax_type_rate').setRequired(true);
                    }
            
                    var inv_organization_id = record.get('inv_organization_id');
                    if (Ext.isEmpty(inv_organization_id)) {
                        record.getField('item_code').setReadOnly(true);
                    } else {
                        record.getField('item_code').setReadOnly(false);
                    }
            
                    header_status = record.get('status');
                    if (header_status == 'ISSUED') {
                        record.getField('line_num').setReadOnly(true);
                        record.getField('line_num').setRequired(false);
            
                        record.getField('inv_organization_desc').setReadOnly(true);
                        record.getField('inv_organization_desc').setRequired(false);
            
                        record.getField('item_code').setReadOnly(true);
                        record.getField('item_code').setRequired(false);
            
                        record.getField('item_desc').setReadOnly(true);
                        record.getField('item_desc').setRequired(false);
            
                        record.getField('item_category_desc').setReadOnly(true);
                        record.getField('item_category_desc').setRequired(false);
            
                        record.getField('need_by_date').setReadOnly(true);
                        record.getField('need_by_date').setRequired(false);
            
                        record.getField('quantity').setReadOnly(true);
                        record.getField('quantity').setRequired(false);
            
                        record.getField('uom_desc').setReadOnly(true);
                        record.getField('uom_desc').setRequired(false);
            
                        record.getField('base_price').setReadOnly(true);
                        record.getField('base_price').setRequired(false);
            
