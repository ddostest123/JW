					                    title: '${l:BID_ENTRUSTMENT_HEADERS.ENTRUSTMENT_APPLICATION_MAINTAIN}',
					                    url: $('bid6070_entrustment_edit_link').getUrl() + '?entrustment_header_id=' + entrustment_header_id,
