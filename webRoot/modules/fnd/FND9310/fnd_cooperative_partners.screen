            //注册日期校检
            function fnd9310_query_register_dateValidator(record,name,value){
                if(name == "register_date_from" || name == "register_date_to"){
                    var start_date = record.get("register_date_from");  
                    var end_date = record.get("register_date_to");
                    if(typeof(end_date) != 'undefined' && !Ext.isEmpty(end_date) && typeof(start_date) != 'undefined' && !Ext.isEmpty(start_date)){
                        if(start_date > end_date){
                            return '${l:PROMPT.START_DATE_LESS_END_DATE}';   
                        }   
                    }
                    return true;
                }  
            }
            
            //关联日期校检
            function fnd9310_query_relation_dateValidator(record,name,value){
                if(name == "relation_date_from" || name == "relation_date_to"){
