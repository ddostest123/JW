                }else if(require_flag == 'N' && supply_flag == 'Y' && service_flag == 'N'){
                    document.getElementById("fnd9310_client_item").className = 'item-selected';
                    document.getElementById("fnd9310_vendor_item").className = 'item-disabled';
                    document.getElementById("fnd9310_vendor_item").onclick = "fnd9310_item_onclick()";
                    document.getElementById("fnd9310_service_item").className = 'item-unselected';
                    document.getElementById("fnd9310_client_div").style.display = "block";
                    document.getElementById("fnd9310_vendor_div").style.display = "none";
                    document.getElementById("fnd9310_service_div").style.display = "none";
                }else if(require_flag == 'N' && supply_flag == 'N' && service_flag == 'Y'){
