        <script><![CDATA[
          function change2LinkItem(e, li){
              if (!li){
                e = e || window.event;
            var target = e.srcElement || e.target;
            if (target.nodeName.toUpperCase() == 'UL'){
              return;
            }else{
              while(target.nodeName.toUpperCase() != 'LI'){
                target = target.parentNode;
              }
            }
              }else{
                  target = li;
              }
          
              var activeLi = target.parentNode.active || target.parentNode.children[0];
          activeLi.className = 'short-link-item';
              target.parentNode.active = target;
              target.className = 'short-link-item active';
          }
          document.getElementById('item_purchase_con').onscroll = function (e){
            e = e || window.event;
          var p = e.srcElement || e.target;
            var top = p.scrollTop;
            
            var i, l = Math.floor(p.children.length / 2);
            for (i = 0; i < l; i++){
                div = p.children[i *2];
                if (top >= p.children[i *2].offsetTop && top < p.children[i *2 + 2].offsetTop){
