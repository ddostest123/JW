<ws:SOAPConfiguration xmlns:ws="aurora.service.ws" model="sys.WS.sys_web_services_for_query">
	<soapResponse xmlns="http://www.aurora-framework.org/schema">   
		<RESPONSE_HEADER>
			<RESPONSE_STATUS>${@status}</RESPONSE_STATUS>	<!-- 返回状态 SUCCESSED 成功 FAILED -->
			<RESPONSE_MESSAGE>${@message}</RESPONSE_MESSAGE>	<!-- 返回文本 错误则返回错误消息 -->
			<RESPONSE_RESULT>${/parameter/@result}</RESPONSE_RESULT>		
		</RESPONSE_HEADER>
	</soapResponse>
</ws:SOAPConfiguration>