<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            
           select count(1) as size_count
			  from b2b_product_picture_size s
			 where s.size_type != 'UPLOAD'
		    
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
