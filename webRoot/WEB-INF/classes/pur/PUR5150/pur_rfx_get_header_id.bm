<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    hs.rfx_header_id
                FROM
                    pur_rfx_header_snaps hs
                WHERE
                    hs.rfx_header_snap_id = ${@rfx_header_snap_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
