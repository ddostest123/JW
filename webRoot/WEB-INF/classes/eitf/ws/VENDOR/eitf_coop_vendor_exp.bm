        <bm:field name="batch_id" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EXCHANGE_RATE"/>
        <bm:field name="c_attribute1" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="C_ATTRIBUTE1"/>
