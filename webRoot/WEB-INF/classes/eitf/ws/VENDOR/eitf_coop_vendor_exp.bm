        <bm:field name="registered_province" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="SOURCE"/>
        <bm:field name="registered_city" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="GL_DATE"/>
