<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    t.import_id,
					t.coop_company_id,
					t.erp_type_code,
					t.company_code,
					t.company_full_name,
					t.email,
					t.registered_country_code,
					t.registered_province,
					t.registered_city,
					t.address,
					t.telephone,
					t.fax_no,
					t.legal_rep_name,
					t.display_code,
					t.agent_name,
					t.agent_phone,
					t.batch_id,
                    t.c_attribute1,
                    t.c_attribute2,
                    t.c_attribute3,
                    t.c_attribute4,
                    t.c_attribute5,
                    t.c_attribute6,
                    t.c_attribute7,
                    t.c_attribute8,
                    t.c_attribute9,
                    t.c_attribute10,
                    t.c_attribute11,
                    t.c_attribute12,
                    t.c_attribute13,
                    t.c_attribute14,
                    t.c_attribute15,
                    t.c_attribute16,
                    t.c_attribute17,
                    t.c_attribute18,
                    t.c_attribute19,
                    t.c_attribute20
                FROM
                    eitf_coop_vendor_exp t
                WHERE
                    t.eitf_instance_id = ${/parameter/@eitf_instance_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
