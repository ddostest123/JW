<?xml version="1.0" encoding="UTF-8"?>

<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[SELECT to_char(SYSDATE,
               'IW') week
  FROM dual]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="week" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="WEEK"/>
    </bm:fields>
</bm:model>
