<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    v.id flex_value,
                    v.showvalue flex_desc
                FROM
                    v3xuser.ctp_enum_item@oalink v
                WHERE
                    v.ref_enumid = '161307127681926592'
                ORDER BY
                    v.id DESC
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
