<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:s="aurora.plugin.script" xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT to_char(sysdate,'yyyy-mm') cur_year_month
                FROM
                    dual
                WHERE
                    1=1
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
