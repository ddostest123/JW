<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    pur_buyer_itf_id,
                    business_group,
                    external_system_id,
                    lot_id,
                    seg_num,
                    itf_instance_id,
                    itf_instance_seg_id,
                    status,
                    finished_flag,
                    error_flag,
                    message_text,
                    es_buyer_id,
                    es_buyer_code,
                    buyer_name,
                    es_user_id,
                    es_user_code,
                    enabled_flag,
                    error_type,
                    error_times,
                    TO_CHAR(creation_date,'YYYY-MM-DD HH24:MI:SS') creation_date,
                    created_by,
                    TO_CHAR(last_update_date,'YYYY-MM-DD HH24:MI:SS') last_update_date,
                    last_updated_by
                FROM
                    pur_buyers_itf #WHERE_CLAUSE#
                ORDER BY
                    pur_buyer_itf_id DESC
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
