<doc watermark='采购品类各机构构成占比分析表' right="2" left="2">
	<header>
        <p align="center" line='900' >
			<t bold="true" font="宋体" fontSize="50"  fontColor="000">采购品类各机构构成占比分析表</t>
		</p>
	</header>
	<body>	
	<p><t></t></p>
    <tbl indLeft="0" border="false">
	 <tr>
		<tc width="8">
			<p  align="left">
				<t font="宋体" fontSize="22">报表类型</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${report_type}</t>
			</p>
       </tc>
	   <tc width="8">
			<p  align="left">
				<t font="宋体" fontSize="22">显示顺序</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr> 
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">年度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_year_from}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">年度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_year_to}</t>
			</p>
       </tc>
	 </tr>
    <#if "${report_type}"=="半年度">
	 <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">半年度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_from_value}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">半年度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_to_value}</t>
			</p>
       </tc>
	 </tr>
   </#if>
   <#if "${report_type}"=="季度">	
	 <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">季度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_from_value}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">季度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_to_value}</t>
			</p>
       </tc>
	 </tr>
  </#if>
  <#if "${report_type}"=="月度">
	 <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">月度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_from_value}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">月度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_to_value}</t>
			</p>
       </tc>
	 </tr>
 </#if>
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">数据汇总级别</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${summary_method}</t>
			</p>
       </tc>
	 </tr>
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">公司</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">部门</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">累计交易金额从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">累计交易金额至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">交易总额排名从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">交易总额排名至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>		      
    </tbl>
	<p><t></t></p>	
	<tbl indLeft="0" border="true">
		<tr>
			<tc width="6" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">公司代码</t>
				</p>
			</tc>
			<tc width="6" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">公司名称</t>
				</p>
			</tc>
		<#if "${summary_method}"=="汇总至部门">
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">部门名称</t>
				</p>
			</tc>
		</#if>
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">累计交易金额（万元）</t>
				</p>
			</tc>
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">占总采购额比重（%）</t>
				</p>
			</tc>
		<#if "${summary_method}"=="汇总至部门">
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">占公司采购额比重（%）</t>
				</p>
			</tc>
		</#if>
		</tr>
		<#if model.getObject("/model/print_test_list_path").getChilds()??>
		    <#list model.getObject('/model/print_test_list_path').getChilds() as cashflow_item >
		    	<tr>
			     <tc>
					 <p align="left">
						 <t font="宋体" fontSize="22">${cashflow_item.get('company_code')!''}</t>
					 </p>
				 </tc>
				 <tc>
					 <p align="left">
						 <t font="宋体" fontSize="22">${cashflow_item.get('company_short_name')!''}</t>
					 </p>
				 </tc>
				<#if "${summary_method}"=="汇总至部门">
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('business_unit_name')!''}</t></p></tc>
				 </#if>
				<tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('total_amount')!''}</t></p></tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('total_ratio')!''}</t></p></tc>
				 <#if "${summary_method}"=="汇总至部门">
				<tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('organization_ratio')!''}</t></p></tc>
				</#if>
				</tr>
		    </#list>
		</#if>
	</tbl>
		<p><t></t></p>	
    </body>
</doc>