<doc watermark='供应商交易额发展分析表' right="2" left="2">
	<header>
        <p align="center" line='900' >
			<t bold="true" font="宋体" fontSize="50"  fontColor="000">供应商交易额发展分析表</t>
		</p>
	</header>
	<body>	
	<p><t></t></p>
    <tbl indLeft="0" border="false">
	 <tr>
		<tc width="8">
			<p  align="left">
				<t font="宋体" fontSize="22">分析方法</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${analysis_method}</t>
			</p>
       </tc>
	   <tc width="8">
			<p  align="left">
				<t font="宋体" fontSize="22">显示顺序</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr> 
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">报表类型</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${report_type}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">采购品类</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<border type="bottom" color="000000" style="single"/>
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>
	 <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">环比发展从（%）</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">环比发展至（%）</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">环比增长从（%）</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">环比增长至（%）</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>      
    </tbl>
	<p><t></t></p>
    <tbl indLeft="0" border="true">
     <tr>
		 <tc width="6" fill="LightSteelBlue">
			<p  align="left">
				<t font="宋体" fontSize="22">基期时段</t>
			</p>
	     </tc>
		 <tc span="3" width="26">
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
		 </tc>
     </tr>
	 <tr>
		<tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">基期年度</t>
			</p>
		</tc>
		<tc  width="10">
			<border type="bottom" color="000000" style="single"/>
			<p  align="right">
					<#if "${analysis_method}"=="基期分析">
					    <t font="宋体" fontSize="22">${link_relative_year_value}</t>
					</#if>
			</p>
       </tc>
	    <tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">基期半年度</t>
			</p>
		</tc>
		<tc  width="10">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
					<#if "${analysis_method}"=="基期分析">
					  <#if "${report_type}"=="半年度">
					    <t font="宋体" fontSize="22">${link_relative_flex_value}</t>
					  </#if>	
					</#if>
			</p>
       </tc>
	 </tr>
	 <tr>
		<tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">基期季度</t>
			</p>
		</tc>
		<tc  width="10">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
					<#if "${analysis_method}"=="基期分析">
					  <#if "${report_type}"=="季度">
					    <t font="宋体" fontSize="22">${link_relative_flex_value}</t>
					  </#if>	
					</#if>
			</p>
       </tc>
	    <tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">基期月度</t>
			</p>
		</tc>
		<tc  width="10">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
					<#if "${analysis_method}"=="基期分析">
					  <#if "${report_type}"=="月度">
					    <t font="宋体" fontSize="22">${link_relative_flex_value}</t>
					  </#if>	
					</#if>
			</p>
       </tc>
	 </tr>
	<p><t></t></p>
    </tbl>
	<p><t></t></p>
    <tbl indLeft="0" border="true">
      <tr>
		 <tc width="6" fill="LightSteelBlue">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期时段</t>
			</p>
	     </tc>
		 <tc span="3" width="26">
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
		 </tc>
      </tr>
      <tr>
		<tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期年度从</t>
			</p>
		</tc>
		<tc  width="10">
			<p  align="right">
				<t font="宋体" fontSize="22">${base_period_year_from}</t>
			</p>
       </tc>
	    <tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期年度至</t>
			</p>
		</tc>
		<tc  width="10">
			<border type="bottom" color="000000" style="single"/>
			<p  align="right">
				<t font="宋体" fontSize="22">${base_period_year_to}</t>
			</p>
       </tc>
	 </tr>
	 <tr>
		<tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期半年度从</t>
			</p>
		</tc>
		<tc  width="10">
			<p  align="left">
					<#if "${report_type}"=="半年度">
					    <t font="宋体" fontSize="22">${base_period_from_value}</t>
					</#if>				
			</p>
       </tc>
	    <tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期半年度至</t>
			</p>
		</tc>
		<tc  width="10">
			<p  align="left">
					<#if "${report_type}"=="半年度">
					    <t font="宋体" fontSize="22">${base_period_to_value}</t>
					</#if>
			</p>
       </tc>
	 </tr>
	 <tr>
		<tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期季度从</t>
			</p>
		</tc>
		<tc  width="10">
			<p  align="left">
					<#if "${report_type}"=="季度">
					    <t font="宋体" fontSize="22">${base_period_from_value}</t>
					</#if>	
			</p>
       </tc>
	    <tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期年度至</t>
			</p>
		</tc>
		<tc  width="10">
			<p  align="left">
					<#if "${report_type}"=="季度">
					    <t font="宋体" fontSize="22">${base_period_to_value}</t>
					</#if>	
			</p>
       </tc>
	 </tr>
	 <tr>
		<tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期月度从</t>
			</p>
		</tc>
		<tc  width="10">
			<p  align="left">
					<#if "${report_type}"=="月度">
					    <t font="宋体" fontSize="22">${base_period_from_value}</t>
					</#if>	
			</p>
       </tc>
	    <tc width="6">
			<p  align="left">
				<t font="宋体" fontSize="22">报告期月度至</t>
			</p>
		</tc>
		<tc  width="10">
			<p  align="left">
					<#if "${report_type}"=="月度">
					    <t font="宋体" fontSize="22">${base_period_to_value}</t>
					</#if>	
			</p>
       </tc>
	 </tr>
    </tbl>
	<p><t></t></p>	
	<tbl indLeft="0" border="true">
		<tr>
			<tc width="7" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">供应商简称</t>
				</p>
			</tc>
			<tc width="7" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">供应商全称</t>
				</p>
			</tc>
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">合作品类范围</t>
				</p>
			</tc>
			<#if "${analysis_method}"=="环比分析">
			<tc width="2" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">交易额环比发展速度</t>
				</p>
			</tc>
			<tc width="2" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">交易额环比增长率（%）</t>
				</p>
			</tc>
			</#if>
			<#if "${analysis_method}"=="基期分析">
			<tc width="2" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">定基指数</t>
				</p>
			</tc>
			</#if>
			<tc width="2" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">本期交易额（万元）</t>
				</p>
			</tc>
			<tc width="2" fill="CornflowerBlue">
				<p align="center" >
					<t font="宋体" fontSize="22">上期交易额（万元）</t>
				</p>
			</tc>
			<tc width="2" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">本期期间</t>
				</p>
			</tc>
			<tc width="2" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">上期期间</t>
				</p>
			</tc>
		</tr>
		<#if model.getObject("/model/print_test_list_path").getChilds()??>
		    <#list model.getObject('/model/print_test_list_path').getChilds() as cashflow_item >
		    	<tr>
			     <tc>
					 <p align="left">
						 <t font="宋体" fontSize="22">${cashflow_item.get('vendor_name')!''}</t>
					 </p>
				 </tc>
				 <tc>
					 <p align="left">
						 <t font="宋体" fontSize="22">${cashflow_item.get('vendor_full_name')!''}</t>
					 </p>
				 </tc>
			     <tc ><p align="right"><t font="宋体" fontSize="22"> </t></p></tc>
				 <#if "${analysis_method}"=="环比分析">
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('growth_speed_out')!''}</t></p></tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('growth_rate_out')!''}</t></p></tc>
				 </#if>
				 <#if "${analysis_method}"=="基期分析">
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('base_index_out')!''}</t></p></tc>
				</#if>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('total_amount')!''}</t></p></tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('last_total_amount')!''}</t></p></tc>
				 <tc ><p align="left"><t font="宋体" fontSize="22">${cashflow_item.get('period_name')!''}</t></p></tc>
				 <tc ><p align="left"><t font="宋体" fontSize="22">${cashflow_item.get('last_period_name')!''}</t></p></tc>
				</tr>
		    </#list>
		</#if>
	</tbl>
		<p><t></t></p>	
    </body>
</doc>