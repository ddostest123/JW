<doc watermark='供应商交易额榜单' right="2" left="2">
	<header>
        <p align="center" line='900' >
			<t bold="true" font="宋体" fontSize="50"  fontColor="000">供应商交易额榜单</t>
		</p>
	</header>
	<body>	
	<p><t></t></p>
    <tbl indLeft="0" border="false">
	 <tr>
		<tc width="8">
			<p  align="left">
				<t font="宋体" fontSize="22">报表类型</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${report_type}</t>
			</p>
       </tc>
	   <tc width="8">
			<p  align="left">
				<t font="宋体" fontSize="22">显示顺序</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr> 
<tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">年度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_year_from}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">年度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_year_to}</t>
			</p>
       </tc>
	 </tr>
    <#if "${report_type}"=="半年度">
	 <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">半年度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_from_value}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">半年度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_to_value}</t>
			</p>
       </tc>
	 </tr>
   </#if>
   <#if "${report_type}"=="季度">	
	 <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">季度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_from_value}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">季度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_to_value}</t>
			</p>
       </tc>
	 </tr>
  </#if>
  <#if "${report_type}"=="月度">
	 <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">月度从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_from_value}</t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">月度至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22">${base_period_to_value}</t>
			</p>
       </tc>
	 </tr>
 </#if>
	  <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">供应商</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">数据汇总</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>	
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">累计交易金额从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">累计交易金额至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>
     <tr>
		<tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">交易总额排名从</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	   <tc width="12">
			<p  align="left">
				<t font="宋体" fontSize="22">交易总额排名至</t>
			</p>
		</tc>
		<tc  width="20">
			<border type="bottom" color="000000" style="single"/>
			<p  align="left">
				<t font="宋体" fontSize="22"></t>
			</p>
       </tc>
	 </tr>		      
    </tbl>
	<p><t></t></p>	
	<tbl indLeft="0" border="true">
		<tr>
			<tc width="6" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">供应商简称</t>
				</p>
			</tc>
			<tc width="6" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">供应商全称</t>
				</p>
			</tc>
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">合作品类范围</t>
				</p>
			</tc>
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">累计交易金额（万元）</t>
				</p>
			</tc>
			<tc width="2" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">总交易额排名</t>
				</p>
			</tc>
			<tc width="2" fill="CornflowerBlue">
				<p align="center" >
					<t font="宋体" fontSize="22">占总交易额比重（%）</t>
				</p>
			</tc>
			<tc width="4" fill="CornflowerBlue">
				<p align="center">
					<t font="宋体" fontSize="22">最大采购份额品类</t>
				</p>
			</tc>
		</tr>
		<#if model.getObject("/model/print_test_list_path").getChilds()??>
		    <#list model.getObject('/model/print_test_list_path').getChilds() as cashflow_item >
		    	<tr>
			     <tc>
					 <p align="left">
						 <t font="宋体" fontSize="22">${cashflow_item.get('coop_company_name')!''}</t>
					 </p>
				 </tc>
				 <tc>
					 <p align="left">
						 <t font="宋体" fontSize="22">${cashflow_item.get('coop_company_full_name')!''}</t>
					 </p>
				 </tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22"></t></p></tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('total_amount')!''}</t></p></tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('overall_ranking')!''}</t></p></tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('total_ratio')!''}</t></p></tc>
				 <tc ><p align="right"><t font="宋体" fontSize="22">${cashflow_item.get('item_category_name')!''}</t></p></tc>
				</tr>
		    </#list>
		</#if>
	</tbl>
		<p><t></t></p>	
    </body>
</doc>